APPL AIBOSS01 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/* 05/16/2016 -JFC- NEW APPL CREATED PER JULIE PETERSON               */
/* 05/16/2016 -JFC- THIS IS FOR AIKEN PASSENGER PLANT - ID # 181      */
/* 02/24/2020 -JFC- UPDATED AGENT FROM R7 TO R11.5.0                  */
/*                - CHANGED JOBQ FROM CYBESPV210.CYBESPV2100          */
/* 03/05/2020 -DSG- ADDED RESOURCE TO AS400 JOBS - INC000002178387
/*09/12/2022-for agent CYBA_CDC1AKFIS user maestro changed to
/*           bfusa\maestro - INC0353221
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
TAG BOSS

JOB LIE.AIPED150 EXTERNAL APPLID(AIPED150)
  RUN DAILY
  RELEASE ADD(LIS.!ESPAPPL)
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
  RUN DAILY
  RELEASE ADD(AIMS_BOSS)
ENDJOB

AS400_JOB AIMS_BOSS
  RESOURCE ADD(1,AKNITS)
  AGENT AKNITS
  COMMAND CYBROBOT AIMS_BOSS
  JOBQ CAWAAGENT.CYBESPJOBS
  RUN DAILY
  RELEASE ADD(AIMS_BOSSC)
ENDJOB

AS400_JOB AIMS_BOSSC
  RESOURCE ADD(1,AKNITS)
  AGENT AKNITS
  COMMAND CYBROBOT AIMS_BOSSC
  JOBQ CAWAAGENT.CYBESPJOBS
  RUN DAILY
  RELEASE ADD(MTIC_LOADBAT)
ENDJOB

NT_JOB MTIC_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\DTS_RUN\BatchFiles\BOSS\MticLoad\MTIC_LOAD.bat
  ARGS
  USER bfusa\maestro
  RUN DAILY
  RELEASE ADD(BOSSSAP_81_LOADBAT)
ENDJOB

NT_JOB BOSSSAP_81_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_RUN\batchfiles\boss\SapLoad\BOSSSAP_81_LOAD.BAT
  ARGS
  USER bfusa\maestro
  RUN DAILY
    NOTWITH BOSSSAP_AO_LOADBAT
    NOTWITH BOSSSAP_BL_LOADBAT
    NOTWITH BOSSSAP_88_LOADBAT
    NOTWITH BOSSSAP_CD_LOADBAT
    NOTWITH BOSSSAP_LV_LOADBAT
    NOTWITH BOSSSAP_8H_LOADBAT
    NOTWITH BOSSSAP_WN_LOADBAT
  RELEASE ADD(BOSSCUREBUILD_81_LOADBAT)
ENDJOB

NT_JOB BOSSCUREBUILD_81_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_RUN\batchfiles\boss\CureLoad\BOSSCureBuild_81_Load.bat
  ARGS
  USER bfusa\maestro
  RUN DAILY
    NOTWITH BOSSCUREBUILD_AO_LOADBAT
    NOTWITH BOSSCUREBUILD_BL_LOADBAT
    NOTWITH BOSSCUREBUILD_88_LOADBAT
    NOTWITH BOSSCUREBUILD_CD_LOADBAT
    NOTWITH BOSSCUREBUILD_LV_LOADBAT
    NOTWITH BOSSCUREBUILD_8H_LOADBAT
    NOTWITH BOSSCUREBUILD_WN_LOADBAT
  RELEASE ADD(BOSSCOMP_81_LOADBAT)
ENDJOB

NT_JOB BOSSCOMP_81_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_RUN\batchfiles\boss\CompLoad\BOSSComp_81_Load.bat
  ARGS
  USER bfusa\maestro
  RUN DAILY
   NOTIFY FAILURE ABEND ALERT(REMD)
   NOTIFY OVERDUE MAILBOX(AIBOSS1)
   NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)
   IF ESPSHH = '13' THEN DUEOUT EXEC 4PM
   IF ESPSHH = '19' THEN DUEOUT EXEC 4AM
    NOTWITH BOSSCOMP_AO_LOADBAT
    NOTWITH BOSSCOMP_BL_LOADBAT
    NOTWITH BOSSCOMP_88_LOADBAT
    NOTWITH BOSSCOMP_CD_LOADBAT
    NOTWITH BOSSCOMP_LV_LOADBAT
    NOTWITH BOSSCOMP_8H_LOADBAT
    NOTWITH BOSSCOMP_WN_LOADBAT
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

AIX_JOB LIE.!ESPAPPL
  AGENT CYBA_ISBFP3
  COMMAND /bin/sleep
  ARGS 3
  USER bfpadm
  RUN DAILY
ENDJOB

