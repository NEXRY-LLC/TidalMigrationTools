APPL QAINVCLS WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

NT_JOB INTERFACE_1085 EXTERNAL APPLID(SSISPPIW) SCOPE(-10)
   RUN DAILY
   NORUN JUL 24 2014
   RELEASE ADD(LIS.QAINVCLS)
ENDJOB

JOB LIS.QAINVCLS LINK PROCESS
   RUN DAILY
   RELEASE ADD(INVENTORY_CLASS_MASTERCMD)
ENDJOB

NT_JOB INVENTORY_CLASS_MASTERCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\inventory_class_master.cmd
   ARGS
   USER maestro
   RUN DAILY
   EARLYSUB 01.00
   RELEASE ADD(INVENTORY_CLASS_MASTER_CHECKS)
ENDJOB

NT_JOB INVENTORY_CLASS_MASTER_CHECKS
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\inventory_class_master_checks.cmd
   ARGS
   USER maestro
   RUN DAILY
   EARLYSUB 01.00
   DUEOUT EXEC 6AM
   RELEASE ADD(LIE.QAINVCLS)
ENDJOB

JOB LIE.QAINVCLS LINK PROCESS
   RUN DAILY
ENDJOB

