APPL QAMXCUST WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/* 11/21/2016 -JFC- REMOVED CDC1 CONNECT DIRECT JOBS PER J. HRITZ     */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

NT_JOB ITEM_MASTER_MXCMD EXTERNAL APPLID(QAMXITEM)
   RUN WED FRI
   RELEASE ADD(LIS.QAMXCUST)
ENDJOB

JOB LIS.QAMXCUST LINK PROCESS
   RUN WED FRI
   EARLYSUB 10.10
   RELEASE (DTS_CONTROL_CUSTOMER_MASTER_MXCMD)
ENDJOB

NT_JOB DTS_CONTROL_CUSTOMER_MASTER_MXCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\dts_control_customer_master_mx.cmd
   ARGS 6
   USER maestro
   RUN WED FRI
   RELEASE ADD(CUSTOMER_MASTER_MXCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER_MXCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\customer_master_mx.cmd
   ARGS
   USER maestro
   RUN WED FRI
   RELEASE ADD(CUSTOMER_MASTER_MX_CHECKSCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER_MX_CHECKSCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\customer_master_mx_checks.cmd
   ARGS
   USER maestro
   RUN WED FRI
   RELEASE ADD(CUSTOMER_MASTER_MX_DELETED_CHECKSCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER_MX_DELETED_CHECKSCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\customer_master_mx_deleted_checks.cmd
   ARGS
   USER maestro
   RUN WED FRI
   RELEASE ADD(LIE.QAMXCUST)
ENDJOB

JOB LIE.QAMXCUST LINK PROCESS
   RUN WED FRI
ENDJOB

