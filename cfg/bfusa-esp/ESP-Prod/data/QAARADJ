APPL QAARADJ WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

JOB LIS.QAARADJ LINK PROCESS
  RUN SAT
  IF TODAY('JULY 15 2017') THEN JOBATTR HOLD
  DELAYSUB 00.03
  RELEASE ADD(WA_HEADER_DETAIL_DELETE_ARCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_DELETE_ARCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wa_header_detail_delete_ar.cmd
  ARGS 5
  RUN SAT
  USER maestro
  RELEASE ADD(WA_HEADER_TEMP_ARCMD)
ENDJOB

NT_JOB WA_HEADER_TEMP_ARCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wa_header_temp_ar.cmd
  ARGS 5
  RUN SAT
  USER maestro
  RELEASE ADD(WA_DETAIL_TEMP_ARCMD)
ENDJOB

NT_JOB WA_DETAIL_TEMP_ARCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wa_detail_temp_ar.cmd
  ARGS 5
  RUN SAT
  USER maestro
  RELEASE ADD(WA_HEADER_SAP_INITIAL_ARCMD)
ENDJOB

