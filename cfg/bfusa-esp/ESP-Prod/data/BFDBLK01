JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   EARLYSUB 01.00
   RELEASE (BLACKLIN_CGL_EXT_BATONA_GL_BL)
ENDJOB

SAP_JOB BLACKLIN_CGL_EXT_BATONA_GL_BL
   AGENT CYBB_BFP030
   SAPJOBNAME BLACKLIN_CGL_EXT_BATONA_GL_BL
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME /BLACKLIN/CGL_EXTRACT
   VARIANT 'BATONA GL BL'
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTSPOOLNAME /BLACKLIN/CG
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   PRINTCOPIES 001
   BANNER D
   RUN DAILY
   EARLYSUB 02.00
   RELEASE (BLACKLIN_CGL_EXT_BATONA_AP_AR)
ENDJOB

