    PRINTDEST CC13
    PRINTCOPIES 001
    PRINTIMMED YES
    EXPIRATION 8
    LINES 58
    COLUMNS 141
    BANNERPAGE YES
    BANNER YES
    RECIPIENT STEWARTTHOMA
    ARCMODE PRINT
    RUN MON
    RELEASE ADD(RFITEMAP_CO_AUTO_BLOCK)
ENDJOB

