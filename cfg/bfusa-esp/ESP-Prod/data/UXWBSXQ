APPL UXWBSXQ

NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*******************************************************************
/*  11/11/15  CREATED NEW JOBS FOR SXQ PER R. DESU - MFM
/*  02/20/19  REPLACED ZOS COMPLETE JOB WITH LIE LINK PROCESS - DSG
/*******************************************************************

JOB LIS.UXWBSXQ LINK PROCESS
   EARLYSUB 18.00
   RUN SUN
   RELEASE ADD(STOTC.SXQAPP3)
ENDJOB

UNIX_JOB STOTC.SXQAPP3
   AGENT CYBA_SXQAPP3
   SCRIPTNAME /apps/apache-tomcat-6.0.43/bin/shutdown.sh
   ARGS -force
   USER tomcat
   RUN SUN
   RELEASE ADD(STOTC.SXQAPP1)
   RELEASE ADD(STOTC.SXQAPP2)
ENDJOB

UNIX_JOB STOTC.SXQAPP1
   AGENT CYBA_SXQAPP1
   SCRIPTNAME /apps/apache-tomcat-7.0.57/bin/shutdown.sh
   ARGS -force
   USER tomcat
   RUN SUN
   RELEASE ADD(SXQDBWKL.SXQDB)
   RELEASE ADD(STATC.SXQAPP1)
   RELEASE ADD(STATC.SXQAPP2)
ENDJOB

UNIX_JOB STOTC.SXQAPP2
   AGENT CYBA_SXQAPP2
   SCRIPTNAME /apps/apache-tomcat-7.0.57/bin/shutdown.sh
   ARGS -force
   USER tomcat
   RUN SUN
   RELEASE ADD(SXQDBWKL.SXQDB)
   RELEASE ADD(STATC.SXQAPP1)
   RELEASE ADD(STATC.SXQAPP2)
ENDJOB

UNIX_JOB SXQDBWKL.SXQDB
   AGENT CYBA_SXQDB
   SCRIPTNAME /local/bin/db2_wklybkup_sbrx.sh
   ARGS QI1 SBXQ ONLINE
   USER root
   RUN SUN
   RELEASE ADD(UXWBSXQ.COMPLETE)
ENDJOB

UNIX_JOB STATC.SXQAPP1
   AGENT CYBA_SXQAPP1
   SCRIPTNAME /apps/apache-tomcat-7.0.57/bin/startup.sh
   ARGS
   USER tomcat
   RUN SUN
   RELEASE ADD(UXWBSXQ.COMPLETE)
ENDJOB

UNIX_JOB STATC.SXQAPP2
   AGENT CYBA_SXQAPP2
   SCRIPTNAME /apps/apache-tomcat-7.0.57/bin/startup.sh
   ARGS
   USER tomcat
   RUN SUN
   RELEASE ADD(STATC.SXQAPP3)
ENDJOB

UNIX_JOB STATC.SXQAPP3
   AGENT CYBA_SXQAPP3
   SCRIPTNAME /apps/apache-tomcat-6.0.43/bin/startup.sh
   ARGS
   USER tomcat
   RUN SUN
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN SUN
ENDJOB


