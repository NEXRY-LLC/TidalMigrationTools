APPL IPMFI001 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB IPMFI001_MF30_MONTHLY_11PM LINK PROCESS
   RUN LAST WORKDAY OF MONTH
   DELAYSUB 23.00
   RELEASE ADD(FSIP_MF30_3003)
   RELEASE ADD(FSIP_MF30_3004)
   RELEASE ADD(FSIP_MF30_3021)
ENDJOB

SAP_JOB FSIP_MF30_3003
   AGENT SAP_IZP100
   SAPJOBNAME 'MF30 3003'
   SAPJOBCLASS C
   STARTMODE ASAP
   PRINTCOPIES 001
   ABAPNAME RMSERI40
   VARIANT MF30_3003
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   RECIPIENT BFDPBATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME RMSERI40_BFD
   PRINTFORMAT X_65_80
   RUN LAST WORKDAY OF MONTH
ENDJOB

SAP_JOB FSIP_MF30_3004
   AGENT SAP_IZP100
   SAPJOBNAME 'MF30 3004'
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RMSERI40
   VARIANT MF30_3004
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT BFDPBATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME RMSERI40_BFD
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
   RUN LAST WORKDAY OF MONTH
ENDJOB

SAP_JOB FSIP_MF30_3021
   AGENT SAP_IZP100
   SAPJOBNAME 'MF30 3021'
   SAPJOBCLASS C
   ABAPNAME RMSERI40
   VARIANT MF30_3021
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT BFDPBATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME RMSERI40_BFD
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
