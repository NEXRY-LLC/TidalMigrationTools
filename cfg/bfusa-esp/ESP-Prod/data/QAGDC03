APPL QAGDC03 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*02/19/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW

JOB LIS.!ESPAPPL LINK PROCESS
   RUN LAST SUNDAY OF MONTH
   DELAYSUB 09.00
   RELEASE ADD(JAPAN_GDC_MONTHLY_TRANSFERCMD)
ENDJOB

NT_JOB JAPAN_GDC_MONTHLY_TRANSFERCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\JAPAN_GDC_MONTHLY_TRANSFER.CMD
   ARGS
   USER maestro
   RUN LAST SUNDAY OF MONTH
   RELEASE ADD(JAPAN_GDC_SEND_FORM_A_MONTHLY_EMAILCMD)
ENDJOB

NT_JOB JAPAN_GDC_SEND_FORM_A_MONTHLY_EMAILCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\JAPAN_GDC_SEND_FORM_A_MONTHLY_EMAIL.CMD
   ARGS
   USER maestro
   RUN LAST SUNDAY OF MONTH
   RELEASE ADD(JAPAN_GDC_SEND_FORM_B_MONTHLY_EMAILCMD)
ENDJOB

NT_JOB JAPAN_GDC_SEND_FORM_B_MONTHLY_EMAILCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\JAPAN_GDC_SEND_FORM_B_MONTHLY_EMAIL.CMD
   ARGS
   USER maestro
   RUN LAST SUNDAY OF MONTH
   RELEASE ADD(JAPAN_GDC_SEND_FORM_C_MONTHLY_EMAILCMD)
ENDJOB

NT_JOB JAPAN_GDC_SEND_FORM_C_MONTHLY_EMAILCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\JAPAN_GDC_SEND_FORM_C_MONTHLY_EMAIL.CMD
   ARGS
   USER maestro
   RUN LAST SUNDAY OF MONTH
   RELEASE ADD(LIE.!ESPAPPL)
