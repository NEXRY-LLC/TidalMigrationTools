APPL LED20ALN WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
/*********************************************************************/
/*03/26/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES */
/*12/18/2020 -ANK- ADDED JOB RVV50R10C_CREATE_DELIV_SO_TOR*/
/*********************************************************************/
NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   RELEASE ADD(RVV50R10C_CREATE_DELIV_SO_ALN,+
RVV50R10C_CREATE_DELIV_SO_TOR)
ENDJOB

SAP_JOB SDV03V02_RESCHED_STO_D005 EXTERNAL APPLID(SAD102) SCOPE(-13)
   RUN MON TUE WED THU
   RELEASE ADD(RVV50R10C_CREATE_DELIV_SO_ALN)
ENDJOB

SAP_JOB SDV03V02_RESCHED_STO_D018 EXTERNAL APPLID(SAD128) SCOPE(-13)
   RUN MON TUE WED THU
   RELEASE ADD(RVV50R10C_CREATE_DELIV_SO_TOR)
ENDJOB

SAP_JOB ZSDV03V2_RESCHED_ORD_4005 EXTERNAL APPLID(SAD100) SCOPE(-13)
   RUN MON TUE WED THU
   RELEASE ADD(RVV50R10C_CREATE_DELIV_SO_ALN,+
RVV50R10C_CREATE_DELIV_SO_TOR)
ENDJOB

SAP_JOB RBDAPP01_CRT_BSRO_ORDERS_9PM EXTERNAL APPLID(SAD008A) SCOPE(-04)
   RUN MON TUE WED THU
   RELEASE ADD(RVV50R10C_CREATE_DELIV_SO_ALN,+
RVV50R10C_CREATE_DELIV_SO_TOR)
ENDJOB

SAP_JOB ZSDLEI36N_I99_SAP_TBL_DCEST EXTERNAL APPLID(LED024DE) SCOPE(-08)
   RUN MON TUE WED THU
   RELEASE ADD(RVV50R10C_CREATE_DELIV_SO_ALN,+
RVV50R10C_CREATE_DELIV_SO_TOR)
ENDJOB

JOB BFPULOCK EXTERNAL APPLID(SUNMAINT) SCOPE(-04)
   RUN MON
   IF ESPSHH LT '02' THEN RUN MONDAY
   IF ESPSHH GE '04' THEN NORUN MON TUE WED THU
   RELEASE ADD(RVV50R10C_CREATE_DELIV_SO_ALN,+
RVV50R10C_CREATE_DELIV_SO_TOR)
ENDJOB

SAP_JOB RVV50R10C_CREATE_DELIV_SO_ALN
   AGENT CYBB_BFP030
   SAPJOBNAME RVV50R10C_CREATE_DELIV_SO_ALN
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RVV50R10C
   VARIANT DLV_SO_ALN_D
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   RECIPIENT SAPADM
   ARCMODE PRINT
   BANNER D
   PRINTCOPIES 001
   RUN MON TUE WED THU
   DUEOUT EXEC 03:30
   NOTIFY ABEND FAILURE ALERT(REMD)
   NOTIFY OVERDUE ALERT(SA03)
   IF ESPAHH GE '03' AND ESPAHH LT '12' THEN NOTIFY END ALERT(SA03)
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
ENDJOB

SAP_JOB RVV50R10C_CREATE_DELIV_SO_TOR
   AGENT CYBB_BFP030
   SAPJOBNAME RVV50R10C_CREATE_DELIV_SO_TOR
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RVV50R10C
   VARIANT DLV_SO_TOR_D
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTSPOOLNAME RVV50R10C_
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN MON TUE WED THU
   DUEOUT EXEC 03:30
   NOTIFY ABEND FAILURE ALERT(REMD)
   NOTIFY OVERDUE ALERT(SA03)
   IF ESPAHH GE '03' AND ESPAHH LT '12' THEN NOTIFY END ALERT(SA03)
   RELEASE (LIE.!ESPAPPL)
ENDJOB


