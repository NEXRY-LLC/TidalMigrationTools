JOB LIS.!ESPAPPL LINK PROCESS
   RUN WEEKDAYS
   EARLYSUB 09.00
   RELEASE (FSIP_ZZSD_INVENTORY_STATUS_ALL,FSIP_RVKRED02_OPEN)
ENDJOB

SAP_JOB FSIP_ZZSD_INVENTORY_STATUS_ALL
   AGENT SAP_IZP100
   SAPJOBNAME ZZSD_INVENTORY_STATUS_ALL
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZZSD_INVENTORY_STATUS
   VARIANT ALL_INVENTORY
   STEPUSER S_BC_CYBERM
   /*PRINTDEST TRSH
   LANGUAGE E
   PRINTCOPIES 001
   PRINTSPOOLNAME ZZSD_INVENTO
   PRINTIMMED YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSIP_BATCH
   ARCMODE PRINT
   RUN WEEKDAYS
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

SAP_JOB FSIP_RVKRED02_OPEN
   AGENT SAP_IZP100
   SAPJOBNAME RVKRED02_OPEN
   SAPJOBCLASS C
   STARTMODE ASAP
   RELEASE (LIE.!ESPAPPL)
   ABAPNAME RVKRED02
   VARIANT OPEN
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   /*PRINTDEST TRSH
   PRINTCOPIES 001
   PRINTSPOOLNAME RVKRED02_S_B
   PRINTIMMED YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT S_BC_CYBERM
   RUN WEEKDAYS
   ARCMODE PRINT
ENDJOB

