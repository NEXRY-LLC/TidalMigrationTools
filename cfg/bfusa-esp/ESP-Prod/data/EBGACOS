   RELDELAY 1
   RUN LAST DAY OF MONTH
   RELEASE ADD(EBGACOS_FT_BFISWEB_GACO_PDF)
ENDJOB

FILE_TRIGGER EBGACOS_FT_BFISWEB_GACO_PDF
   AGENT BFISWEB
   FILENAME 'D:\ftproot\wolverine\Gaco_Stmt*.pdf' create
   RUN LAST DAY OF MONTH
   RELEASE ADD(EBGACOS_PASS1)
ENDJOB

