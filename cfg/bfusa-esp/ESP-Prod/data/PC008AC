APPL PC008AC WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*04/01/2021 - SCTASK0047454 - NEW JOB CREATED                       */
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
  RUN DAILY
  DELAYSUB 01:30
  RELEASE ADD(PC008AC)
ENDJOB

JOB PC008AC
  /*@@ SYSTEM=PRDPROC
  CCCHK RC(1:4) CONTINUE
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN DAILY
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
  RUN DAILY
ENDJOB

