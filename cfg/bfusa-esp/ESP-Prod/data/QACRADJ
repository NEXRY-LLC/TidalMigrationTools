APPL QACRADJ WAIT

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
TAG PPIW

JOB LIS.QACRADJ LINK PROCESS
   RUN SAT
   IF TODAY('JULY 15 2017') THEN JOBATTR HOLD
   EARLYSUB 00.03
   RELEASE ADD(WA_HEADER_DETAIL_DELETE_CRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_DELETE_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_detail_delete_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_TEMP_CRCMD)
ENDJOB

NT_JOB WA_HEADER_TEMP_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_temp_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_TEMP_CRCMD)
ENDJOB

NT_JOB WA_DETAIL_TEMP_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_temp_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_SAP_INITIAL_CRCMD)
ENDJOB

NT_JOB WA_HEADER_SAP_INITIAL_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_sap_initial_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_SAP_TEMP_CRCMD)
ENDJOB

NT_JOB WA_HEADER_SAP_TEMP_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_sap_temp_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_INITIAL_CRCMD)
ENDJOB

NT_JOB WA_DETAIL_SAP_INITIAL_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_initial_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_TEMP_CRCMD)
ENDJOB

NT_JOB WA_DETAIL_SAP_TEMP_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_temp_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_DETAIL_REBUILD_CRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_REBUILD_CRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_detail_rebuild_CR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(LIE.QACRADJ)
