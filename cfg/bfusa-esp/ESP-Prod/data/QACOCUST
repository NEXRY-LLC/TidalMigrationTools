NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

NT_JOB ITEM_MASTER_COCMD EXTERNAL APPLID(QACOITEM)
  RUN WED FRI
  RELEASE ADD(LIS.QACOCUST)
ENDJOB

JOB LIS.QACOCUST LINK PROCESS
   RUN WED FRI
   DELAYSUB 11.10
   RELEASE ADD(DTS_CONTROL_CUSTOMER_MASTER_COCMD)
ENDJOB

NT_JOB DTS_CONTROL_CUSTOMER_MASTER_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\dts_control_customer_master_CO.cmd
   ARGS 6
   USER maestro
   RUN WED FRI
   RELEASE ADD(CUSTOMER_MASTER_COCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\customer_master_CO.cmd
   ARGS
   USER maestro
   RUN WED FRI
   RELEASE ADD(CUSTOMER_MASTER_CO_CHECKSCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER_CO_CHECKSCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\customer_master_CO_checks.cmd
   ARGS
   USER maestro
   RUN WED FRI
   RELEASE ADD(CUSTOMER_MASTER_CO_DELETED_CHECKSCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER_CO_DELETED_CHECKSCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\customer_master_CO_deleted_checks.cmd
   ARGS 6
   USER maestro
   RUN WED FRI
   RELEASE ADD(LIE.QACOCUST)
ENDJOB

JOB LIE.QACOCUST LINK PROCESS
   RUN WED FRI
ENDJOB

