NT_JOB BSRO_PULL_FILES
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\BSRO_PULL_FILES.bat
  ARGS
  USER maestro
  RUN TODAY
  DELAYSUB 22.00
  RELEASE ADD(BSRO_FILECHECK)
ENDJOB

NT_JOB BSRO_FILECHECK
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\BSRO_FileCheck.cmd
  ARGS
  USER maestro
  RUN TODAY
  RELDELAY 5
  RELEASE ADD(BSRO_BEGIN_PROCESSING)
ENDJOB

