JOB LIS.WEEKDAYS LINK PROCESS
  RELEASE ADD(USS3DB2A)
  RELEASE ADD(USS3DB2B)
  RELEASE ADD(USS3DB2C)
  RELEASE ADD(USDB2CMP)
  RELEASE ADD(USCATCK3)
  RUN DAILY
ENDJOB

JOB USS3DB2A
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN WEEKDAYS
  RELEASE ADD(USS3DB2S)
  RELEASE ADD(USS3DB2E)
ENDJOB

JOB USS3DB2B
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN WEEKDAYS
  RELEASE ADD(USS3DB2S)
  RELEASE ADD(USS3DB2E)
ENDJOB

JOB USS3DB2C
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN WEEKDAYS
  RELEASE ADD(USS3DB2S)
  RELEASE ADD(USS3DB2E)
ENDJOB

JOB USDB2CMP REQUEST
ENCPARM PREDICT DSNOTFOUND(NO)
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN WEEKDAYS
  RESOURCE ADD(1,SYS3)
  RELEASE ADD(USS3DB2S)
ENDJOB

JOB USCATCK3
  CCCHK RC(1:4095) FAIL CONTINUE
/*RUN WEEKDAYS
