/********************************************************************/
/*   MONTHLY JOBS
/********************************************************************/

JOB ELR704
  OPTIONS NORESTARTSTEP
  CCCHK RC(1:4095) FAIL CONTINUE
  IF TODAY('SAT') & TODAY('1ST SAT OF MONTH') +
  & ESPSHH EQ '05' THEN +
  RUN TODAY
     ELSE +
    NORUN TODAY
  RELEASE ADD(ELW000)
ENDJOB

JOB ELM000
  CCCHK RC(1:4095) FAIL CONTINUE
  IF TODAY('SAT') & TODAY('1ST SAT OF MONTH') +
  & ESPSHH EQ '05' THEN +
  RUN TODAY
     ELSE +
    NORUN TODAY
  RELEASE ADD(ELM100)
  RELEASE ADD(OMM300)
  NOTWITH (OMD900.-)
ENDJOB

JOB ELM100
