APPL ELD001 WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)

/*********************************************************************/
/*05/10/2017 -JFC- NEW APPL TO RUN MONTHLY JOBS THAT WERE IN ELD001  */
/*********************************************************************/

JOB ELD500 EXTERNAL SCOPE(-8:00)
  IF TODAY('TUE WED THU FRI SAT') & ESPSHH EQ '05' THEN +
  RUN TODAY
  ELSE +
  NORUN TODAY
  RELEASE ADD(ELD001)

JOB ELD001
  /*@@ SYSTEM=EDI
  RUN WEEKDAYS SAT
  /* NORUN HOLIDAYS
  RELEASE ADD(ELD000)
ENDJOB

JOB ELD000
  /*@@ SYSTEM=EDI
  CCCHK RC(9:4095) FAIL CONTINUE
  RUN WEEKDAYS SAT
  /* NORUN HOLIDAYS
  RELEASE ADD(ELD090)
  RELEASE ADD(ELD741D)
  RELEASE ADD(ELD701D)
  RELEASE ADD(ELD714D)
  RELEASE ADD(ELD722D)
  RELEASE ADD(ELD739D)
  RELEASE ADD(ELD806S9)
  RELEASE ADD(ELD972AS)
  NOTWITH (OMD900.-)
ENDJOB

