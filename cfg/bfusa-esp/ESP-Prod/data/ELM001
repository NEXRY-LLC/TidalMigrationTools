JOB ELD090
  /*@@ SYSTEM=EDI
  CCCHK STEP(ELD090) PROC(S070) RC(08) OK
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN WEEKDAYS SAT
  /* NORUN HOLIDAYS
  RELEASE ADD(ELD812KO)
  RELEASE ADD(ELD812QF)
  RELEASE ADD(ELD812X0)
  RELEASE ADD(ELD813A9)
  RELEASE ADD(ELD813PF)
  RELEASE ADD(ELD715D)
  RELEASE ADD(OMD150)
  NOTWITH (ELD470.-)
  NOTWITH (ELD503.-)
  NOTWITH (ELD755D.-)
  NOTWITH (OMD900.-)
ENDJOB

