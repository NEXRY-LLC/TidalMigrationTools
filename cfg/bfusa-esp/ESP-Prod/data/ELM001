JOB OMD000
  /*@@ SYSTEM=EDI
  CCCHK RC(3:4095) FAIL CONTINUE
  RUN WEEKDAYS SAT
  /* NORUN HOLIDAYS
  RELEASE ADD(ELD731D)
  RELEASE ADD(OMD200)
  NOTWITH (ELD825B.-)
  NOTWITH (ELD831B.-)
  NOTWITH (OMD900.-)
ENDJOB

  JOB OMD100
    /*@@ SYSTEM=EDI
    CCCHK RC(1:4095) FAIL CONTINUE
    RUN WEEKDAYS SAT
    IF TODAY('TUE WED THU FRI SAT') & ESPSHH EQ '02' THEN +
      NORUN TODAY
    IF TODAY('WEEKDAYS') & ESPSHH EQ '20' THEN +
      NORUN TODAY
    IF TODAY('WEEKDAYS') & ESPSHH EQ '22' THEN +
      NORUN TODAY
     /* NORUN HOLIDAYS
     RELEASE ADD(ELD811AD)
     ENDJOB

JOB ELD811AD
  OPTIONS NORESTARTSTEP
  RUN WEEKDAYS SAT
    IF TODAY('TUE WED THU FRI SAT') & ESPSHH EQ '02' THEN +
      NORUN TODAY
    IF TODAY('WEEKDAYS') & ESPSHH EQ '20' THEN +
     NORUN TODAY
    IF TODAY('WEEKDAYS') & ESPSHH EQ '22' THEN +
     NORUN TODAY
  /* NORUN HOLIDAYS
ENDJOB

