   /**************************************************
JOB OMW090
  CCCHK RC(5:4095) FAIL CONTINUE
  IF TODAY('SAT') & ESPSHH EQ '05' THEN +
    RUN TODAY
     ELSE +
    NORUN TODAY
  RELEASE ADD(ELR702)
  RELEASE ADD(ELM000)
  RELEASE ADD(OMM300)
  NOTWITH (OMD900.-)
ENDJOB

JOB ELR702
  OPTIONS NORESTARTSTEP
  CCCHK RC(1:4095) FAIL CONTINUE
  IF TODAY('SAT') & ESPSHH EQ '05' THEN +
    RUN TODAY
     ELSE +
    NORUN TODAY
  RELEASE ADD(ELW000)
  RELEASE ADD(ELR704)
ENDJOB

JOB ELW000
  CCCHK RC(1:4095) FAIL CONTINUE
  IF TODAY('SAT') & ESPSHH EQ '05' THEN +
    RUN TODAY
     ELSE +
