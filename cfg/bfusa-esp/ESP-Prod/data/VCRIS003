APPL VCRIS003 WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
/*********************************************************************/
/*04/29/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES */
/*********************************************************************/
NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   RELEASE ADD(MM70AMRB_KONS_SEND)
   RELEASE ADD(MM70AMRB_KONS_SEND_BFCA)
ENDJOB

SAP_JOB MM70AMRB_KONS_SEND
   AGENT CYBB_BFP030
   SAPJOBNAME MM70AMRB_KONS_SEND
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME MM70AMRB
   VARIANT KONS_SEND
   STEPUSER SAPADM
   LANGUAGE E
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   RECIPIENT SAPADM
   ARCMODE PRINT
   BANNER D
   PRINTCOPIES 001
   RUN TODAY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

SAP_JOB MM70AMRB_KONS_SEND_BFCA
   AGENT CYBB_BFP030
   SAPJOBNAME MM70AMRB_KONS_SEND_BFCA
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME MM70AMRB
   VARIANT KONS_SEND_BFCA
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTSPOOLNAME RMVKON00_S_B
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN TODAY
   RELEASE (LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
ENDJOB

