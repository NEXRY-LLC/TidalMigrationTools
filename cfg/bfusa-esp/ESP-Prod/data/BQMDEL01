NOTIFY ABEND FAILURE ALERT(REMD)
NOTIFY OVERDUE ALERT(LATE)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN 12TH DAY OF MONTH
   EARLYSUB 08.00
   RELEASE (ZFI_FYEAR_PERIOD_VARI_UPDATE)
ENDJOB

SAP_JOB ZFI_FYEAR_PERIOD_VARI_UPDATE
   AGENT SAP_BZQ100
   SAPJOBNAME ZFI_FYEAR_PERIOD_VARI_UPDATE
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFI_FYEAR_PERIOD_VARI_UPDATE
   VARIANT PERIOD_FYEAR
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZFI_FYEAR_PE
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN 12TH DAY OF MONTH
   RELEASE (ZFIGLI19_AUTO_RUN)
ENDJOB

SAP_JOB ZFIGLI19_AUTO_RUN
   AGENT SAP_BZQ100
   SAPJOBNAME ZFIGLI19_AUTO_RUN
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIGLI19
   VARIANT AUTO_RUN
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZFI_FYEAR_PE
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN 12TH DAY OF MONTH
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN 12TH DAY OF MONTH
ENDJOB


