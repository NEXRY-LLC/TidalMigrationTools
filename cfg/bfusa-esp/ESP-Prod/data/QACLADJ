   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_sap_initial_CL.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_SAP_TEMP_CLCMD)
ENDJOB

