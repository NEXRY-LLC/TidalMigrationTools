   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_initial_CL.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_TEMP_CLCMD)
ENDJOB

NT_JOB WA_DETAIL_SAP_TEMP_CLCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_temp_CL.cmd
   ARGS 5
