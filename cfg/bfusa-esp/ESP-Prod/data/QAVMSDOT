APPL QAVMSDOT WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY LATE FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

NT_JOB ITEM_MASTER_DELETED_CHECKSCMD EXTERNAL APPLID(QAITMMST)
  RUN THU
  RELEASE ADD(LIS.QAVMSDOT)
ENDJOB

JOB LIS.QAVMSDOT LINK PROCESS
  RUN WEEKDAYS SUN
  EARLYSUB 01.00
  RELEASE ADD(VMSDOTCMD)
ENDJOB

NT_JOB VMSDOTCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\vmsdot.cmd
  ARGS
  USER maestro
  RUN WEEKDAYS SUN
  RELEASE ADD(VMSDOT_CHECKS)
  RELEASE ADD(DOT_ITEM_MASTER_CHECKS)
ENDJOB

NT_JOB VMSDOT_CHECKS
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\vmsdot_checks.cmd
  ARGS
  USER maestro
  RUN WEEKDAYS SUN
  RELEASE ADD(LIE.QAVMSDOT)
ENDJOB

NT_JOB DOT_ITEM_MASTER_CHECKS
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\dot_item_master_checks.cmd
  ARGS
  USER maestro
  RUN WEEKDAYS SUN
  RELEASE ADD(PPIW_DOT_ITEM_MASTER_EXPORT)
ENDJOB

NT_JOB PPIW_DOT_ITEM_MASTER_EXPORT
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\ppiw_dot_item_master_export.cmd
  ARGS
  USER maestro
  RUN WEEKDAYS SUN
  RELEASE ADD(PPIW_DOT_ITEM_MASTER_EXPORT_CTL)
ENDJOB

NT_JOB PPIW_DOT_ITEM_MASTER_EXPORT_CTL
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\ppiw_dot_item_master_export_ctl.cmd
  ARGS
  USER maestro
  RUN WEEKDAYS SUN
  RELEASE ADD(DOT_ITEM_MASTER)
ENDJOB

NT_JOB DOT_ITEM_MASTER
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\dot_item_master.cmd
  ARGS
  USER maestro
  RUN WEEKDAYS SUN
  RELEASE ADD(LIE.QAVMSDOT)
ENDJOB

NT_JOB LIE.QAVMSDOT
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\LinkedInEnd.cmd
  ARGS
  USER maestro
  RUN WEEKDAYS SUN
  DUEOUT EXEC 07:00
ENDJOB

