APPL QALOISCK WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO NT                     */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND OVERDUE MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB LOIS_UNIVERSE_EXTRACTCMD EXTERNAL APPLID(QALOIS)
  RUN SUN
  RELEASE ADD(LIS.QALOISCK)
ENDJOB

JOB LIS.QALOISCK LINK PROCESS
  RUN SUN
  DELAYSUB 01.00
  RELEASE ADD(LOIS_UNIVERSE_EXTRACT_CHECKSCMD)
ENDJOB

NT_JOB LOIS_UNIVERSE_EXTRACT_CHECKSCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\lois_universe_extract_checks.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(LOIS_UNIVERSE_EXTRACT_DEL_CLM_CHKSCMD)
ENDJOB

NT_JOB LOIS_UNIVERSE_EXTRACT_DEL_CLM_CHKSCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\lois_universe_extract_del_clm_chks.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(LIE.QALOISCK)
ENDJOB

NT_JOB LIE.QALOISCK
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\LinkedInEnd.cmd
  USER maestro
  RUN SUN
  DUEOUT EXEC 08.00
ENDJOB

