SAP_JOB RSNAST00_BILLING_INVOICE_FAX_02AM
   AGENT CYBB_BFP030
   SAPJOBNAME RSNAST00_BILLING_INVOICE_FAX
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RSNAST00
   VARIANT ZBILLOUT_FAX
   STEPUSER INVOICE_FAX
   LANGUAGE E
   PRINTDEST LOCL
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT SOLMAN
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN WEEKDAYS SAT
   DELAYSUB 02.00
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB RESUME.BLD002PT LINK PROCESS
   RUN WEEKDAYS SAT
   RELDELAY 2
   ESP RESUME BL01.BLD002PT
   RELEASE ADD(BLDAKPR1_CHILDREN_COMPLETE)
ENDJOB

FILE_TRIGGER BLDAKPR1_CHILDREN_COMPLETE
   AGENT CYBB_BFP030
   FILENAME +
'/data/BFP/interfaces/output/dataout/zsdbli10_bld002p_complete.txt' +
CREATE NOCHANGE(1)
   RUN WEEKDAYS SAT
   RELEASE ADD(ARCBLDAKPR1)
ENDJOB

UNIX_JOB ARCBLDAKPR1
   AGENT CYBA_ISBFP3
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/output/dataout/zsdbli10_bld002p_complete.txt D
   USER bfpadm
   RUN WEEKDAYS SAT
   RELEASE ADD(SUSPEND.BLD002PT)
ENDJOB

JOB SUSPEND.BLD002PT LINK PROCESS
   RUN WEEKDAYS SAT
   ESP SUSPEND BL01.BLD002PT
   RELEASE ADD(BLDAKPR1.COMPLETE)
ENDJOB

AIX_JOB BLDAKPR1.COMPLETE
   AGENT CYBA_ISBFP3
   COMMAND /bin/sleep
   ARGS 3
   USER bfpadm
   RUN WEEKDAYS SAT
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
   DUEOUT EXEC 6AM
ENDJOB


