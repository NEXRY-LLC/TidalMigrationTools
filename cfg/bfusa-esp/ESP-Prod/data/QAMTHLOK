APPL QAMTHLOK WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO NT                     */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND OVERDUE MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

JOB LIS.QAMTHLOK LINK PROCESS
   RUN 2nd WORKDAY OF MONTH
   EARLYSUB 19.00
   RELEASE ADD(LOCK_PPIW_PROD_MONTHLYCMD)
ENDJOB

NT_JOB LOCK_PPIW_PROD_MONTHLYCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\LOCK_PPIW_PROD_MONTHLY.cmd
   ARGS
   USER maestro
   RUN 2nd WORKDAY OF MONTH
   RELEASE ADD(LOCK_QA_SCOPING_MONTHLYCMD)
ENDJOB

NT_JOB LOCK_QA_SCOPING_MONTHLYCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\LOCK_QA_SCOPING_MONTHLY.cmd
   ARGS
   USER maestro
   RUN 2nd WORKDAY OF MONTH
   RELEASE ADD(WEB_SITE_LOCK_PPIW_MONTHLYCMD)
   RELEASE ADD(PRODUCTION_LA_CONS_WKCMD)
   RELEASE ADD(PRODUCTION_LA_CONS_MOCMD)
ENDJOB

NT_JOB WEB_SITE_LOCK_PPIW_MONTHLYCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\WEB_SITE_LOCK_PPIW_MONTHLY.cmd
   ARGS
   USER maestro
   RUN 2nd WORKDAY OF MONTH
   RELEASE ADD(LIE.QAMTHLOK)
ENDJOB

NT_JOB PRODUCTION_LA_CONS_WKCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\PRODUCTION_LA_CONS_WK.cmd
   ARGS
   USER maestro
   RUN 2nd WORKDAY OF MONTH
   RELEASE ADD(LIE.QAMTHLOK)
ENDJOB

NT_JOB PRODUCTION_LA_CONS_MOCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\PRODUCTION_LA_CONS_MO.cmd
   ARGS
   USER maestro
   RUN 2nd WORKDAY OF MONTH
   RELEASE ADD(LIE.QAMTHLOK)
ENDJOB

NT_JOB LIE.QAMTHLOK
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\LinkedInEnd.cmd
   USER maestro
   RUN 2nd WORKDAY OF MONTH
ENDJOB

