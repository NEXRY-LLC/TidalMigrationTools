APPL DSDNCDLA WAIT

NOTIFY ABEND FAILURE ALERT(REMD)

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
OPTIONS NODUEOUTPROPAGATION

/*    JOB_NCD_ACES_DELTA_LOAD
/*    JOB_NCD_EDGE_DELTA_LOAD
/*    JOB_NCD_PRODUCT_SCD_LOAD
/*    JOB_NCD_LOC_STORE_SCD_LOAD
/*    JOB_NCD_TIRE_SRCH_DELTA_LOAD

JOB DSDNCDLA LINK PROCESS
    /*RUN DAILY
    DELAYSUB 02.00
    RELEASE ADD(DS_NCD_TIRE_SRCH_DELTA_LOAD)
    RELEASE ADD(DS_NCD_EDGE_DELTA_LOAD)
    RELEASE ADD(DS_NCD_LOC_STORE_SCD_LOAD)
    RELEASE ADD(DS_NCD_PRODUCT_SCD_LOAD)
    RELEASE ADD(DS_NCD_ACES_DELTA_LOAD)
ENDJOB

FILE_TRIGGER BSRO_FILE_TIRE_SRCH_DELTA_LOAD_COMPLETE
   AGENT LNX_DSRSAP01PAKR
   FILENAME '/mnt/bods/hems/prod/outbound/+
bsro_tdw_tsdc_adw*' CREATE NOCHANGE(1)
   RUN DAILY
   RELEASE ADD(DS_NCD_TIRE_SRCH_DELTA_LOAD)
ENDJOB

