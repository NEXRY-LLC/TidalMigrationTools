APPL SAD008B WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB RBDAPP01_CRT_BSRO_ORDERS EXTERNAL SCOPE(-02.00) +
APPLID(SAD008A)
   RUN DAILY
   IF ESPSHH LT '21' THEN RUN TODAY
   IF ESPSHH EQ '21' THEN NORUN TODAY
   IF ESPSHH GT '22' THEN RUN TODAY
   RELEASE ADD(RBDMANI2_REPROCESS_BSRO_ORDERS)
ENDJOB

SAP_JOB RBDAPP01_CRT_BSRO_ORDERS_9PM EXTERNAL SCOPE(-02.00) +
APPLID(SAD008A)
   RUN DAILY
   IF ESPSHH LT '21' THEN NORUN TODAY
   IF ESPSHH GT '22' THEN NORUN TODAY
   RELEASE ADD(RBDMANI2_REPROCESS_BSRO_ORDERS)
ENDJOB

SAP_JOB RBDMANI2_REPROCESS_BSRO_ORDERS
   AGENT CYBB_BFP030
   SAPJOBNAME RBDMANI2_REPROCESS_BSRO_ORDERS
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RBDMANI2
   VARIANT ZMTSD_JDA_ORD
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST LP01
   PRINTCOPIES 001
   EXPIRATION 8
   LINES 65
   COLUMNS 132
   BANNER D
   RECIPIENT ZFF_OTC_CO23
   ARCMODE PRINT
   RUN DAILY
ENDJOB


