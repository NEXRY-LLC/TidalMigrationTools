APPL QASCS001 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO NT                     */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW

JOB LIS.QASCS001 LINK PROCESS
   RUN WEEKDAYS SAT
   IF TODAY('JULY 15 2017') THEN JOBATTR HOLD
   EARLYSUB 07.45
   RELEASE ADD(SCS_JOL_ITM_MFG_XREF_TEMPCMD)
ENDJOB

NT_JOB SCS_JOL_ITM_MFG_XREF_TEMPCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\CMD_SCRIPTS\SCS_JOL_ITM_MFG_XREF_TEMP.cmd
   USER maestro
   RUN WEEKDAYS SAT
   RELEASE ADD(SCS_JOL_ITM_MFG_XREF_TEMP_CHKCMD)
ENDJOB

