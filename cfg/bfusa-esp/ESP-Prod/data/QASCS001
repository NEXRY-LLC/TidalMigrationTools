   AGENT CYBA_PPIWPROD
   CMDNAME X:\CMD_SCRIPTS\SCS_JOL_ITM_MFG_XREF_TEMP_CHK.cmd
   USER maestro
   RUN WEEKDAYS SAT
   RELEASE ADD(SCS_JOL_ITM_MFG_XREFCMD)
ENDJOB

