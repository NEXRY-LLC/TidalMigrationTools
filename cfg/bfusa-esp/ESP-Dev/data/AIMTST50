APPL AIMTST50 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*06/12/2018 -VMD NEW APPL CREATED FOR TESTING PER Rakhee Regmi*/
/*********************************************************************/

/*NOTIFY FAILURE ABEND ALERT(REMD) */

FILE_TRIGGER F181_RAW_MATERIAL_CONSUMPTION_FILE_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME '/data/BFT/interfaces/input/datain/+
   zmmimi07_iid_f181' CREATE
   RUN DAILY
   RELEASE ADD(ZMMIMI07_f181)
ENDJOB

SAP_JOB ZMMIMI07_F181
   AGENT CYBB_BFT030
   SAPJOBNAME ZMMIMI07_F181
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMIMI07
   VARIANT F181
   STEPUSER SAPADM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMMIMI07_SAP
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 132
   PRINTFORMAT X_PAPER
   BANNER D
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   RUN DAILY
   RELEASE ADD(ARCHIVE_ZMMIMI07_IID_F181)
ENDJOB

UNIX_JOB ARCHIVE_ZMMIMI07_IID_F181
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/zmmimi07_iid_f181 W
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
ENDJOB

