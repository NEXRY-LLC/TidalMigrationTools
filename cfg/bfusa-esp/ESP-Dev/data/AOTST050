/*NOTIFY FAILURE ABEND ALERT(REMD)

FILE_TRIGGER F191_RAW_MATERIAL_CONSUMPTION_FILE_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/f191_rm_gi CREATE
   RUN DAILY
   RELEASE ADD(ZMMIMI04_F191_RM_GI)
ENDJOB

SAP_JOB ZMMIMI04_F191_RM_GI
    SAPUSER SAPADM
    AGENT CYBB_BFT030
    SAPJOBNAME ZMMIMI04_F191_RM_GI
    SAPJOBCLASS C
    ABAPNAME ZMMIMI04
    VARIANT 'VARIANT F191'
    LANGUAGE E
    PRINTDEST C-AI
    PRINTCOPIES 001
    PRINTIMMED YES
    EXPIRATION 8
    COLUMNS 132
    BANNER D
    RECIPIENT DUETTAD
    ARCMODE PRINT
    RUN DAILY
    RELDELAY 1
    NOTIFY OVERDUE ALERT(AI03)
   /* IF ESPSHH = '06' THEN DUEOUT EXEC 10.30
   /* IF ESPSHH = '01' THEN DUEOUT EXEC 06.00
    RELEASE ADD(AR_F191_RM_GI)
ENDJOB

UNIX_JOB AR_F191_RM_GI
    AGENT CYBA_ISBFT
    USER bftadm
    SCRIPTNAME /local/bin/arcfile.sh
    ARGS interfaces/input/datain/f191_rm_gi W
    RUN DAILY
    RELEASE ADD(LIE.AOTTD050)
ENDJOB

JOB LIE.AOTTD050 LINK PROCESS
    RUN DAILY
ENDJOB

