/* CHANGED DUEOUT TO 130 MIN TO INCLUDE 2 HR EVENT WINDOW - DSG
/* ADDED EXTERNAL FOR JDAOP003 PER PRESTON FIELD - DSG - 8/14/18
/* REMOVED LINUX_JOB DS_JDA_IDP_INVENTORY
/*              PER PRESTON FIELD - DSG - 8/16/18
/* 03/06/22 -VV- created new APPL for DSD jobs
/* 03/25/22 -VV- RITM0141495- new job added-master copy
/***************************************************************

LINUX_JOB LIE.JDAOP003 EXTERNAL APPL(JDAOP003)
   RUN DAILY
   RELEASE ADD(DS_JDA_IDP_CUSTORDER_BATO)
   RELEASE ADD(DS_JDA_IDP_OPENDELIVERIES)
   RELEASE ADD(DS_JDA_IDP_STOCK_TRANSFERS_BATO)
   RELEASE ADD(DS_JDA_IDP_INVENTORY_BATO)
   RELEASE ADD(DS_JDA_IDP_PURCHASE_ORDER_SAP)
   RELEASE ADD(DS_JDA_IDP_INVENTORY)
   RELEASE ADD(DS_JDA_IDP_CUSTOMER_MASTER_CPY)
IF ESPSHH = "09" THEN DELAYSUB 11.30
IF ESPSHH = "21" THEN DELAYSUB 23.30
IF ESPSHH = "17" THEN DELAYSUB 19.00
ENDJOB
LINUX_JOB DS_JDA_IDP_CUSTORDER_BATO
    AGENT LNX_DSRSAP01PAKR
    USER bodsadmin
    /*SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/
    SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
    JOB_JDA_IDP_CUSTORDER_BATO_CPY.sh
    RUN DAILY
    RELEASE ADD(DSDJDA01_3AM_NOW_COMPLETE)
    RELEASE ADD(DSDJDA01_12PM_NOW_COMPLETE)
 ENDJOB

