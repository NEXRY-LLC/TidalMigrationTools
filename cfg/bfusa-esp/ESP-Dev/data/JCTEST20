APPL JCTEST20 WAIT NOPOST_WHILE_WAITING

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/* TEMPLATES - THIS APPLICATION IS OPEN FOR TESTING                  */
/*********************************************************************/

TAG TEST

TEMPLATE BAOPJOB (3,JOBNAME,SCRIPT,ARGUMENTS,NEXT())
  LINUX_JOB !JOBNAME
    AGENT CFNA_BAOPDEV001
    USER ROOT
    SCRIPTNAME /OPT/CA/BAOP-AGENT-SCRIPTS/-
    !SCRIPT
    ARGS !ARGUMENTS
    EXITCODE 0 SUCCESS
    RUN !S201
    NOTIFY JOBEND ALERT(CF01)
    IF NEXT NE '' THEN RELEASE ADD(!NEXT)
  ENDJOB
ENDTEMPL

/*FOR EACH JOB - ENTER THE FOLLOWING-IN ORDER-WITH CONINUATION AT END*/
/*BAOPJOB FOLLOWED BY JOBNAME
/*SCRIPT NAME ONLY - PATH SHOULD BE THE SAME AS TEMPLATE JOB
/*ARGUMENTS
/*NEXT JOB TO RELEASE IN APPLNTS AND NEXT JOB NAME FOR EACH NEW JOB*/

BAOPJOB CFNA_PULL_NEW_TRANS_TO_STAGING -
STARTBECC101ATRANSFER.SH -
ARG1 PULLINNEWTRANSACTIONSTOSTAGING BAOP-STAGE-USER TEST1234 -
CFNA_ETL_NEW_TRANS_INBOUND

BAOPJOB CFNA_ETL_NEW_TRANS_INBOUND













