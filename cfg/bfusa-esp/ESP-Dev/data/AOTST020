APPL AOTST020 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*05/15/2018-JFC-MOVED ARCHIVE JOBS TO RUN BEFORE THE AS400 JOB
/**********************************************************************/

/*NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   IF ESPSHH EQ '09' THEN DELAYSUB 11.00
   IF ESPSHH EQ '19' THEN DELAYSUB 21.00
   RELEASE ADD(ARCHIVE_BOM1T)
   RELEASE ADD(ARCHIVE_BOM2T)
   RELEASE ADD(ARCHIVE_BOM3T)
   RELEASE ADD(ARCHIVE_MMT)
ENDJOB

UNIX_JOB ARCHIVE_BOM1T
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom1t_f191.txt D
   RUN DAILY
   RELEASE ADD(AOMS_SAP)
ENDJOB

UNIX_JOB ARCHIVE_BOM2T
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom2t_f191.txt D
   RUN DAILY
   RELEASE ADD(AOMS_SAP)
ENDJOB

UNIX_JOB ARCHIVE_BOM3T
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom3t_f191.txt D
   RUN DAILY
   RELEASE ADD(AOMS_SAP)
ENDJOB

UNIX_JOB ARCHIVE_MMT
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/mmt_f191.txt D
   RUN DAILY
   RELEASE ADD(AOMS_SAP)
ENDJOB

AS400_JOB AOMS_SAP
   AGENT AKNDEV
   RUN DAILY
   COMMAND CYBROBOT AOMS_SAP
   JOBQ CYBESPV210.CYBESPV210
   RELEASE ADD(BOM1T_F191_FROM_SI)
   RELEASE ADD(BOM2T_F191_FROM_SI)
   RELEASE ADD(BOM3T_F191_FROM_SI)
   RELEASE ADD(MMT_F191_FROM_SI)
   RELEASE ADD(ZMMBI002_CFS_F191)
ENDJOB

FILE_TRIGGER BOM1T_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/bom1t_f191.txt CREATE
   RUN DAILY
   RELEASE ADD(ZMMBI002_TESS_F191)
ENDJOB

FILE_TRIGGER BOM2T_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/bom2t_f191.txt CREATE
   RUN DAILY
   RELEASE ADD(ZMMBI002_TESS_F191)
ENDJOB

FILE_TRIGGER BOM3T_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/bom3t_f191.txt CREATE
   RUN DAILY
   RELEASE ADD(ZMMBI002_TESS_F191)
ENDJOB

FILE_TRIGGER MMT_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/mmt_f191*      create
   RUN DAILY
   RELEASE ADD(ZMMBI002_TESS_F191)
ENDJOB

SAP_JOB ZMMBI002_TESS_F191
   AGENT CYBB_BFT030
   SAPJOBNAME ZMMBI002_TESS_F191
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMBI002
   VARIANT ZMMBI002_F191T
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMMBI002_SAP
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 132
   PRINTFORMAT X_65_132
   BANNER D
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   RUN DAILY
   RELEASE ADD(ZMMBI025_TESS_F191)
ENDJOB

SAP_JOB ZMMBI025_TESS_F191
   AGENT CYBB_BFT030
   SAPJOBNAME ZMMBI025_TESS_F191
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMBI025
   VARIANT ZMMBI025_F191T
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMMBI025_SAP
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 132
   PRINTFORMAT X_65_132
   BANNER D
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   RUN DAILY
   RELEASE ADD(ZMMBI018_TESS_F191)
ENDJOB

SAP_JOB ZMMBI018_TESS_F191
   AGENT CYBB_BFT030
   SAPJOBNAME ZMMBI018_TESS_F191
   SAPJOBCLASS C
   ABAPNAME ZMMBI018
   VARIANT ZMMBI018_F191T
   STEPUSER SAPADM
   LANGUAGE E
