   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\vmsdot_bfrc_control_export_script.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(VMSDOT_BFRC_EXPORT_SCRIPTCMD)
ENDJOB

NT_JOB VMSDOT_BFRC_EXPORT_SCRIPTCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\vmsdot_bfrc_export_script.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(WHSE_PLT_XREF_BFRC_EXPORT_SCRIPTCMD)
ENDJOB

NT_JOB WHSE_PLT_XREF_BFRC_EXPORT_SCRIPTCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\whse_plt_xref_bfrc_export_script.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(CD_VMSDOT_BFRC_EXPORTCDP)
ENDJOB

NT_JOB CD_VMSDOT_BFRC_EXPORTCDP
  AGENT CYBA_CDC1AKFIS
  EARLYSUB REALNOW PLUS 10 SECONDS
  PID= WOBDATA('PARMSET.!ESPAPPL','PID')
  TMSTMP= WOBDATA('PARMSET.!ESPAPPL','TMSTMP')
  CMDNAME D:\site\apps\cdnt\batchcmd\submit.cmd
  ARGS cd_vmsdot_bfrc_export.cdp 0 !PID
  USER cdprod
  RUN SUN
  RELEASE ADD(CD_WHSE_PLT_XREF_BFRC_EXPORTCDP)
ENDJOB

NT_JOB CD_WHSE_PLT_XREF_BFRC_EXPORTCDP
  AGENT CYBA_CDC1AKFIS
  EARLYSUB REALNOW PLUS 10 SECONDS
  PID= WOBDATA('PARMSET.!ESPAPPL','PID')
  TMSTMP= WOBDATA('PARMSET.!ESPAPPL','TMSTMP')
  CMDNAME D:\site\apps\cdnt\batchcmd\submit.cmd
  ARGS cd_whse_plt_xref_bfrc_export.cdp 0 !PID
  USER cdprod
  RUN SUN
  RELEASE ADD(CD_VMSDOT_BFRC_CONTROL_EXPORTCDP)
ENDJOB

NT_JOB CD_VMSDOT_BFRC_CONTROL_EXPORTCDP
  AGENT CYBA_CDC1AKFIS
  EARLYSUB REALNOW PLUS 10 SECONDS
  PID= WOBDATA('PARMSET.!ESPAPPL','PID')
  TMSTMP= WOBDATA('PARMSET.!ESPAPPL','TMSTMP')
  CMDNAME D:\site\apps\cdnt\batchcmd\submit.cmd
  ARGS cd_vmsdot_bfrc_control_export.cdp 0 !PID
  USER cdprod
  RUN SUN
  RELEASE ADD(LIE.QABFRC)
ENDJOB

JOB LIE.QABFRC LINK PROCESS
   RUN SUN
    /*NO DOWNSTREAM APPLS
ENDJOB

