/*EXTERNAL APPLID(QAVMSDOT) - QATHUSUN

DATA_OBJECT PARMSET.!ESPAPPL
   /*Create a timestamp in hhmmMMDDYY format*/
   SETVAR TMSTMP='!ESPSHH.:!ESPSMN.,!ESPSMM./!ESPSDD./!ESPSYY'
   SETVAR PID=''
   SETVAR STMPTM='!ESPSMM./!ESPSDD./!ESPSYY.,!ESPSHH.:!ESPSMN'
ENDJOB

JOB SETPID.!ESPAPPL LINK PROCESS
   VINCR (PID) TABLE(CDPROD) CLANG
   VGET  (PID) TABLE(CDPROD) CLANG
   ESP MGRMSG * . . . PARMSET.!ESPAPPL/!ESPAPPL..!ESPAPGEN/MAIN +
   ACTION SET PID(!PID)
   RUN SUN
   RELEASE ADD(CD_VMSDOT_BFRC_EXPORTCDP)
ENDJOB

JOB SETPIDA.!ESPAPPL LINK PROCESS
   VINCR (PID) TABLE(CDPROD) CLANG
   VGET  (PID) TABLE(CDPROD) CLANG
   ESP MGRMSG * . . . PARMSET.!ESPAPPL/!ESPAPPL..!ESPAPGEN/MAIN +
   ACTION SET PID(!PID)
   RUN SUN
   RELEASE ADD(CD_WHSE_PLT_XREF_BFRC_EXPORTCDP)
ENDJOB

JOB SETPIDB.!ESPAPPL LINK PROCESS
   VINCR (PID) TABLE(CDPROD) CLANG
   VGET  (PID) TABLE(CDPROD) CLANG
   ESP MGRMSG * . . . PARMSET.!ESPAPPL/!ESPAPPL..!ESPAPGEN/MAIN +
   ACTION SET PID(!PID)
   RUN SUN
   RELEASE ADD(CD_VMSDOT_BFRC_CONTROL_EXPORTCDP)
ENDJOB

JOB LIS.QABFRC LINK PROCESS
  RUN SUN
  EARLYSUB 01.00
  RELEASE ADD(VMSDOT_BFRC_EXPORTCMD)
ENDJOB

NT_JOB VMSDOT_BFRC_EXPORTCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\vmsdot_bfrc_export.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(WHSE_PLT_XREF_BFRC_EXPORTCMD)
ENDJOB

NT_JOB WHSE_PLT_XREF_BFRC_EXPORTCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\whse_plt_xref_bfrc_export.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(VMSDOT_BFRC_CONTROLCMD)
ENDJOB

NT_JOB VMSDOT_BFRC_CONTROLCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\vmsdot_bfrc_control.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(VMSDOT_BFRC_CONTROL_EXPORTCMD)
ENDJOB

