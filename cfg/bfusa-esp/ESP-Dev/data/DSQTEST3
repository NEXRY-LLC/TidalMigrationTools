APPL DSQTEST3 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*****************************************************************/
/*08/22/2019 -JFC- APPLICATION TO TEST FILE TRIGGER FOR TABLEAU
/*****************************************************************/

JOB LIS.!ESPAPPL LINK PROCESS
  RUN TODAY
  RELEASE ADD(BSRO_PRICING_DISCOUNTING_TRIGGER)
ENDJOB

NT_JOB BSRO_PRICING_DISCOUNTING_TRIGGER
  AGENT CYBA_BIRDMAKFIS
  USER sapcyber
  CMDNAME "\\BIRDP-AK-FIS\DS_Tableau_Triggers\QA\Script\+
          BSRO_Pricing_Discounting_Trigger.bat"
  RUN TODAY
  RELEASE ADD(DISCPRICINGLOADGEN)
ENDJOB

FILE_TRIGGER DISCPRICINGLOADGEN
  AGENT CYBA_BIRDMAKFIS
  FILENAME '\\BIRDP-AK-FIS\DS_TABLEAU_TRIGGERS\QA\OUTPUT\+
           DiscPricingLoadGen_2019*.done' CREATE NOCHANGE(1)
  RUN TODAY
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
  RUN TODAY
ENDJOB


