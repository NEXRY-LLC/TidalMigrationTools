APPL AOTST010 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*05/15/2018 -JFC-MOVED ARCHIVE JOBS TO RUN BEFORE THE AS400 JOB
/**********************************************************************/

/*NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   IF ESPSHH EQ '09' THEN DELAYSUB 11.00
   IF ESPSHH EQ '19' THEN DELAYSUB 21.00
   RELEASE ADD(ARCHIVE_BOM1)
   RELEASE ADD(ARCHIVE_BOM2)
   RELEASE ADD(ARCHIVE_BOM3)
   RELEASE ADD(ARCHIVE_MMC)
ENDJOB

UNIX_JOB ARCHIVE_BOM1
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom1c_f191.txt D
   RUN DAILY
   RELEASE ADD(SAPCFAO)
ENDJOB

UNIX_JOB ARCHIVE_BOM2
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom2c_f191.txt D
   RUN DAILY
   RELEASE ADD(SAPCFAO)
ENDJOB

UNIX_JOB ARCHIVE_BOM3
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom3c_f191.txt D
   RUN DAILY
   RELEASE ADD(SAPCFAO)
ENDJOB

UNIX_JOB ARCHIVE_MMC
   AGENT CYBA_ISBFT
   USER bftadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/mmc_f191.txt D
   RUN DAILY
   RELEASE ADD(SAPCFAO)
ENDJOB

AS400_JOB SAPCFAO
   AGENT AKNDEV
   COMMAND CYBROBOT SAPCFAO
   JOBQ CAWAAGENT.CYBESPJOBS
 /*JOBQ CYBESPV210.CYBESPV210
   RUN DAILY
   RELEASE ADD(BOM1C_F191_FROM_SI)
   RELEASE ADD(BOM2C_F191_FROM_SI)
   RELEASE ADD(BOM3C_F191_FROM_SI)
   RELEASE ADD(MMC_F191_FROM_SI)
   RELEASE ADD(ZMMBI002_CFS_F191)
ENDJOB

FILE_TRIGGER BOM1C_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/bom1c_f191.txt CREATE
   RUN TODAY
   RELEASE ADD(ZMMBI002_CFS_F191)
ENDJOB

FILE_TRIGGER BOM2C_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/bom2c_f191.txt CREATE
   RUN TODAY
   RELEASE ADD(ZMMBI002_CFS_F191)
ENDJOB

FILE_TRIGGER BOM3C_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/bom3c_f191.txt CREATE
   RUN TODAY
   RELEASE ADD(ZMMBI002_CFS_F191)
ENDJOB

FILE_TRIGGER MMC_F191_FROM_SI
   AGENT CYBA_ISBFT
   FILENAME /data/BFT/interfaces/input/datain/mmc_f191.txt   create
   RUN TODAY
   RELEASE ADD(ZMMBI002_CFS_F191)
ENDJOB

SAP_JOB ZMMBI002_CFS_F191
  /* THIS JOB PROCESSES FILE mmc_f191.txt */
   AGENT CYBB_BFT030
   SAPJOBNAME ZMMBI002_CFS_F191
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMBI002
   VARIANT ZMMBI002_F191C
   STEPUSER SAPADM
   LANGUAGE E
   PRINTSPOOLNAME ZMMBI002_SAP
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 132
   PRINTFORMAT X_PAPER
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   PRINTCOPIES 001
   BANNER D
   RUN DAILY
   RELDElAY 1
   RELEASE (ZMMBI025_CFS_F191)
ENDJOB

