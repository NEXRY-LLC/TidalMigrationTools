APPL DGEVNTST WAIT
INVOKE 'SWS.ESP.APPL.TESTLIB(#DISTRIB)'
/*********************************************************************/
/*12/09/2016 -JFC- APPL TO TEST ESP AND SM36 ON SAP*/
/*********************************************************************/
AGENTNAME= ''
IF !USER1 = '' THEN !AGENTNAME = 'CYBB_BFT030'
IF !USER1 = 'BFP' THEN !AGENTNAME = 'CYBB_BFP030'
IF !USER1 = 'BFT' THEN !AGENTNAME = 'CYBB_BFT030'
IF !USER1 = 'BFG' THEN !AGENTNAME = 'CYBB_BFG030'
IF !USER1 = 'IZP' THEN !AGENTNAME = 'SAP_IZP100'
IF !USER1 = 'BZP' THEN !AGENTNAME = 'SAP_BZP100'
/*ENTER YOUR USER NAME IN USER2 PARAMETER FOR SPOOLFILE EMAIL */

SAP_JOB Z_TEST_CYBERMATION_BFT030
   AGENT CYBB_BFT030
   SAPJOBNAME Z_TEST_CYBERMATION
   SAPJOBCLASS A
   STARTMODE ASAP
   SPOOLRECIPIENT 'GRIMMETTDAVI@BFUSA.COM'
   RECIPIENTTYPE INT
   RECIPIENTBCC NO
   RECIPIENTCC NO
   RUN TODAY
   RELDELAY  15
   ABAPNAME RSUSR000
   STEPUSER SAPADM
   ARCMODE PRINT
   LINES 50
   PRINTNEWSPOOL YES
   RELEASE (Z_TEST_CYBER2)
ENDJOB

SAPE_JOB SAPEM1
   AGENT CYBB_BFT030
   RELEASE (Z_TEST_CYBERMATION_BFT030)
   EVENT 'ZSD_CONTAINER_INV' REGISTER
   RUN TODAY
ENDJOB


