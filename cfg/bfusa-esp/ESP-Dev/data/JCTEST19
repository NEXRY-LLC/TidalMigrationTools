APPL JCTEST19 WAIT NOPOST_WHILE_WAITING

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/* TEMPLATES - THIS APPLICATION IS FOR TESTING TEMPLATES             */
/*********************************************************************/

TAG TEST

TEMPLATE MULTIRUN (1,OFFSET NEXT() RUN())
  JOB SWESPJC2.!OFFSET
    RUN !RUN
    DELAYSUB !OFFSET
    RELEASE ADD(!NEXT)
  ENDJOB
ENDTEMPL

/***MULTIRUN NUMBERS REPRESENT DELAYSUB TIMES FOR EACH JOB***/
MULTIRUN 00.15 NEXT(SWESPJC2.01.15) RUN(DAILY)
MULTIRUN 01.15 NEXT(SWESPJC2.02.15) RUN(WEEKDAYS)
MULTIRUN 02.15 NEXT(SWESPJC2.03.15) RUN(DAILY)
MULTIRUN 03.15 NEXT(SWESPJC2.04.15) RUN(WEEKDAYS)
MULTIRUN 04.15 NEXT(SWESPJC2.05.15) RUN(DAILY)
MULTIRUN 05.15 NEXT(SWESPJC2.05.45) RUN(WEEKDAYS)
MULTIRUN 05.45 NEXT(SWESPJC2.06.15) RUN(WEEKDAYS)
MULTIRUN 06.15 NEXT(SWESPJC2.06.45) RUN(DAILY)
MULTIRUN 06.45 NEXT(SWESPJC2.07.15) RUN(WEEKDAYS)
MULTIRUN 07.15 NEXT(SWESPJC2.07.45) RUN(WEEKDAYS)
MULTIRUN 07.45 NEXT(SWESPJC2.08.15) RUN(WEEKDAYS)
MULTIRUN 08.15 NEXT(SWESPJC2.08.45) RUN(DAILY)
MULTIRUN 08.45 NEXT(SWESPJC2.09.15) RUN(WEEKDAYS)
MULTIRUN 09.15 NEXT(SWESPJC2.09.45) RUN(WEEKDAYS)
MULTIRUN 09.45 NEXT(SWESPJC2.10.15) RUN(WEEKDAYS)
MULTIRUN 10.15 NEXT(SWESPJC2.10.45) RUN(DAILY)
MULTIRUN 10.45 NEXT(SWESPJC2.11.15) RUN(WEEKDAYS)
MULTIRUN 11.15 NEXT(SWESPJC2.11.45) RUN(WEEKDAYS)
MULTIRUN 11.45 NEXT(SWESPJC2.12.15) RUN(WEEKDAYS)
MULTIRUN 12.15 NEXT(SWESPJC2.12.45) RUN(DAILY)
MULTIRUN 12.45 NEXT(SWESPJC2.13.15) RUN(WEEKDAYS)
MULTIRUN 13.15 NEXT(SWESPJC2.13.45) RUN(WEEKDAYS)
MULTIRUN 13.45 NEXT(SWESPJC2.14.15) RUN(WEEKDAYS)
MULTIRUN 14.15 NEXT(SWESPJC2.14.45) RUN(DAILY)
