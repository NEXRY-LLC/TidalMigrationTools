APPL SCTEST01 WAIT

INVOKE 'SWS.ESP.APPL.TESTLIB(#DISTRIB)'

/*********************************************************************/
/*12/09/2016 -JFC- APPL TO TEST ESP AND SM36 ON SAP                  */
/*********************************************************************/

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   RELEASE ADD(RKAEP000_LEGAL_DD)
ENDJOB

SAP_JOB RKAEP000_LEGAL_DD
  AGENT CYBB_BFT030
     SAPJOBNAME RKAEP000_LEGAL_DD
     SAPJOBCLASS C
     ABAPNAME RKAEP000
     VARIANT 'LEGAL DD'
     STEPUSER SAPADM
     LANGUAGE E
    PRINTDEST WINP
     PRINTCOPIES 001
     PRINTSPOOLNAME RKAEP000_SAP
     PRINTREL YES
     PRINTNEWSPOOL YES
     EXPIRATION 8
     LINES 65
     COLUMNS 80
     PRINTFORMAT X_65_80
     BANNER D
     RECIPIENT SAPADM
     PRINTDEPARTMENT BASIS
     ARCMODE PRINT
   RUN TODAY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB


JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
ENDJOB


