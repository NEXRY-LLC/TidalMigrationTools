APPL LEI05549 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY ABEND FAILURE ALERT(REMD)
NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)

/**********************************************************************/
/*CREATED ON 12/22/15 PER A. JACOBS - MFM
/* JOB PREVENTS LOG FILES FROM FILLING UP.
/* 6/29/17 - DSG - RESTRUCTURED JOB PER REMEDY TICKET INC000001335608
/**********************************************************************/
/*THIS APPL USES A TEMPLATE TO REPLICATE THE JOB                      */
/***MULTIRUN NUMBERS REPRESENT DELAYSUB TIMES FOR EACH JOB          ***/
/**********************************************************************/

TEMPLATE MULTIRUN (1,OFFSET NEXT() RUN())
  SAP_JOB RSXMB_RESTART_MESSAGES_INT49_!OFFSET
     AGENT CYBB_BFP030
     SAPJOBNAME RSXMB_RESTART_MESSAGES_INT49
     SAPJOBCLASS C
     STARTMODE ASAP
     ABAPNAME RSXMB_RESTART_MESSAGES
     VARIANT /INT49_RESTART
     SAPEMAILADDR 'RESOURCE_SAPLOGISTICS_OWNERS@BFUSA.COM,-
     FUJITSU-AMS-ONSITE@BFUSA.COM,FUJITSU-AMS-OFFSHORE@BFUSA.COM,-
     SAPPISUPPORT@BFUSA.COM'
     STEPUSER SAPADM
     LANGUAGE E
     PRINTCOPIES 001
     EXPIRATION 8
     LINES 65
     COLUMNS 132
     BANNER D
     RECIPIENT SAPADM
     ARCMODE PRINT
      RUN !RUN
      DELAYSUB !OFFSET
   /*DUEOUT EXEC NOW PLUS 20 MINUTES
      RELEASE ADD(!NEXT)
  ENDJOB
ENDTEMPL

