APPL QATEST16 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*APPL TO RUN PPIW/DRP FILE TRANSFER JOBS ON BFT - TEST - TEST -TEST */
/* **IF ANY JOBS FAIL - JUST MARK THEM COMPLETE - NO ACTION NEEDED   */
/*********************************************************************/

NOTIFY FAILURE ABEND MAILBOX(SCHEDULERS)
TAG TEST

JOB LIS.!ESPAPPL LINK PROCESS
  RUN DAILY
  IF ESPSHH = '05' THEN DELAYSUB 7.00
  IF ESPSHH = '09' THEN DELAYSUB 11.30
  IF ESPSHH = '16' THEN DELAYSUB 18.00
  IF ESPSHH = '21' THEN DELAYSUB 23.00
  RELEASE ADD(ZMMIMI37_STO_DRP)
  RELEASE ADD(ZMMIMI44_INVENTORY_EXT_DRP)
  RELEASE ADD(ZMMPUI01_PO_DRP)
  RELEASE ADD(ZSDLEI07_SALES_ORDERS_DRP_ALL)
  RELEASE ADD(ZSDLEI07_SALES_ORDERS_DRP_D002)
  RELEASE ADD(ZSDLEI07_SALES_ORDERS_DRP_D005)
  RELEASE ADD(ZSDLEI07_SALES_ORDERS_DRP_D009)
  RELEASE ADD(ZSDLEI07_SALES_ORDERS_DRP_D012)
  RELEASE ADD(ZSDLEI07_SALES_ORDERS_DRP_D014)
  RELEASE ADD(ZSDLEI12_SCHED_AGREEMENTS_DRP)
ENDJOB

SAP_JOB ZMMIMI37_STO_DRP
   AGENT CYBB_BFT030
   SAPJOBNAME ZMMIMI37_STO_DRP
   SAPJOBCLASS C
   ABAPNAME ZMMIMI37
   VARIANT STOTODRP
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST WINP
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMMIMI37_SAP
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
  RUN DAILY
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

