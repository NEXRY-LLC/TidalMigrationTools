APPL JCTEST10 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/******************************************************************/
/* THIS APPL WILL GENERATE MULTIPLE JOBS WITH SAME NAME           */
/* MAY BE USEFUL FOR SOMETHING DOWN THE ROAD                      */
/******************************************************************/

/*NOTIFY FAILURE ABEND END OVERDUE MAILBOX(SCHEDULERS)
RESOURCE ADD(1,JCTEST16)
TAG TEST

REXXON GEN
 DO I=1 TO 10
  "JOB SWESPJC2."I
   "CCCHK RC(16:4095) FAIL CONTINUE"
   "RUN TODAY"
   "RELEASE ADD(JCTEST1B)"
  "ENDJOB"
 END
REXXOFF

REXXON GEN
 DO I=00 TO 25
  "JOB JCTEST10."I LINK PROCESS
 /*"RUN TODAY"
   "RELEASE ADD(JCTEST1B)"
  "ENDJOB"
 END
REXXOFF

JOB JCTEST1B LINK PROCESS
  RUN TODAY
ENDJOB

