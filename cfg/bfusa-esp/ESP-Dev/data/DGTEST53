APPL DGTEST53 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB LIS.SWSAPTST LINK PROCESS
   RUN TODAY
   /*DELAYSUB 9.20
   /*RELDELAY 120*/
   RELEASE ADD(Z_TEST_CYBER2)
ENDJOB

SAP_JOB Z_TEST_CYBER2
   AGENT CYBB_BFT030
   SAPJOBNAME Z_TEST_CYBER2
   SPOOLRECIPIENT 'GRIMMETTDAVI@BFUSA.COM'
   RECIPIENTTYPE INT
   SAPJOBCLASS C
   STARTMODE ASAP
   BANNER D
   PRINTCOPIES 001
   NORUN DAILY
   ABAPNAME RSUSR000
   STEPUSER SAPADM
   LANGUAGE E
   PRINTSPOOLNAME RSUSR000_SAP
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 144
   PRINTFORMAT X_PAPER
   RECIPIENT SAPADM
   PRINTDEPARTMENT BASIS
   ARCMODE PRINT
   RUN TODAY
   DELAYSUB 9.20
   RELEASE ADD(Z_TEST_CYBER3)
ENDJOB

SAP_JOB Z_TEST_CYBER3
   AGENT CYBB_BFT030
   SAPJOBNAME Z_TEST_CYBER3
   SPOOLRECIPIENT 'GRIMMETTDAVI@BFUSA.COM'
   RECIPIENTTYPE INT
   SAPJOBCLASS C
   STARTMODE ASAP
   BANNER D
   PRINTCOPIES 001
   NORUN DAILY
   ABAPNAME RSUSR000
   STEPUSER SAPADM
   LANGUAGE E
   PRINTSPOOLNAME RSUSR000_SAP
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 144
   PRINTFORMAT X_PAPER
   RECIPIENT SAPADM
   PRINTDEPARTMENT BASIS
   ARCMODE PRINT
   RUN TODAY
   DELAYSUB 9.35
   IF TODAY('FRI') THEN DELAYSUB 6.30 TODAY PLUS 1 WORKDAY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
ENDJOB

