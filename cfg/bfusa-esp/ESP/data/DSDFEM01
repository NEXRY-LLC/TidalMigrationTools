APPL DSDFEM01 WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
/**********************************************************************/
/* 11/04/19 - CREATED PER RAGHAV PANCHAPAKESAN -DSG- INC00000XXXXXXX */
/**********************************************************************/
NOTIFY ABEND FAILURE ALERT(REMD)
NOTIFY OVERDUE ALERT(LATE)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   DELAYSUB 4.00
   RELEASE (FEMS_CPN_CDE_ADW_LOAD,FEMS_DISC_CAP_ADW_LOAD,+
FEMS_CPN_CDE_TDW_LOAD,FEMS_DISC_CAP_TDW_LOAD)
ENDJOB

LINUX_JOB FEMS_CPN_CDE_ADW_LOAD
   AGENT LNX_DSRSAP01PAKR
   USER bodsadmin
   SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
   JOB_FEMS_CPN_CDE_ADW_LOAD.sh
   /*NOTIFY ABEND FAILURE ALERT(ADW1) MAILBOX(DGRIMMETT1) */
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

LINUX_JOB FEMS_DISC_CAP_ADW_LOAD
   AGENT LNX_DSRSAP01PAKR
   USER bodsadmin
   SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
   JOB_FEMS_DISC_CAP_ADW_LOAD.sh
   /*NOTIFY ABEND FAILURE ALERT(ADW1) MAILBOX(DGRIMMETT1) */
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

LINUX_JOB FEMS_CPN_CDE_TDW_LOAD
   AGENT LNX_DSRSAP01PAKR
   USER bodsadmin
   SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
   JOB_FEMS_CPN_CDE_TDW_LOAD.sh
   /*NOTIFY ABEND FAILURE ALERT(ADW1) MAILBOX(DGRIMMETT1) */
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

LINUX_JOB FEMS_DISC_CAP_TDW_LOAD
   AGENT LNX_DSRSAP01PAKR
   USER bodsadmin
   SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
   JOB_FEMS_DISC_CAP_TDW_LOAD.sh
   /*NOTIFY ABEND FAILURE ALERT(ADW1) MAILBOX(DGRIMMETT1) */
   RUN DAILY
   RELEASE ADD(TDW_SPD_FF_EXTRACT)
ENDJOB

LINUX_JOB TDW_SPD_FF_EXTRACT
   AGENT LNX_DSRSAP01PAKR
   USER bodsadmin
   SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
   JOB_TDW_SPD_FF_EXTRACT.sh
   /*NOTIFY ABEND FAILURE ALERT(ADW1) MAILBOX(DGRIMMETT1) */
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

LINUX_JOB LIE.!ESPAPPL
   AGENT LNX_DSRSAP01PAKR
   USER bodsadmin
   COMMAND /bin/sleep
   ARGS 3
   RUN DAILY
ENDJOB


