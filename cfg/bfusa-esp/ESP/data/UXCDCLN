APPL UXCDCLN

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*04/09/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN SUN
   EARLYSUB 10.35
   RELEASE ADD(CDCLEAN.ISBFD)
   RELEASE ADD(CDCLEAN.ISHRP1)
ENDJOB

UNIX_JOB CDCLEAN.ISBFP3
   AGENT CYBA_ISBFP3
   SCRIPTNAME /local/bin/cdclean.sh
   RUN SUN
   USER cduser
   RELEASE (LIE.!ESPAPPL)
ENDJOB

UNIX_JOB CDCLEAN.ISHRP1
   AGENT CYBA_ISHRP1
   SCRIPTNAME /local/bin/cdclean.sh
   RUN SUN
   USER cduser
   RELEASE (CDCLEAN.ISBFP3)
ENDJOB

UNIX_JOB CDCLEAN.ISBFD
   AGENT CYBA_ISBFD
   SCRIPTNAME /local/bin/cdclean.sh
   RUN SUN
   USER cduser
   RELEASE (CDCLEAN.SAPDE1)
ENDJOB

UNIX_JOB CDCLEAN.SAPDE1
   AGENT CYBA_SAPDE1
   SCRIPTNAME /local/bin/cdclean.sh
   RUN SUN
   USER cduser
   RELEASE (CDCLEAN.ISHRQ)
ENDJOB

UNIX_JOB CDCLEAN.ISHRQ
   AGENT CYBA_ISHRQ
   SCRIPTNAME /local/bin/cdclean.sh
   RUN SUN
   USER cduser
   RELEASE (CDCLEAN.ISBFM2)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN SUN
ENDJOB

