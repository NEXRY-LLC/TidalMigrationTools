APPL QACLSHIP WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/* 11/21/2016 -JFC- REMOVED CDC1 CONNECT DIRECT JOBS PER J. HRITZ     */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY OVERDUE MAILBOX(SQLSHAREDSERVICES)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

JOB LIS.QACLSHIP LINK PROCESS
  RUN 2ND WORKDAY OF MONTH
  DELAYSUB 13.00
  RELEASE ADD(DTS_CONTROL_SHIPMENTS_CL)
ENDJOB

NT_JOB DTS_CONTROL_SHIPMENTS_CL
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\dts_control_shipments_cl.cmd
  ARGS 6
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(SHIPMENTS_CLCMD)
ENDJOB

NT_JOB SHIPMENTS_CLCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\shipments_cl.cmd
  ARGS
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(SHIPMENTS_CL_CHECKS)
ENDJOB

NT_JOB SHIPMENTS_CL_CHECKS
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\shipments_cl_checks.cmd
  ARGS
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(LIE.QACLSHIP)
ENDJOB

JOB LIE.QACLSHIP LINK PROCESS
  RUN 2ND WORKDAY OF MONTH
    DUEOUT EXEC 16:00
ENDJOB

