AS400_JOB BA_BOSSC.BFBRPRO
  AGENT BFBRPRO
  CLPNAME TEBOSSCOM
  LIBL (MSBAEXT MSBAFILE MSBAOBJ QGPL YOB610 QTEMP)
  JOBD MSBAEXT/TESSJOBD
  RUN DAILY
  RELEASE ADD(BOSSSAP_BA_LOADBAT)
ENDJOB

NT_JOB BOSSSAP_BA_LOADBAT
  AGENT WEB3AKATC
  USER bfusa\maestro
  CMDNAME D:\BOSS_SAP\batch_files\BossSapLoad_19.bat
  ARGS
  RUN DAILY
    NOTWITH BOSSSAP_AO_LOADBAT
    NOTWITH BOSSSAP_BL_LOADBAT
    NOTWITH BOSSSAP_88_LOADBAT
    NOTWITH BOSSSAP_CD_LOADBAT
    NOTWITH BOSSSAP_LV_LOADBAT
    NOTWITH BOSSSAP_8H_LOADBAT
    NOTWITH BOSSSAP_WN_LOADBAT
  RELEASE ADD(BOSSCURETAM_BA_LOADBAT)
ENDJOB

NT_JOB BOSSCURETAM_BA_LOADBAT
  AGENT WEB3AKATC
  CMDNAME D:\TESS_TO_BOSS\batch_files\ExecCureTamLoad_19.bat
  USER bfusa\maestro
  ARGS
  RUN DAILY
    NOTWITH BOSSSAP_AO_LOADBAT
    NOTWITH BOSSSAP_BL_LOADBAT
    NOTWITH BOSSSAP_88_LOADBAT
    NOTWITH BOSSSAP_CD_LOADBAT
    NOTWITH BOSSSAP_LV_LOADBAT
    NOTWITH BOSSSAP_8H_LOADBAT
    NOTWITH BOSSSAP_WN_LOADBAT
  RELEASE ADD(BOSSCUREBUILD_BA_LOADBAT)
ENDJOB

NT_JOB BOSSCUREBUILD_BA_LOADBAT
  AGENT WEB3AKATC
  CMDNAME D:\TESS_TO_BOSS\batch_files\ExecCureLoad_19.bat
  ARGS
  RUN DAILY
    NOTWITH BOSSCUREBUILD_AO_LOADBAT
    NOTWITH BOSSCUREBUILD_BL_LOADBAT
    NOTWITH BOSSCUREBUILD_88_LOADBAT
    NOTWITH BOSSCUREBUILD_CD_LOADBAT
    NOTWITH BOSSCUREBUILD_LV_LOADBAT
    NOTWITH BOSSCUREBUILD_8H_LOADBAT
    NOTWITH BOSSCUREBUILD_WN_LOADBAT
  RELEASE ADD(BOSSCOMP_BA_LOADBAT)
ENDJOB

NT_JOB BOSSCOMP_BA_LOADBAT
  AGENT WEB3AKATC
  CMDNAME D:\TESS_TO_BOSS\batch_files\ExecCompLoad_19.bat
  ARGS
  RUN DAILY
   NOTIFY FAILURE ABEND ALERT(REMD)
   NOTIFY OVERDUE MAILBOX(AIBOSS1)
   NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)
   IF ESPSHH = '13' THEN DUEOUT EXEC 4PM
   IF ESPSHH = '19' THEN DUEOUT EXEC 4AM
    NOTWITH BOSSCOMP_AO_LOADBAT
    NOTWITH BOSSCOMP_BL_LOADBAT
    NOTWITH BOSSCOMP_88_LOADBAT
    NOTWITH BOSSCOMP_CD_LOADBAT
    NOTWITH BOSSCOMP_LV_LOADBAT
    NOTWITH BOSSCOMP_8H_LOADBAT
    NOTWITH BOSSCOMP_WN_LOADBAT
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

AIX_JOB LIE.!ESPAPPL
  AGENT CYBA_ISBFP3
  COMMAND /bin/sleep
  ARGS 3
  USER bfpadm
  RUN DAILY
ENDJOB
