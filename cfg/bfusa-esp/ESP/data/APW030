APPL APW030 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*03/11/2019 -JFC-  UPDATED PROC PER  INC000001858289                */
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

FILE_TRIGGER APW030_CONCUR
   AGENT CYBA_ISBFP3
   FILENAME /data/BFP/interfaces/input/datain/ZFIAPI14CONCUREXP.dat
   RUN MONDAY
   RELEASE ADD(ZFIAPI20_CONVERT_UID_TO_VENDOR)
ENDJOB

SAP_JOB ZFIAPI20_CONVERT_UID_TO_VENDOR
   NOTIFY FAILURE ABEND MAILBOX(SAPFIGROUP)
   NOTIFY FAILURE ABEND ALERT(REMD)                                     00010000
   AGENT CYBB_BFP030
   SAPJOBNAME ZFIAPI20_CONVERT_UID_TO_VENDOR
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIAPI20
   VARIANT EXP-UID-CONCUR
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   COLUMNS 170
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN MONDAY
   RELEASE ADD(ZFIAPI14_CONCUR_EXPENSE_INVOICES)
ENDJOB

SAP_JOB ZFIAPI14_CONCUR_EXPENSE_INVOICES
   AGENT CYBB_BFP030
   SAPJOBNAME ZFIAPI14_CONCUR_EXPENSE_INVOICES
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIAPI14
   VARIANT EXPENSE-CONCUR
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   COLUMNS 170
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN MONDAY
   RELEASE ADD(ARZFIAPI14_CONCUR)
ENDJOB

UNIX_JOB ARZFIAPI14_CONCUR
   AGENT CYBA_ISBFP3
   scriptname /local/bin/arcfile.sh
   args interfaces/input/datain/ZFIAPI14CONCUREXP.dat W
   RUN MONDAY
   RELEASE ADD(ARZFIAPI20_CONCUR)
   USER bfpadm
ENDJOB

UNIX_JOB ARZFIAPI20_CONCUR
   AGENT CYBA_ISBFP3
   USER bfpadm
   scriptname /local/bin/arcfile.sh
   args interfaces/input/datain/zfiapi20concur.txt W
   RUN MONDAY
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
  RUN DAILY
ENDJOB

