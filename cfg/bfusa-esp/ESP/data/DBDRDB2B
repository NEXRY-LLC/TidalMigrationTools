APPL DBDRDB2B HOLD

NOTIFY FAILURE ABEND ALERT(REMD)


SCHD255 = 'YESTERDAY'

IF !USER2 = 255 THEN SCHD255 = 'TODAY'

JCLLIB 'FTRF.PRDJOBS'
COPYJCL 'SWS.ESP.COPYLIB'
TEMPLIB 'SWS.ESP.TEMPLIB'
DOCLIB 'SWS.ESP.DOCLIB'

IF !ESPAPSUB# > 1 THEN JCLLIB 'SWS.ESP.COPYLIB'

OPTIONS NORESTARTSTEP

JOB USERREQ.DBDRDB2B TASK DOCMEM(DBDRDB2B)
  RUN REF DBDRDB2B
  RELEASE ADD(DBDRDB2B)
ENDJOB

JOB DBDRDB2B
  MEMBER USIEBR14
  /*@@ SYSTEM=DB2DR
  /*@@ SCHID=255
  RUN !SCHD255
  RELEASE ADD(DA2300RO)
  RELEASE ADD(DB1000RO)
  RELEASE ADD(DB2015RO)
  RELEASE ADD(DB2174RO)
  RELEASE ADD(DB2175RO)
  RELEASE ADD(DB2176RO)
  RELEASE ADD(DB2179RO)
  RELEASE ADD(DB2300RO)
  RELEASE ADD(DB2301RO)
  RELEASE ADD(DB2302RO)
  RELEASE ADD(DB4100RO)
ENDJOB

JOB DA2300RO
  /*@@ SYSTEM=DB2DR
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN !SCHD255
  RELEASE ADD(DBDRDB2E)
ENDJOB

