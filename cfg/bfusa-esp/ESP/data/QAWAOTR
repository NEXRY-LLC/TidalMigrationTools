APPL QAWAOTR WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO LINK                   */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND OVERDUE MAILBOX(QAFAIL)
TAG PPIW

JOB WA2190S EXTERNAL SCOPE(-24:00)
   RUN SUN
   RELEASE ADD(DTS_CONTROL_WA_OTR)
ENDJOB

JOB WA1578D EXTERNAL SCOPE(-24:00)
   RUN SUN
   RELEASE ADD(DTS_CONTROL_WA_OTR)
ENDJOB

JOB WA1577D EXTERNAL SCOPE(-24:00)
   RUN SUN
   RELEASE ADD(WA_OTR_TEMP)
ENDJOB

NT_JOB CUSTOMER_MASTER_CHECKS EXTERNAL APPLID(QACUSCHK)
   RUN SUN
   RELEASE ADD(WA_OTR_TEMP)
ENDJOB

NT_JOB SERCONDXREF EXTERNAL APPLID(QASVCCND)
   RUN SUN
   RELEASE ADD(WA_OTR_TEMP)
ENDJOB

NT_JOB BASE_MODELS_LOADCMD EXTERNAL APPLID(QABASMDL)
   RUN SUN
   RELEASE ADD(WA_OTR_TEMP)
ENDJOB

NT_JOB VMSDOTCMD EXTERNAL APPLID(QAVMSDOT)
   RUN SUN
   RELEASE ADD(WA_OTR_TEMP)
ENDJOB

JOB LIS.QAWAOTR LINK PROCESS
   RUN SUN
   EARLYSUB 01.00
   RELEASE ADD(DTS_CONTROL_WA_OTR)
ENDJOB

NT_JOB DTS_CONTROL_WA_OTR
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\dts_control_wa_otr.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(WA_OTR_TEMP)
ENDJOB

NT_JOB WA_OTR_TEMP
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_otr_temp.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(WA_OTR)
ENDJOB

NT_JOB WA_OTR
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_otr.cmd
   USER maestro
   RUN SUN
   DUEOUT EXEC 4AM
   RELEASE ADD(LIE.QAWAOTR)
ENDJOB

JOB LIE.QAWAOTR LINK PROCESS
   RUN SUN
ENDJOB

