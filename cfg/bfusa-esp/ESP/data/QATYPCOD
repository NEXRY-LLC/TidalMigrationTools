APPL QATYPCOD WAIT

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)

TAG PPIW
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB WA2241S EXTERNAL SCOPE(-48.00)
  RUN THU SUN
  RELEASE ADD(LIS.QATYPCOD)
ENDJOB

JOB LIS.QATYPCOD LINK PROCESS
  RUN THU SUN
  DELAYSUB 01.00
  RELEASE ADD(TYPE_CODE_DESC_XREF)
ENDJOB

NT_JOB TYPE_CODE_DESC_XREF
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\type_code_desc_xref.cmd
  ARGS
  USER maestro
  RUN THU SUN
  RELEASE ADD(LIE.QATYPCOD)
ENDJOB

JOB LIE.QATYPCOD LINK PROCESS
  RUN THU SUN
ENDJOB

