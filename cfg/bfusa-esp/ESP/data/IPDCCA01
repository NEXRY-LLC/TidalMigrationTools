APPL IPDCCA01 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB FSIP_COST_CENTER_ACCOUNTING_DAILY LINK PROCESS
   RUN DAILY
   EARLYSUB 02.20
   RELEASE ADD(FSIP_CCA0223)
   RELEASE ADD(FSIP_CCA0500)
   RELEASE ADD(FSIP_CCA1100)
   RELEASE ADD(FSIP_CCA1400)
   RELEASE ADD(FSIP_CCA1700)
   RELEASE ADD(FSIP_CCA2350)
ENDJOB

SAP_JOB FSIP_CCA0223
   AGENT SAP_IZP100
   SAPJOBNAME CCA1
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RGUREC10
   VARIANT CCA
   STEPUSER FSIP_BATCH
   LANGUAGE E
   PRINTDEST DEV1
   PRINTSPOOLNAME RGUREC10_IBM
   PRINTNEWSPOOL YES
   EXPIRATION 9
   LINES 65
   COLUMNS 180
   PRINTFORMAT X_65_200
   BANNER D
   RECIPIENT IBMPRAGHU
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   EARLYSUB 02.23
ENDJOB

SAP_JOB FSIP_CCA0500
