APPL QADTSLOG WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

JOB LIS.QADTSLOG LINK PROCESS
   RUN DAILY
   EARLYSUB 01.00
   RELEASE ADD(SYSDTSLOG90_DSW1CMD)
ENDJOB

NT_JOB SYSDTSLOG90_DSW1CMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\sysdtslog90_dsw1.cmd
   ARGS
   USER maestro
   RUN DAILY
   RELEASE ADD(DELETE_SSIS_LOG_FILESCMD)
ENDJOB

NT_JOB DELETE_SSIS_LOG_FILESCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\delete_ssis_log_files.cmd
   ARGS
   USER maestro
   RUN DAILY
   RELEASE ADD(LIE.QADTSLOG)
ENDJOB

JOB LIE.QADTSLOG LINK PROCESS
   RUN DAILY
ENDJOB

