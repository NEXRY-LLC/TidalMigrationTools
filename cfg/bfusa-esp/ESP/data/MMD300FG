NT_JOB DEL_CHARS
    AGENT CYBA_APPMNACHQ
    USER maestro
    CMDNAME e:\APPS\DataSync\DEL_CHARS.bat
    ARGS
    RUN WEEKDAYS
    RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

AIX_JOB LIE.!ESPAPPL
    AGENT CYBA_ISBFP3
    USER bfpadm
    COMMAND /bin/sleep
    ARGS 3
    RUN WEEKDAYS
ENDJOB

