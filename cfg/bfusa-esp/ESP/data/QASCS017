APPL QASCS017 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO LINK                   */
/*09/26/2019-JFC-set job SCS_AUSP_FG to request only per              */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW

AIX_JOB LIE.MMD300FG EXTERNAL APPLID(MMD300FG)
   RUN WEEKDAYS
   RELEASE ADD(LIS.QASCS017)
ENDJOB

JOB LIS.QASCS017 LINK PROCESS
   RUN DAILY
   DELAYSUB 07.00
   RELEASE ADD(SCS_SAP_CHARS_FG_DELETEcmd)
ENDJOB

NT_JOB SCS_SAP_CHARS_FG_DELETEcmd
   AGENT CYBA_PPIWPROD
   CMDNAME X:\CMD_SCRIPTS\SCS_SAP_CHARS_FG_DELETE.cmd
   RUN DAILY
   USER maestro
   RELEASE ADD(SCS_MARA_FGcmd)
ENDJOB

