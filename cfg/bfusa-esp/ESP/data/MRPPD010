   ARGS interfaces/input/datain/mmc_2000.txt D
   USER bfpadm
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

AIX_JOB LIE.!ESPAPPL
   AGENT CYBA_ISBFP3
   COMMAND /bin/sleep
   ARGS 3
   USER bfpadm
   RUN DAILY
    DUEOUT EXEC 21.50
    IF TODAY('SUNDAY') THEN DUEOUT EXEC 23.50
ENDJOB

