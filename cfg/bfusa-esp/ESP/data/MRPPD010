SAP_JOB ZMMBI002_CFS_2000
   SAPUSER SAPADM
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMBI002_CFS_2000
   SAPJOBCLASS C
   ABAPNAME ZMMBI002
   VARIANT ZMMBI002_2000C
   LANGUAGE E
   PRINTDEST CP44
   ARCMODE PRINT
   BANNER D
   RECIPIENT SAPADM
   COLUMNS 132
   EXPIRATION 8
   PRINTCOPIES 001
   STARTMODE ASAP
   RUN DAILY
   RELEASE ADD(ZMMBI025_CFS_2000)
ENDJOB

SAP_JOB ZMMBI025_CFS_2000
   SAPUSER SAPADM
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMBI025_CFS_2000
   SAPJOBCLASS C
   PRINTCOPIES 001
   RUN DAILY
   STARTMODE ASAP
   ABAPNAME ZMMBI025
   VARIANT ZMMBI025_2000C
   LANGUAGE E
   PRINTDEST CP44
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   RUN DAILY
   RELEASE ADD(ZMMBI018_CFS_2000)
ENDJOB

SAP_JOB ZMMBI018_CFS_2000
   SAPUSER SAPADM
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMBI018_CFS_2000
   RECIPIENTTYPE MAIL
   SPOOLRECIPIENT 'restrepohernan@BFUSA.COM'
   SAPJOBCLASS C
   ABAPNAME ZMMBI018
   VARIANT ZMMBI018_2000C
   LANGUAGE E
   PRINTDEST CP44
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   RELEASE ADD(AR_BOM1C_2000TXT)
ENDJOB

UNIX_JOB AR_BOM1C_2000TXT
   AGENT CYBA_ISBFP3
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom1c_2000.txt D
   USER bfpadm
   RUN DAILY
   RELEASE ADD(AR_BOM2C_2000TXT)
ENDJOB

