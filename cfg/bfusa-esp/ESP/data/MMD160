NOTIFY FAILURE ABEND ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
  RUN WEEKDAYS
  RELEASE ADD(ZMMPOI10_LAND_SHIPMT_ASN_EXTRACT)
ENDJOB

SAP_JOB ZMMPOI10_LAND_SHIPMT_ASN_EXTRACT
  AGENT CYBB_BFP030
  SAPJOBNAME ZMMPOI10_LAND_SHIPMT_ASN_EXTRACT
  SAPJOBCLASS C
  SAPUSER SAPADM
  ABAPNAME ZMMPOI10
  VARIANT COLLEY
  LANGUAGE E
  PRINTDEST CP21
  PRINTCOPIES 001
  EXPIRATION 8
  COLUMNS 132
  BANNER D
  RECIPIENT SAPADM
  ARCMODE PRINT
  RUN WEEKDAYS
  RELEASE ADD(ZMMPOI10_ASN_EORLEANS)
ENDJOB

UNIX_JOB ZMMPOI10_ASN_EORLEANS
  AGENT CYBA_ISBFP3
  USER sftpprod
  SCRIPTNAME /apps/ssh/bin/scp2.sh
ARGS put /data/BFP/interfaces/output/dataout/zmmpoi10_asn_eorle+
          ans.txt sftp1.bsaecomm.com:/GFM/A2S/SAP/Upload/zmmpoi1+
          0_asn_eorleans.txt
  RUN WEEKDAYS
  RELEASE ADD(ZMMPOI017_ZASN_BATCH_REFRESH)
ENDJOB

SAP_JOB ZMMPOI017_ZASN_BATCH_REFRESH
  AGENT CYBB_BFP030
  SAPJOBNAME ZMMPOI017_ZASN_BATCH_REFRESH
  SAPJOBCLASS C
  SAPUSER SAPADM
  ABAPNAME ZMMPOI17
  LANGUAGE E
  PRINTDEST CP21
  ARCMODE PRINT
  BANNER D
  RECIPIENT SAPADM
  COLUMNS 132
  EXPIRATION 8
  PRINTCOPIES 001
  STARTMODE ASAP
  RUN WEEKDAYS
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
  RUN WEEKDAYS
ENDJOB

