APPL IPDFI010 WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
/**********************************************************************/
/* CREATED ON 01/11/18 PER ASHWINI GARJE - DSG - INC00000XXXXXXX*/
/**********************************************************************/
NOTIFY ABEND FAILURE ALERT(REMD)

SAP_JOB FSIP_LOCKBOX_LOAD_AFTER_8AM_1
   AGENT SAP_IZP100
   SAPJOBNAME LOCKBOX_LOAD_AFTER_8AM
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIIR_LOCKBOX_JOB_SCH
   VARIANT AFTER_8AM
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZFIIR_LOCKBO
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 200
   PRINTFORMAT X_65_200
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   NORUN TODAY
   EARLYSUB 08.10
   IF ESPSHH EQ '04' THEN RUN TODAY
ENDJOB

SAP_JOB FSIP_LOCKBOX_LOAD_AFTER_8AM_2
   AGENT SAP_IZP100
   SAPJOBNAME LOCKBOX_LOAD_AFTER_8AM
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIIR_LOCKBOX_JOB_SCH
   VARIANT AFTER_8AM
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZFIIR_LOCKBO
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 200
   PRINTFORMAT X_65_200
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   DELAYSUB 13.13.42
   NORUN DAILY
   IF ESPSHH EQ '11' THEN RUN TODAY
ENDJOB

