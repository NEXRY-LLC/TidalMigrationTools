APPL LED024DW WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB ZSDLEI33_UPDATE_NEXT_DELV_DATE EXTERNAL APPLID(LED053) +
SCOPE(-08)
   RUN SUN MON TUE WED THU
   RELEASE (ZSDLEI36N_I99_SAP_TBL_DCWST)
ENDJOB

SAP_JOB ZSDLEI36N_I99_SAP_TBL_DCWST
   AGENT CYBB_BFP030
   SAPJOBNAME ZSDLEI36N_I99_SAP_TBL_DCWST
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSDLEI36N
   VARIANT ZLE_99N_DCWST
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN SUN MON TUE WED THU
   DELAYSUB 21.00
   IF TODAY('SUN') THEN EARLYSUB 19.00
   RELEASE (ZSDLEI35N_I99_SAP_TMS_PROXY)
ENDJOB

SAP_JOB ZSDLEI35N_I99_SAP_TMS_PROXY
   AGENT CYBB_BFP030
   SAPJOBNAME ZSDLEI35N_I99_SAP_TMS_PROXY
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSDLEI35N
   VARIANT PROXY_99
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN SUN MON TUE WED THU
ENDJOB


