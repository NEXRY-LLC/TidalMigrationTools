APPL IPWCONCR WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*11/19/2018 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN SUNDAY
   EARLYSUB 03.55
   RELEASE ADD(FSIP_CONCUR_COST_CENTER_EXTRACT)
   RELEASE ADD(FSIP_ZFILMI10_CONCUR_INTERNAL_ORDER)
ENDJOB

SAP_JOB FSIP_CONCUR_COST_CENTER_EXTRACT
   AGENT SAP_IZP100
   SAPJOBNAME 'CONCUR COST CENTER EXTRACT'
   SAPJOBCLASS C
   PRINTCOPIES 001
   STARTMODE ASAP
   ABAPNAME ZFILMI09
   VARIANT 'CONCUR CC EXT'
   STEPUSER FSIP_BATCH
   LANGUAGE E
   PRINTSPOOLNAME ZFILMI09_BFD
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 255
   PRINTFORMAT X_PAPER
   BANNER D
   RECIPIENT BFDPBATCH
   ARCMODE PRINT
   RUN SUNDAY
   EARLYSUB 04.00
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

SAP_JOB FSIP_ZFILMI10_CONCUR_INTERNAL_ORDER
   AGENT SAP_IZP100
   SAPJOBNAME ZFILMI10_CONCUR_INTERNAL_ORDER
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFILMI10
   VARIANT IO_SAP2CONCUR
   STEPUSER FSIP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTSPOOLNAME ZFILMI10_BFD
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 255
   PRINTFORMAT X_65_255
   BANNER D
   RECIPIENT BFDPBATCH
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN SUNDAY
   EARLYSUB 04.05
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
   DUEOUT EXEC 5 AM
ENDJOB


