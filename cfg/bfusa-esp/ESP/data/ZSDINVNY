APPL ZSDINVNY WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
/**********************************************************************/
/* 11/15/2022 - SCTASK0180715 - NEW SCHEDULE CREATED
/**********************************************************************/
NOTIFY ABEND FAILURE ALERT(REMD)
JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   DELAYSUB 04.00
   RELEASE ADD(ZSD_INVENTORY_STATUS_1)
ENDJOB

SAP_JOB ZSD_INVENTORY_STATUS_1
   AGENT SAP_IZP100
   SAPJOBNAME ZSD_INVENTORY_STATUS_1
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZZSD_INVENTORY_STATUS
   VARIANT INV_REPORT_1
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   RECIPIENTTYPE DLI
   SPOOLRECIPIENT 'INV_REPORT'
   PRINTDEST P060
   PRINTCOPIES 001
   PRINTSPOOLNAME ZZSD_INVENTO
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RELEASE (ZSD_INVENTORY_STATUS_2)
   RUN DAILY
ENDJOB

