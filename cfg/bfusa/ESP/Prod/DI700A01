  CCCHK RC(1:4095) FAIL CONTINUE
  INVOKE 'SWS.ESP.APPL.PROCLIB(#DIRNDAT)'
  /*RUN 4TH THU OF NOV LESS 1 DAY
  IF TODAY('4TH THU OF NOV') THEN NORUN TODAY
  RELEASE ADD(LIM2.!ESPAPPL)
ENDJOB

JOB DI230A
  /*@@ SYSTEM=DRP
  CCCHK RC(2:4095) FAIL CONTINUE
  /*RUN MON = OLD RUN STATEMENT FROM APPL DI810D
  RUN SUN
  DUEOUT INPUT 01.45
  RELEASE ADD(DI003A2)
  RELEASE ADD(DI008A2)
  RELEASE ADD(DI015A2)
  RELEASE ADD(DI017A2)
ENDJOB

