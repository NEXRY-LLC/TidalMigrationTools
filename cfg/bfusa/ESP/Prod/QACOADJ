   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_sap_temp_co.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_INITIAL_COCMD)
ENDJOB

NT_JOB WA_DETAIL_SAP_INITIAL_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_initial_co.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_TEMP_COCMD)
ENDJOB

NT_JOB WA_DETAIL_SAP_TEMP_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_temp_co.cmd
   ARGS 5
   USER maestro
