APPL QACOADJ WAIT

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
TAG PPIW

JOB LIS.QACOADJ LINK PROCESS
   RUN SAT
   IF TODAY('JULY 15 2017') THEN JOBATTR HOLD
   EARLYSUB 00.03
   RELEASE ADD(WA_HEADER_DETAIL_DELETE_COCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_DELETE_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_detail_delete_co.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_TEMP_COCMD)
ENDJOB

NT_JOB WA_HEADER_TEMP_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_temp_co.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_TEMP_COCMD)
ENDJOB

NT_JOB WA_DETAIL_TEMP_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_temp_co.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_SAP_INITIAL_COCMD)
ENDJOB

NT_JOB WA_HEADER_SAP_INITIAL_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_sap_initial_co.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_SAP_TEMP_COCMD)
ENDJOB

