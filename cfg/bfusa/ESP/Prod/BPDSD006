APPL BPDSD006 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*08/18/2019 -VMD- ADJUSTED APPL TO RUN AT 0:30 PER INC000002012235
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   DELAYSUB 00.30
   RELEASE ADD(FSBP_SD_BILLINGRUN_INT_DAILY030AM_1)
ENDJOB

SAP_JOB FSBP_SD_BILLINGRUN_INT_DAILY030AM_1
   AGENT SAP_BZP100
   SAPJOBNAME SD_BILLINGRUN_INT_DAILY030AM_1
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RV60SBAT
   VARIANT INV_EU_SO_F5
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME RV60SBAT_FSB
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN DAILY
   RELEASE (FSBP_SD_BILLINGRUN_INT_DAILY030AM_2)
ENDJOB

