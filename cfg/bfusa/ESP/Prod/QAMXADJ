APPL QAMXADJ WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

JOB LIS.QAMXADJ LINK PROCESS
   RUN SAT
   IF TODAY('JULY 15 2017') THEN JOBATTR HOLD
   EARLYSUB 00.03
   RELEASE ADD(WA_HEADER_DETAIL_DELETE_MXCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_DELETE_MXCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_detail_delete_mx.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_TEMP_MXCMD)
ENDJOB

NT_JOB WA_HEADER_TEMP_MXCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_temp_mx.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_TEMP_MXCMD)
ENDJOB

NT_JOB WA_DETAIL_TEMP_MXCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_temp_mx.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_SAP_INITIAL_MXCMD)
ENDJOB

