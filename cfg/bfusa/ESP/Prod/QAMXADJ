   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_initial_mx.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_TEMP_MXCMD)
ENDJOB

NT_JOB WA_DETAIL_SAP_TEMP_MXCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_temp_mx.cmd
   ARGS 5
   USER maestro
