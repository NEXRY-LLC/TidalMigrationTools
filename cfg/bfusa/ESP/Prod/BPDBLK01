NOTIFY ABEND FAILURE ALERT(REMD)
NOTIFY OVERDUE ALERT(LATE)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   EARLYSUB 01.00
   RELEASE (BLACKLIN_SGL_EXT_BP_GL_BL)
ENDJOB

SAP_JOB BLACKLIN_SGL_EXT_BP_GL_BL
   AGENT SAP_BZP100
   SAPJOBNAME BLACKLIN_SGL_EXT_BP_GL_BL
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME /BLACKLIN/SGL_EXTRACT
   VARIANT 'BP GL BL'
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME /BLACKLIN/SG
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN DAILY
   DELAYSUB 02.00
   RELEASE (BLACKLIN_SAR_EXT_BP_AR_SL)
ENDJOB

