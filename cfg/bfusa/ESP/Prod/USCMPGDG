APPL USCMPGDG WAIT

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB USCMPGDG
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  DELAYSUB 10:00
  RELEASE ADD(USCMPSG1)
ENDJOB

JOB USCMPSG1
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(USWKSTGX)
ENDJOB

JOB USWKSTGX
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(USWKSTG0)
  RELEASE ADD(USWKSTG1)
  RELEASE ADD(USWKSTG2)
  RELEASE ADD(USWKSTG3)
ENDJOB

JOB USWKSTG0
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(SWGDGSC)
ENDJOB

JOB USWKSTG1
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(SWGDGSC)
ENDJOB

JOB USWKSTG2
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(SWGDGSC)
ENDJOB

JOB USWKSTG3
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(SWGDGSC)
ENDJOB

JOB SWGDGSC
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(USWVTC)
ENDJOB

JOB USWVTC
  OPTIONS NORESTARTSTEP
  RUN SAT
  RELEASE ADD(SWCATLG)
ENDJOB

JOB SWCATLG
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(SWCATLG2)
  RELEASE ADD(USVSMRPT)
ENDJOB

JOB SWCATLG2
  CCCHK RC(1:3) FAIL CONTINUE
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN SAT
  NOTIFY JOBEND ALERT(US01)
  NOTIFY FAILURE ABEND ALERT(REMD)
ENDJOB

JOB USVSMRPT
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(SWOFFRP3)
ENDJOB

JOB SWOFFRP3
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN SAT
  RELEASE ADD(SWOFFRP4)
ENDJOB

JOB SWOFFRP4
  CCCHK RC(1:4095) FAIL CONTINUE
  RUN FIRST SAT OF MONTH
ENDJOB

