APPL BLD008 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*11/07/2018 -JFC- CHANGED DUMMY ZOS TO LINK JOBS                    */
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
  RUN WEEKDAYS SAT
  DELAYSUB 15.45
  RELEASE ADD(RBDAPP01_CFNA_ORDERS_1550)
  RELEASE ADD(RBDAPP01_AFFILIATE_ORDERS_1650)
  RELEASE ADD(RBDAPP01_FSNA_ORDERS_1750)
  RELEASE ADD(RBDAPP01_MILEAGE_ORDERS_1950)
ENDJOB

SAP_JOB RBDAPP01_CFNA_ORDERS_1550
   AGENT CYBB_BFP030
   SAPJOBNAME RBDAPP01_CFNA_ORDERS
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RBDAPP01
   VARIANT CFNA_ORDERS
   STEPUSER SAPADM
   LANGUAGE E
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 152
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN WEEKDAYS SAT
   DELAYSUB 15.50
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

