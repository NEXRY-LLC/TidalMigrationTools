APPL BPISD002 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB FSBP_SD_SAP2TMS_OUTBOUND_DLVRY
   AGENT SAP_BZP100
   SAPJOBNAME SD_SAP2TMS_OUTBOUND_DLVRY
   SAPJOBCLASS C
   ABAPNAME RSNAST00
   VARIANT TMS
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT FSBP_BATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME RSNAST00_FSB
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
   STARTMODE ASAP
   RUN DAILY
ENDJOB


