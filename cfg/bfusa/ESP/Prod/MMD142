APPL MMD142 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*05/09/2018 -JFC- UPDATED PROC AND CHANGED EVENT PREFIX FROM OP01
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

DATA_OBJECT PARMSET.MMD142
   /*Create a timestamp in hhmmMMDDYY format*/
   SETVAR TMSTMP='!ESPSHH.:!ESPSMN.,!ESPSMM./!ESPSDD./!ESPSYY'
   SETVAR PID=''
   SETVAR STMPTM='!ESPSMM./!ESPSDD./!ESPSYY.,!ESPSHH.:!ESPSMN'
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
   RUN SUN MON TUE WED THU
   DELAYSUB 21.05
   IF TODAY('SUN') THEN DELAYSUB 19.05
   RELEASE ADD(ZMMPOI18_FS_POLYMERS_PO_EXT)
ENDJOB

SAP_JOB ZMMPOI18_FS_POLYMERS_PO_EXT
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMPOI18_FS_POLYMERS_PO_EXT
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMPOI18
   VARIANT ZMMPOI18_01PO
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   RUN REF LIS.MMD142
   RELEASE ADD(ZMMPOI18_FP_POCD)
ENDJOB

JOB SETPID.MMD142 LINK PROCESS
   /*get the next PID number from the ConnectDirect*/
   /*Process ID table (CDPROD).  Save the value into*/
   /*the applications DataObject for use by the C:D*/
   /*job and the CDSTAT job (if needed).*/
   VINCR (PID) TABLE(CDPROD) CLANG
   VGET  (PID) TABLE(CDPROD) CLANG
   ESP MGRMSG * . . . PARMSET.!ESPAPPL/!ESPAPPL..!ESPAPGEN/MAIN +
ACTION SET PID(!PID)
   RUN REF LIS.!ESPAPPL
   RELEASE ADD(ZMMPOI18_FP_POCD)
ENDJOB

UNIX_JOB ZMMPOI18_FP_POCD
   AGENT CYBA_ISBFP3
   USER cdprod
   EARLYSUB REALNOW PLUS 20 SECONDS
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   TMSTMP= WOBDATA('PARMSET.!ESPAPPL','TMSTMP')
   SCRIPTNAME /apps/condir/bfsbin/submitcd.sh
   ARGS zmmpoi18_01po.cd 10 !PID !TMSTMP
   RUN REF LIS.MMD142
   RELEASE ADD(CDSTAT.ZMMPOI18(A))
   RELEASE ADD(ZMMPOI04_ZEXTRACTDATE_UPD_01PO)
ENDJOB

AIX_JOB CDSTAT.ZMMPOI18 CONDITIONAL
   /*This job only runs if the C:D job does not end with*/
   /*a EXITCODE of 0.  It can be rerun as often as*/
   /*required to check the status of the C:D process.*/
   /*When the C:D process finishes, complete the job*/
   /*to release the successors.*/
   AGENT CYBA_ISBFP3
   USER cduser
   STMPTM= WOBDATA('PARMSET.!ESPAPPL','STMPTM')
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   CMDNAME /apps/condir/bfsbin/cdstat.sh
   ARGS !STMPTM  j!PID
   RUN REF LIS.!ESPAPPL
ENDJOB

SAP_JOB ZMMPOI04_ZEXTRACTDATE_UPD_01PO
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMPOI04_ZEXTRACTDATE_UPD_01PO
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMPOI04
   VARIANT ZMMPOI18_01PO
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   PRINTCOPIES 001
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   RUN REF LIS.MMD142
   RELEASE ADD(ZMMPOI18_FS_POLYMERS_POC_EXT)
ENDJOB

SAP_JOB ZMMPOI18_FS_POLYMERS_POC_EXT
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMPOI18_FS_POLYMERS_POC_EXT
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMPOI18
   VARIANT ZMMPOI18_01POC
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   RUN REF LIS.!ESPAPPL
   RELEASE ADD(ZMMPOI18_FP_POCCD )
ENDJOB

JOB SETPIDA.!ESPAPPL LINK PROCESS
   /*get the next PID number from the ConnectDirect*/
   /*Process ID table (CDPROD).  Save the value into*/
   /*the applications DataObject for use by the C:D*/
   /*job and the CDSTAT job (if needed).*/
   VINCR (PID) TABLE(CDPROD) CLANG
   VGET  (PID) TABLE(CDPROD) CLANG
   ESP MGRMSG * . . . PARMSET.!ESPAPPL/!ESPAPPL..!ESPAPGEN/MAIN +
ACTION SET PID(!PID)
   RUN REF LIS.!ESPAPPL
   RELEASE ADD(ZMMPOI18_FP_POCCD )
ENDJOB

UNIX_JOB ZMMPOI18_FP_POCCD
   AGENT CYBA_ISBFP3
   USER cdprod
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   TMSTMP= WOBDATA('PARMSET.!ESPAPPL','TMSTMP')
   SCRIPTNAME /apps/condir/bfsbin/submitcd.sh
   ARGS zmmpoi18_01poc.cd 5 !PID !TMSTMP
   RESOURCE ADD(1,ISBFP3CD)
   RUN REF LIS.!ESPAPPL
   RELEASE ADD(CDSTAT.ZMMPOI18_FP_POCCD(A))
   RELEASE ADD(ZMMPOI04_ZEXTRACTDATE_UPD_01POC)
ENDJOB

AIX_JOB CDSTAT.ZMMPOI18_FP_POCCD CONDITIONAL
   /*This job only runs if the C:D job does not end with*/
   /*a EXITCODE of 0.  It can be rerun as often as*/
   /*required to check the status of the C:D process.*/
   /*When the C:D process finishes, complete the job*/
   /*to release the successors.*/
   AGENT CYBA_ISBFP3
   USER cduser
   STMPTM= WOBDATA('PARMSET.!ESPAPPL','STMPTM')
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   CMDNAME /apps/condir/bfsbin/cdstat.sh
   ARGS !STMPTM  j!PID
   RUN REF LIS.!ESPAPPL
ENDJOB

SAP_JOB ZMMPOI04_ZEXTRACTDATE_UPD_01POC
