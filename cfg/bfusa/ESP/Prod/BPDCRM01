NOTIFY ABEND FAILURE ALERT(REMD)

JOB BPDCRM01_ZAUTO_JOBS_1AM LINK PROCESS
   RUN DAILY
   EARLYSUB 01.00
   RELEASE ADD(FSBP_CRM_ZAUTO_CLOSURE_BZC_100)
   RELEASE ADD(FSBP_CRM_ZAUTO_ZSRV_COMPL)
   RELEASE ADD(FSBP_CRM_ZAUTO_ZTDR_COMPL_BZC_100)
ENDJOB

SAP_JOB FSBP_CRM_ZAUTO_CLOSURE_BZC_100
   AGENT SAP_BZC100
   SAPJOBNAME ZAUTO_CLOSURE_BZC_100
   SAPJOBCLASS B
   ABAPNAME RSPPFPROCESS
   VARIANT E_148
   STEPUSER FSBP_BATCH
   LANGUAGE E
   ARCMODE PRINT
   BANNER D
   RECIPIENT KOTAKARTHIK
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME RSPPFPROCESS
   PRINTFORMAT X_PAPER
   PRINTCOPIES 001
   STARTMODE ASAP
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

