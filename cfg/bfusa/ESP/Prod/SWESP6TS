APPL SWESP6TS JOB_ANCESTOR_WAIT

TAG 'SWTEST'

NOTIFY FAILURE ABEND ALERT(REMD)


JCLLIB 'FTRF.PRDJOBS'
COPYJCL 'SWS.ESP.COPYLIB'
TEMPLIB 'SWS.ESP.TEMPLIB'
DOCLIB 'SWS.ESP.DOCLIB'

IF !ESPAPSUB# > 1 THEN JCLLIB 'SWS.ESP.COPYLIB'

OPTIONS RESTARTSTEP

VS 'F SWESPWM6,STATUS'
VS 'ABCDEFGHIJK'
VS '$VS,XYZDEFGHIJK'

JOB SWESP6TS
  /* OPTIONS NORESTARTSTEP                  */
  /* CCCHK RC(6:4095) FAIL CONTINUE         */
  RUN ANY
  /*  DUEOUT EXEC NOW PLUS 2 MINUTES        */
  /*  NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)   */
  /*  NOTIFY FAILURE ABEND ALERT(REMD)      */
  DEQUEUE NAME(ESPENQ_JOB(ELD813PF.)_APPL(ELD001.13170))
ENDJOB

