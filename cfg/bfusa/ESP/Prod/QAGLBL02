APPL QAGLBL02 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO LINK                   */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
NOTIFY OVERDUE MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB WA_CONSOLIDATEDCMD EXTERNAL APPLID(QAWACNSL)
   RUN SUN
   RELEASE ADD(LIS.QAGLBL02)
ENDJOB

JOB LIS.QAGLBL02 LINK PROCESS
   RUN SUN
   DELAYSUB 01.00
   RELEASE ADD(GLOBAL_WA_CONSOLIDATED_DELETE)
ENDJOB

NT_JOB GLOBAL_WA_CONSOLIDATED_DELETE
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\global_wa_consolidated_delete.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(GLOBAL_WA_CONSOLIDATED_HDR)
ENDJOB

NT_JOB GLOBAL_WA_CONSOLIDATED_HDR
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\global_wa_consolidated_hdr.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(GLOBAL_WA_CONSOLIDATED_DTL)
ENDJOB

NT_JOB GLOBAL_WA_CONSOLIDATED_DTL
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\global_wa_consolidated_dtl.cmd
   ARGS
   USER maestro
   RUN SUN
   DUEOUT INPUT 9PM
   RELEASE ADD(LIE.QAGLBL02)
ENDJOB

JOB LIE.QAGLBL02 LINK PROCESS
   RUN SUN
ENDJOB

