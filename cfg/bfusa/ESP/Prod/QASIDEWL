APPL QASIDEWL WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND OVERDUE MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

NT_JOB INTERFACE_1089 EXTERNAL APPLID(SSISPPIW) SCOPE(-10)
   RUN DAILY
   NORUN JUL 24 2014
   RELEASE ADD(LIS.QASIDEWL)
ENDJOB

JOB LIS.QASIDEWL LINK PROCESS
  RUN DAILY
  DELAYSUB 01.00
  RELEASE ADD(SIDEWALL_TYPE_XREF)
ENDJOB

NT_JOB SIDEWALL_TYPE_XREF
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\sidewall_type_xref.cmd
  ARGS
  USER maestro
  RUN DAILY
  RELEASE ADD(SIDEWALL_TYPE_XREF_CHECKSCMD)
ENDJOB

NT_JOB SIDEWALL_TYPE_XREF_CHECKSCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\sidewall_type_xref_checks.cmd
  ARGS
  USER maestro
  RUN DAILY
  DUEOUT EXEC 6AM
  RELEASE ADD(LIE.QASIDEWL)
ENDJOB

JOB LIE.QASIDEWL LINK PROCESS
  RUN DAILY
ENDJOB

