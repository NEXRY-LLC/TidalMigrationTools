APPL BPWFI001 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*04/24/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN SUNDAY
   DELAYSUB 04.00
   RELEASE ADD(FSBP_FI_CONCUR_MASTER_CCENTER_OUTBOUN)
   RELEASE ADD(FSBP_FI_CONCUR_MASTER_IO_OUTBOUND_WEE)
ENDJOB

SAP_JOB FSBP_FI_CONCUR_MASTER_CCENTER_OUTBOUN
   AGENT SAP_BZP100
   SAPJOBNAME FI_CONCUR_MASTER_CCENTER_OUTBOUN
   SAPJOBCLASS B
   STARTMODE ASAP
   ABAPNAME ZFI_CONCUR_EXTRACT
   VARIANT CONCUR_EXTRACT
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTSPOOLNAME ZFI_CONCUR_E
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 255
   PRINTFORMAT X_65_255
   BANNER D
   RECIPIENT KUMARDINESH
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN SUNDAY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

SAP_JOB FSBP_FI_CONCUR_MASTER_IO_OUTBOUND_WEE
   AGENT SAP_BZP100
   SAPJOBNAME FI_CONCUR_MASTER_IO_OUTBOUND_WEE
   SAPJOBCLASS B
   STARTMODE ASAP
   ABAPNAME ZIF_FI_CONCUR_INTORDERS
   VARIANT CON_INTORDERS
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZIF_FI_CONCU
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 60
   COLUMNS 255
   PRINTFORMAT X_65_255
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN SUNDAY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN SUNDAY
ENDJOB


