SAP_JOB JDAOP_ZOP_RE_BRU_OPZSP_SAP_SYNC
   AGENT CYBB_BFP030
   SAPJOBNAME JDAOP_ZOP_RE_BRU_OPZSP_SAP_SYNC
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME /JDAOP/ZOP_RE_BRU
   VARIANT OPZSP_SAP_SYNC
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTREL YES
   EXPIRATION 8
   COLUMNS 80
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   NOTIFY JOBSTART MAILBOX(JDAOP)
   PRINTCOPIES 001
   BANNER D
   RELDELAY  2
   IF ESPSHH = "13" THEN RUN TODAY
   RELEASE (JDAOP_ZOP_RE_BRU_RE_SYNC_PART_1)
ENDJOB

