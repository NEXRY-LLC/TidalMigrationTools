APPL BPDSD003 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB FSBP_ZSD_BACKORDER_BATCH
   AGENT SAP_BZP100
   SAPJOBNAME 'ZSD_BACKORDER BATCH'
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSDR_OPEN_ORDERS_REPORT
   VARIANT BATCH
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTSPOOLNAME ZSDR_OPEN_OR
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT ZFF_SAPDELC2
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   EARLYSUB 04.00
ENDJOB


