APPL WNBOSS01 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/* 05/20/206  -JFC- TAGGED APPL AS BOSS
/* 02/25/2020 -DSG- UPDATED AGENT FROM R7 TO R11.5.0
/*                - CHANGED JOBQ FROM CYBESPV210.CYBESPV2100
/* 03/05/2020 -DSG- ADDED RESOURCE TO AS400 JOBS - INC000002178397
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
TAG BOSS

AIX_JOB LIE.WNPED150 EXTERNAL APPLID(WNPED150) SCOPE(-00)
  RUN DAILY
  RELEASE ADD(LIS.!ESPAPPL)
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
  TAG MAJORAPPS
  RUN DAILY
  RELEASE ADD(MS_BOSS)
ENDJOB

AS400_JOB MS_BOSS
  RESOURCE ADD(1,WRNPROD)
  AGENT WRNPROD
  COMMAND CYBROBOT MS_BOSS
  JOBQ CAWAAGENT.CYBESPJOBS
  RUN DAILY
  RELEASE ADD(MS_BOSSC)
ENDJOB

AS400_JOB MS_BOSSC
  RESOURCE ADD(1,WRNPROD)
  AGENT WRNPROD
  COMMAND CYBROBOT MS_BOSSC
  JOBQ CAWAAGENT.CYBESPJOBS
  RUN DAILY
  RELEASE ADD(MTIC_LOADBAT)
ENDJOB

NT_JOB MTIC_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_run\batchfiles\boss\MTICLOAD\MTIC_LOAD.bat
  ARGS
  USER maestro
  RUN DAILY
    NOTWITH MTIC_LOADBAT/AIBOSS01
    NOTWITH MTIC_LOADBAT/AOBOSS01
    NOTWITH MTIC_LOADBAT/BLBOSS01
    NOTWITH MTIC_LOADBAT/DMBOSS01
    NOTWITH MTIC_LOADBAT/JOBOSS01
    NOTWITH MTIC_LOADBAT/LVBOSS01
    NOTWITH MTIC_LOADBAT/WGBOSS01
  RELEASE ADD(BOSSSAP_WN_LOADBAT)
ENDJOB

NT_JOB BOSSSAP_WN_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_run\batchfiles\boss\SAPLOAD\BOSSSAP_WN_LOAD.BAT
  ARGS
  USER maestro
  RUN DAILY
    NOTWITH BOSSSAP_81_LOADBAT
    NOTWITH BOSSSAP_AO_LOADBAT
    NOTWITH BOSSSAP_BL_LOADBAT
    NOTWITH BOSSSAP_88_LOADBAT
    NOTWITH BOSSSAP_CD_LOADBAT
    NOTWITH BOSSSAP_LV_LOADBAT
    NOTWITH BOSSSAP_8H_LOADBAT
  RELEASE ADD(BOSSCUREBUILD_WN_LOADBAT)
ENDJOB

NT_JOB BOSSCUREBUILD_WN_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_run\batchfiles\boss\CURELOAD\BOSSCureBuild_WN_Load.bat
  ARGS
  USER maestro
  RUN DAILY
    NOTWITH BOSSCUREBUILD_81_LOADBAT
    NOTWITH BOSSCUREBUILD_AO_LOADBAT
    NOTWITH BOSSCUREBUILD_BL_LOADBAT
    NOTWITH BOSSCUREBUILD_88_LOADBAT
    NOTWITH BOSSCUREBUILD_CD_LOADBAT
    NOTWITH BOSSCUREBUILD_LV_LOADBAT
    NOTWITH BOSSCUREBUILD_8H_LOADBAT
  RELEASE ADD(BOSSCOMP_WN_LOADBAT)
ENDJOB

NT_JOB BOSSCOMP_WN_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_run\batchfiles\boss\COMPLOAD\BOSSComp_WN_Load.bat
  ARGS
  USER maestro
  RUN DAILY
   NOTIFY OVERDUE MAILBOX(WNBOSS1)
   NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)
   NOTIFY FAILURE ABEND ALERT(REMD)
   IF ESPSHH = '13' THEN DUEOUT EXEC 4PM
   IF ESPSHH = '19' THEN DUEOUT EXEC 3AM
    NOTWITH BOSSCOMP_81_LOADBAT
    NOTWITH BOSSCOMP_AO_LOADBAT
    NOTWITH BOSSCOMP_BL_LOADBAT
    NOTWITH BOSSCOMP_88_LOADBAT
    NOTWITH BOSSCOMP_CD_LOADBAT
    NOTWITH BOSSCOMP_LV_LOADBAT
    NOTWITH BOSSCOMP_8H_LOADBAT
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

AIX_JOB LIE.!ESPAPPL
  TAG MAJORAPPS
  AGENT CYBA_ISBFP3
  COMMAND /bin/sleep
  ARGS 3
  USER bfpadm
  RUN DAILY
ENDJOB

