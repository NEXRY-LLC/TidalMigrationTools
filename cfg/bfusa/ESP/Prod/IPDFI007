NOTIFY ABEND FAILURE ALERT(REMD)

FILE_TRIGGER INT0037_IP
   AGENT SAP_IZP100
   FILENAME '/xfer/inbound/bp/INT0037_IP_*.txt' CREATE NOCHANGE(1)
   RELEASE ADD(FSIP_FCKR_JOB_SCH_PROCESS_IP)
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   RELEASE ADD(FSIP_FCKR_JOB_SCH_PROCESS_BP)
   RELEASE ADD(FSIP_FCKR_JOB_SCH_PROCESS_IP)
ENDJOB

SAP_JOB FSIP_FCKR_JOB_SCH_PROCESS_BP
   AGENT SAP_IZP100
   SAPJOBNAME FCKR_JOB_SCH_PROCESS_BP
   SAPJOBCLASS C
   STARTMODE ASAP
   RECIPIENTTYPE DLI
   SPOOLRECIPIENT 'FCKR JOB'
   ABAPNAME ZFIIR_CASHED_CHECKS_JB_SCH
   VARIANT BP_RENAME
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZFIIR_CASHED
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 200
   PRINTFORMAT X_65_200
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN DAILY
   EARLYSUB 05.00
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

SAP_JOB FSIP_FCKR_JOB_SCH_PROCESS_IP
   AGENT SAP_IZP100
   SAPJOBNAME FCKR_JOB_SCH_PROCESS_IP
   SAPJOBCLASS C
   STARTMODE ASAP
   RECIPIENTTYPE DLI
   SPOOLRECIPIENT 'FCKR JOB'
   ABAPNAME ZFIIR_CASHED_CHECKS_JB_SCH
   VARIANT IP_RENAME
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZFIIR_CASHED
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 200
   PRINTFORMAT X_65_200
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN DAILY
   EARLYSUB 05.00
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
   DUEOUT EXEC 8AM
ENDJOB

