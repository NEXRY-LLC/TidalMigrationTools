   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_sap_temp_BR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_INITIAL_BRCMD)
ENDJOB

