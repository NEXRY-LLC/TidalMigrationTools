   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_initial_BR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_SAP_TEMP_BRCMD)
ENDJOB

NT_JOB WA_DETAIL_SAP_TEMP_BRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_sap_temp_BR.cmd
   ARGS 5
   USER maestro
