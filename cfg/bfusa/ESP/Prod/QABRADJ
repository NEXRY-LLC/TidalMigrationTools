APPL QABRADJ WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

JOB LIS.QABRADJ LINK PROCESS
   RUN SAT
   IF TODAY('JULY 15 2017') THEN JOBATTR HOLD
   EARLYSUB 00.03
   RELEASE ADD(WA_HEADER_DETAIL_DELETE_BRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_DELETE_BRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_detail_delete_BR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_TEMP_BRCMD)
ENDJOB

NT_JOB WA_HEADER_TEMP_BRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_header_temp_BR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_DETAIL_TEMP_BRCMD)
ENDJOB

NT_JOB WA_DETAIL_TEMP_BRCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_detail_temp_BR.cmd
   ARGS 5
   USER maestro
   RUN SAT
   RELEASE ADD(WA_HEADER_SAP_INITIAL_BRCMD)
ENDJOB

