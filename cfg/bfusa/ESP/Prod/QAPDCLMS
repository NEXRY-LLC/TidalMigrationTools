APPL QAPDCLMS WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO LINK                   */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB LIE.QABASMDL EXTERNAL APPLID(QABASMDL)
  RUN SUN
  RELEASE ADD(PD_CLAIMS_TEMPCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER_CHECKS EXTERNAL APPLID(QACUSCHK)
  RUN SUN
  RELEASE ADD(PD_CLAIMS_TEMPCMD)
ENDJOB

NT_JOB VMSDOTCMD EXTERNAL APPLID(QAVMSDOT)
  RUN SUN
  RELEASE ADD(PD_CLAIMS_TEMPCMD)
ENDJOB

JOB LIS.QAPDCLMS LINK PROCESS
  RUN SUN
  RELEASE ADD(PD_CLAIMS_TEMPCMD)
  DELAYSUB 01.00
ENDJOB

NT_JOB PD_CLAIMS_TEMPCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\pd_claims_temp.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(PD_VEHICLE_DETAILS_TEMP)
ENDJOB

NT_JOB PD_VEHICLE_DETAILS_TEMP
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\pd_vehicle_details_temp.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(PD_DOT_DETAILS_TEMPCMD)
ENDJOB

NT_JOB PD_DOT_DETAILS_TEMPCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\pd_dot_details_temp.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(LIE.QAPDCLMS)
ENDJOB

JOB LIE.QAPDCLMS LINK PROCESS
   RUN SUN
ENDJOB

