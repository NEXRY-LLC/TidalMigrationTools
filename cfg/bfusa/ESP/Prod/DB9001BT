APPL DB9001BT WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)

JOB DB9001BT
  /*@@ SYSTEM=QMF
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN SUN
  DELAYSUB 01:15
  RELEASE ADD(DB9001GT)
  RELEASE ADD(DB9001IS)
  RELEASE ADD(DB9001IT)
  RELEASE ADD(DB9002GT)
  RELEASE ADD(DB9003GT)
  RELEASE ADD(DB9004GT)
  RELEASE ADD(DB9005GT)
  RELEASE ADD(DB9006GT)
  RELEASE ADD(DB9007GT)
  RELEASE ADD(DB9008GT)
  RELEASE ADD(DB9009GT)
  RELEASE ADD(DB9010GT)
ENDJOB

JOB DB9001GT
  /*@@ SYSTEM=QMF
  OPTIONS NORESTARTSTEP
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN SUN
  RELEASE ADD(DB9001IT)
ENDJOB

JOB DB9001IS
  /*@@ SYSTEM=QMF
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN SUN
  RUN JAN 04 2017
   IF TODAY('JAN 04 2017') THEN DELAYSUB 6:30
ENDJOB

