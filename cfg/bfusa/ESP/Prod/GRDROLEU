APPL GRDROLEU WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB GRC_ACTION_USAGE_SYNC_BGPCLNT030 EXTERNAL APPLID(GRDTRXUS) +
SCOPE(-4)
   RUN DAILY
   RELEASE ADD(GRC_ROLE_USAGE_SYNC_BFPCLNT030)
ENDJOB

SAP_JOB GRC_ROLE_USAGE_SYNC_BFPCLNT030
   AGENT CYBB_BGP030
   SAPJOBNAME GRC_ROLE_USAGE_SYNC_BFPCLNT030
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME GRAC_ROLE_USAGE_SYNC
   VARIANT BFPCLNT030
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTSPOOLNAME GRAC_ROLE_US
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   RECIPIENT S_BC_CYBERM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   BANNER D
   PRINTCOPIES 001
   RUN DAILY
   EARLYSUB 22.00
   IF TODAY('AUG 13 2017') THEN DELAYSUB 22.30
   RELEASE ADD(GRC_ROLE_USAGE_SYNC_PLACLNT400)
ENDJOB

