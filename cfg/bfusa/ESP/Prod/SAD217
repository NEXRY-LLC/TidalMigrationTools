NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN WEEKDAYS SAT
   IF TODAY('MON') AND ESPSHH LT '04' THEN RELDELAY 5
   IF TODAY('WEEKDAY SAT') AND ESPSHH GT '19' THEN DELAYSUB 22.30
   RELEASE ADD(ZSD_ZTAM_862_IDOC_CREATE)
ENDJOB

SAP_JOB ZSD_ZTAM_862_IDOC_CREATE
