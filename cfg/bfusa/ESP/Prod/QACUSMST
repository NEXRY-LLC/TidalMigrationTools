APPL QACUSMST WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND OVERDUE MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

NT_JOB CUST_TYPE_XREFCMD EXTERNAL APPLID(QACUSXRF)
  RUN DAILY
  RELEASE ADD(CUSTOMER_MASTER)
ENDJOB

NT_JOB DEST_XREFCMD EXTERNAL APPLID(QADEST)
  RUN DAILY
  RELEASE ADD(CUSTOMER_MASTER)
ENDJOB

NT_JOB GROUP_54_XREFCMD EXTERNAL APPLID(QAGRP54)
  RUN DAILY
  RELEASE ADD(CUSTOMER_MASTER)
ENDJOB

NT_JOB SALES_CHANNEL_XREFCMD EXTERNAL APPLID(QASALCHN)
  RUN DAILY
  RELEASE ADD(CUSTOMER_MASTER)
ENDJOB

NT_JOB WHSE_XREF EXTERNAL APPLID(QAWHSE)
  RUN DAILY
  RELEASE ADD(CUSTOMER_MASTER)
ENDJOB

NT_JOB B_GROUP_XREFCMD EXTERNAL APPLID(QABGROUP)
  RUN DAILY
  RELEASE ADD(CUSTOMER_MASTER)
ENDJOB

NT_JOB INTERFACE_1069 EXTERNAL APPLID(SSISPPIW) SCOPE(-10)
  RUN DAILY
  NORUN JUL 24 2014
  RELEASE ADD(LIS.QACUSMST)
ENDJOB

JOB LIS.QACUSMST LINK
  RUN DAILY
  RELEASE ADD(CUSTOMER_MASTER)
ENDJOB

NT_JOB CUSTOMER_MASTER
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\customer_master.cmd
  USER maestro
  RUN DAILY
  RELEASE ADD(PPIW_CUSTOMER_MASTER_EXPORT)
ENDJOB

NT_JOB PPIW_CUSTOMER_MASTER_EXPORT
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\ppiw_customer_master_export.cmd
  USER maestro
  RUN DAILY
  RELEASE ADD(PPIW_CUSTOMER_MASTER_EXPORT_CTL)
ENDJOB

NT_JOB PPIW_CUSTOMER_MASTER_EXPORT_CTL
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\ppiw_customer_master_export_ctl.cmd
  USER maestro
  RUN DAILY
  DUEOUT EXEC 4AM
  RELEASE ADD(LIE.QACUSMST)
ENDJOB

JOB LIE.QACUSMST LINK PROCESS
  RUN DAILY
ENDJOB

