APPL BPISD003 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB FSBP_SD_TMS2SAP_INBOUND1_SHPMT_DAILY
   AGENT SAP_BZP100
   SAPJOBNAME SD_TMS2SAP_INBOUND1_SHPMT_DAILY
   SAPJOBCLASS C
   ABAPNAME ZSDI_TMS2SAP_ERR2IN_FILE_TRANF
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT FSBP_BATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME ZSDI_TMS2SAP
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
   STARTMODE ASAP
   ABAPNAME ZSDI_SHPMNT_TMS_INTERFACE
   VARIANT TMS_SHPMT
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT FSBP_BATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME ZSDI_SHPMNT_
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
   RUN DAILY
   DUEOUT EXEC REALNOW PLUS 20 MINUTES
   NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)
ENDJOB


