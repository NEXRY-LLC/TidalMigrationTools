  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wa_dealers.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(WA_DEALERS_CHECKS)
ENDJOB

