  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wa_dealers_temp_load.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(WA_DEALERSCMD)
ENDJOB

