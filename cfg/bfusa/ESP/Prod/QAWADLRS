APPL QAWADLRS WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO LINK                   */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

JOB WA1571D EXTERNAL SCOPE(-24:00)
  RUN SUN
  RELEASE ADD(DTS_CONTROL_WA_DEALERS)
ENDJOB

NT_JOB LIE.QABASMDL EXTERNAL APPLID(QABASMDL)
  RUN SUN
  RELEASE ADD(WA_DEALERS_TEMP_LOAD)
ENDJOB

NT_JOB CUSTOMER_MASTER_CHECKS EXTERNAL APPLID(QACUSCHK)
  RUN SUN
  RELEASE ADD(WA_DEALERS_TEMP_LOAD)
ENDJOB

NT_JOB SERCONDXREF EXTERNAL APPLID(QASVCCND)
  RUN SUN
  RELEASE ADD(WA_DEALERS_TEMP_LOAD)
ENDJOB

NT_JOB VMSDOTCMD EXTERNAL APPLID(QAVMSDOT)
  RUN SUN
  RELEASE ADD(WA_DEALERS_TEMP_LOAD)
ENDJOB

JOB WA1570D EXTERNAL SCOPE(-24:00)
  RUN SUN
  RELEASE ADD(WA_DEALERS_TEMP_LOAD)
ENDJOB

JOB LIS.QAWADLRS LINK PROCESS
  RUN SUN
  DELAYSUB 01.00
  RELEASE ADD(DTS_CONTROL_WA_DEALERS)
ENDJOB

NT_JOB DTS_CONTROL_WA_DEALERS
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\dts_control_wa_dealers.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(WA_DEALERS_TEMP_LOAD)
ENDJOB

