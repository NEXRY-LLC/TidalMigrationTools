JOB LIS.!ESPAPPL LINK PROCESS
   RUN TODAY
   RELEASE ADD(ZMMIMI10_DAILY_COSTCO_INV_US)
ENDJOB

SAP_JOB ZMMIMI10_DAILY_COSTCO_INV_US
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMIMI10_DAILY_COSTCO_INV_US
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMMIMI10
   VARIANT COSTCO_INV_US
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN TODAY
   IF ESPSTIME GE '23.45' THEN NORUN TODAY
   IF ESPSTIME LE '07.00' THEN NORUN TODAY
   RELEASE (ZMMIMI10_DAILY_COSTCO_INV_CANADA)
ENDJOB

