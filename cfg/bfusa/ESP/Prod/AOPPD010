AIX_JOB LIE.!ESPAPPL
   AGENT CYBA_ISBFP3
   COMMAND /bin/sleep
   ARGS 3
   USER bfpadm
    DUEOUT EXEC 22.00
    IF TODAY('SUNDAY') THEN DUEOUT EXEC 23.50
   RUN DAILY
ENDJOB

