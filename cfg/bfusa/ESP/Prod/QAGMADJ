APPL QAGMADJ WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

JOB WA2240S EXTERNAL SCOPE(-48.00)
   RUN THU SUN
   RELEASE ADD(LIS.QAGMADJ)
ENDJOB

JOB LIS.QAGMADJ LINK PROCESS
   RUN THU SUN
   DELAYSUB 01.00
   RELEASE ADD(GM_ADJ_CODE_DESC_XREF)
ENDJOB

NT_JOB GM_ADJ_CODE_DESC_XREF
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\gm_adj_code_desc_xref.cmd
   ARGS
   USER maestro
   RUN THU SUN
   RELEASE ADD(LIE.QAGMADJ)
ENDJOB

JOB LIE.QAGMADJ LINK PROCESS
   RUN THU SUN
ENDJOB

