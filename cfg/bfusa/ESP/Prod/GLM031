APPL GLM031

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*  REMOVED AFTER ADD AND RUN REF STMTS     JRE  12/02/15
/*04/01/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
    RUN 4TH WORKDAY OF MONTH
    DELAYSUB 23.00
    RELEASE ADD(ZF_CREATE_TXGRP_IN_BSET_HAMMER)
ENDJOB

SAP_JOB ZF_CREATE_TXGRP_IN_BSET_HAMMER
    SAPUSER SAPADM
    AGENT CYBB_BFP030
    SAPJOBNAME ZF_CREATE_TXGRP_IN_BSET_HAMMER
    SAPJOBCLASS C
    ABAPNAME ZF_CREATE_TXGRP_IN_BSET
    VARIANT 'PRIOR MONTH'
    LANGUAGE E
    PRINTDEST CC13
    PRINTCOPIES 001
    EXPIRATION 8
    LINES 58
    COLUMNS 140
    BANNERPAGE YES
    BANNER D
    RECIPIENT STEWARTTHOMA
    ARCMODE PRINT
    RUN 4TH WORKDAY OF MONTH
    RELEASE ADD(ZF_CREATE_TXGRP_HAMMER_BSEG)
ENDJOB

SAP_JOB ZF_CREATE_TXGRP_HAMMER_BSEG
    SAPUSER SAPADM
    AGENT CYBB_BFP030
    SAPJOBNAME ZF_CREATE_TXGRP_HAMMER_BSEG
    SAPJOBCLASS C
    ABAPNAME ZF_CREATE_TXGRP
    VARIANT 'PRIOR MONTH'
    LANGUAGE E
    PRINTDEST CC13
    PRINTCOPIES 001
    EXPIRATION 8
    LINES 58
    COLUMNS 140
    BANNERPAGE YES
    BANNER D
    RECIPIENT STEWARTTHOMA
    ARCMODE PRINT
    RUN 4TH WORKDAY OF MONTH
    RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
    RUN 4TH WORKDAY OF MONTH
ENDJOB

