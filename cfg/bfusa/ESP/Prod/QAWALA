APPL QAWALA WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/* 04/18/2016 -JFC- ADDED DUEOUT STATEMENT                            */
/* 05/31/2016 -JFC- PUT EXTERNAL FOR QAVEITEM ON REQUEST PER J. HRITZ */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

NT_JOB WA_HEADER_DETAIL_REBUILD_MXCMD EXTERNAL APPLID(QAMXADJ)
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_HDRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_REBUILD_COCMD EXTERNAL APPLID(QACOADJ)
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_HDRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_REBUILD_CLCMD EXTERNAL APPLID(QACLADJ)
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_HDRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_REBUILD_BRCMD EXTERNAL APPLID(QABRADJ)
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_HDRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_REBUILD_CRCMD EXTERNAL APPLID(QACRADJ)
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_HDRCMD)
ENDJOB

NT_JOB WA_HEADER_DETAIL_REBUILD_ARCMD EXTERNAL APPLID(QAARADJ)
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_HDRCMD)
ENDJOB

JOB LIS.QAWALA LINK PROCESS
  DELAYSUB 00:05
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_HDRCMD)
ENDJOB

NT_JOB WALA_CONSOLIDATED_HDRCMD
 AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wala_consolidated_hdr.cmd
  ARGS
  USER maestro
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_DTLCMD)
ENDJOB

NT_JOB WALA_CONSOLIDATED_DTLCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wala_consolidated_dtl.cmd
  ARGS
  USER maestro
  RUN SAT
  RELEASE ADD(WALA_CONSOLIDATED_DTL_CHECKS)
ENDJOB

NT_JOB WALA_CONSOLIDATED_DTL_CHECKS
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\wala_consolidated_dtl_checks.cmd
  ARGS
  USER maestro
  RUN SAT
  DUEOUT EXEC 6AM
  RELEASE ADD(LIE.QAWALA)
ENDJOB

JOB LIE.QAWALA LINK PROCESS
  RUN SAT
ENDJOB

