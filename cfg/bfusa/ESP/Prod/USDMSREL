  /*@@ SYSTEM=USCHKSYS
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN WEEKDAYS
  NORUN 1ST WORKDAY OF MONTH
  RELEASE ADD(USDREL03)
ENDJOB

JOB USDREL03
  /*@@ SYSTEM=USCHKSYS
  CCCHK RC(5:4095) FAIL CONTINUE
  RUN WEEKDAYS
  NORUN 1ST WORKDAY OF MONTH
  RELEASE ADD(USDELSTG)
ENDJOB

JOB USDELSTG
  /*@@ SYSTEM=USCHKSYS
  CCCHK RC(9:4095) FAIL CONTINUE
  RUN WEEKDAYS
  NORUN 1ST WORKDAY OF MONTH
