APPL IPDPOPAY WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB FSIP_IP_POSITIVE_PAY
   AGENT SAP_IZP100
   SAPJOBNAME IP_POSTIVE_PAY
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZRFCHKE00
   VARIANT 'IPPOSITIVE PAY'
   STEPUSER ZBATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTSPOOLNAME ZRFCHKE00_
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 132
   PRINTFORMAT X_65_132
   RECIPIENT ZBATCH
   ARCMODE PRINT
   BANNER D
   PRINTCOPIES 001
   RUN DAILY
   EARLYSUB 20.00
   RELEASE ADD(IPDPOPAY.END)
ENDJOB

SAP_JOB FSIP_BP_POSTIVE_PAY
   AGENT SAP_IZP100
   SAPJOBNAME BP_POSTIVE_PAY
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZRFCHKE00
   VARIANT 'BPPOSITIVE PAY'
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZRFCHKE00_
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 132
   PRINTFORMAT X_65_132
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN TODAY
   DELAYSUB 20.10
   RELEASE ADD(IPDPOPAY.END)
ENDJOB

