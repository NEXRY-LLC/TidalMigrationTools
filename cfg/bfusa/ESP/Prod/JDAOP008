APPL JDAOP008 WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
/**********************************************************************/
/* 04/13/18 - CREATED PER JOHN GREGORIC - DSG -INC000001568677
/* 05/08/18 - ADDED DUEOUT STATEMENT TO FILE TRIGGER - DSG
/**********************************************************************/
NOTIFY ABEND FAILURE ALERT(REMD)

FILE_TRIGGER OUT_UDT_BOR_LOG_REPORT_FLAT
   AGENT CYBB_BFP030
   FILENAME +
'/data/BFP/interfaces/input/datain/OUT_UDT_BOR_LOG_REPORT_FLAT*.*' +
CREATE
   RUN DAILY
   RELEASE ADD(JDAOP_ZJDA_BOR_UPDATE,ZJDA_BOR_UPDATE)
   DUEOUT EXEC REALNOW PLUS 8 HOURS
   NOTIFY OVERDUE MAILBOX(DGRIMMETT1)
ENDJOB

SAP_JOB ZJDA_BOR_UPDATE
   AGENT CYBB_BFP030
   SAPJOBNAME ZJDA_BOR_UPDATE
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZJDA_BOR_UPDATE
   VARIANT ZJDA_BOR_UPDAT
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   NOTIFY JOBSTART MAILBOX(JDAOP)
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN DAILY
   RELDELAY 2
   RELEASE (ZJDA_BOR_DELETE)
ENDJOB

SAP_JOB ZJDA_BOR_DELETE
   AGENT CYBB_BFP030
   SAPJOBNAME ZJDA_BOR_DELETE
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZJDA_BOR_DELETE
   VARIANT ZJDA_BOR_DELET
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   NOTIFY JOBEND MAILBOX(JDAOP)
   RUN DAILY
ENDJOB


