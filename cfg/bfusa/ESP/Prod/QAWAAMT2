APPL QAWAAMT2 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO LINK                   */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

NT_JOB LIE.QAVMSDOT EXTERNAL APPLID(QAVMSDOT)
   RUN SUN
   RELEASE ADD(WAAMTS_TIRE_AND_TICKET_TMP)
ENDJOB

NT_JOB LIE.QACUSCHK EXTERNAL APPLID(QACUSCHK)
   RUN SUN
   RELEASE ADD(WAAMTS_TIRE_AND_TICKET_TMP)
ENDJOB

NT_JOB CUSTOMER_MASTER_CHECKS EXTERNAL APPLID(QACUSCHK)
   RUN SUN
   RELEASE ADD(WAAMTS_TIRE_AND_TICKET_TMP)
ENDJOB

NT_JOB LIE.QAOVRW EXTERNAL APPLID(QAOVRW)
   RUN SUN
   RELEASE ADD(WAAMTS_TIRE_AND_TICKET_TMP)
ENDJOB

JOB LIS.QAWAAMT2 LINK PROCESS
   RUN SUN
   EARLYSUB 01.00
   RELEASE ADD(WAAMTS_TIRE_AND_TICKET_TMP)
ENDJOB

NT_JOB WAAMTS_TIRE_AND_TICKET_TMP
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_amts_tire_and_ticket_tmp.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(WAAMTS_TIRE_AND_TICKET)
ENDJOB

NT_JOB WAAMTS_TIRE_AND_TICKET
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_amts_tire_and_ticket.cmd
   ARGS
   USER maestro
   RUN SUN
   RELEASE ADD(LIE.QAWAAMT2)
ENDJOB

JOB LIE.QAWAAMT2 LINK PROCESS
   RUN SUN
ENDJOB

