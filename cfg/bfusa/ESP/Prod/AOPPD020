ENDJOB

AIX_JOB LIE.!ESPAPPL
   AGENT CYBA_ISBFP3
   COMMAND /bin/sleep
   ARGS 3
   USER bfpadm
   RUN DAILY
    IF ESPSHH = '10' THEN DUEOUT EXEC 12.00
    IF ESPSHH = '19' THEN DUEOUT EXEC 22.00
    IF TODAY('SUN') AND ESPSHH EQ '19' THEN DUEOUT EXEC 23.50
   NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)
ENDJOB

