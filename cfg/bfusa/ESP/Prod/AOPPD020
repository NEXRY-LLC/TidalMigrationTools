ENDJOB

UNIX_JOB ARC2AOT1
   AGENT CYBA_ISBFP3
   USER bfpadm
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/bom1t_f191.txt D
   RUN DAILY
   RELEASE ADD(ARC2AOT2)
ENDJOB

