APPL BPIMM005
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
/*JOB TO RUN HOURLY*/

SAP_JOB BPFP_ZMM_MC_OUTBOUND_HOURLY
   AGENT SAP_BZP100
   SAPJOBNAME ZMM_MC_OUTBOUND_HOURLY
   SAPJOBCLASS A
   ABAPNAME ZMMI_MC_GET_STOCK
   VARIANT MC_OUTBOUND
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMMI_MC_GET_
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN DAILY
ENDJOB


