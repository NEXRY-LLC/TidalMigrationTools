JOB JDAOQ003 LINK PROCESS
   RUN DAILY
   RELDELAY  120
   RELEASE ADD(ZJDA_UDT_ROUTE_DETAILS)
   RELEASE ADD(JDAOP_ZMMIMI44_JDA)
   RELEASE ADD(ZSDLER29_UNCHECK_COSTCO,ZMMPOJDA_OPENPO_JDA01,+
ZMMSTO_OPENSTO_JDA01,ZJDA_SUPPLYPT_FILELOAD)
ENDJOB

SAP_JOB ZSDLER29_UNCHECK_COSTCO
   AGENT CYBB_BFT030
   SAPJOBNAME ZSDLER29_UNCHECK_COSTCO
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSDLER29
   VARIANT 'UNCHECK FLAG'
   STEPUSER SAPADM
   LANGUAGE E
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   RECIPIENT SAPADM
   ARCMODE PRINT
   BANNER D
   PRINTCOPIES 001
   /*RUN DAILY
   RELEASE ADD(ZSDJDA01_JDAOP)
ENDJOB

