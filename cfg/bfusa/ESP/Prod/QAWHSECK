APPL QAWHSECK WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

NT_JOB WHSE_XREF EXTERNAL APPLID(QAWHSE)
  RUN THU SUN
  RELEASE ADD(LIS.QAWHSECK)
ENDJOB

JOB LIS.QAWHSECK LINK PROCESS
  RUN THU SUN
  DELAYSUB 01.00
  RELEASE ADD(WHSE_XREF_CHECKSCMD)
ENDJOB

NT_JOB WHSE_XREF_CHECKSCMD
  AGENT CYBA_PPIWPROD
  USER maestro
  CMDNAME X:\cmd_scripts\whse_xref_checks.cmd
  ARGS
  RUN THU SUN
  DUEOUT EXEC 6AM
  RELEASE ADD(LIE.QAWHSECK)
ENDJOB

JOB LIE.QAWHSECK LINK PROCESS
  RUN THU SUN
ENDJOB

