JOB LIS.!ESPAPPL LINK PROCESS
   RUN TODAY
   RELEASE ADD(FSBP_ARIBA_REMOVE_MANUAL_INV)
ENDJOB

SAP_JOB FSBP_ARIBA_REMOVE_MANUAL_INV
   AGENT SAP_BZP100
   SAPJOBNAME ARIBA_REMOVE_MANUAL_INV
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZARIBA_REMOVE_MANUAL_INV
   VARIANT ARIBA_INV_M
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZARIBA_REMOV
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN TODAY
   RELEASE (FSBP_REPROCESS_ARIBA_INV_ERR)
ENDJOB

