APPL QALOIS WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO NT JOB                 */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND OVERDUE MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB LIE.QABASMDL EXTERNAL APPLID(QABASMDL)
  RUN SUN
  RELEASE ADD(LIS.QALOIS)
ENDJOB

NT_JOB LIE.QAVMSDOT EXTERNAL APPLID(QAVMSDOT)
  RUN SUN
  RELEASE ADD(LIS.QALOIS)
ENDJOB

NT_JOB LIE.QACUSCHK EXTERNAL APPLID(QACUSCHK)
  RUN SUN
  RELEASE ADD(LIS.QALOIS)
ENDJOB

JOB LIS.QALOIS LINK PROCESS
  RUN SUN
  DELAYSUB 01.00
  RELEASE ADD(DTS_CONTROL_LOIS_UNIVERSE_EXTRACT_TEMPCMD)
ENDJOB

NT_JOB DTS_CONTROL_LOIS_UNIVERSE_EXTRACT_TEMPCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\dts_control_lois_universe_extract_temp.cmd
  ARGS
  RUN SUN
  USER maestro
  RELEASE ADD(LOIS_UNIVERSE_EXTRACT_TEMPCMD)
ENDJOB

NT_JOB LOIS_UNIVERSE_EXTRACT_TEMPCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\lois_universe_extract_temp.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(LOIS_UNIVERSE_LAWSUITCMD)
ENDJOB

NT_JOB LOIS_UNIVERSE_LAWSUITCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\lois_universe_lawsuit.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(LOIS_UNIVERSE_EXTRACTCMD)
ENDJOB

NT_JOB LOIS_UNIVERSE_EXTRACTCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\lois_universe_extract.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(LIE.QALOIS)
ENDJOB

NT_JOB LIE.QALOIS
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\LinkedInEnd.cmd
  USER maestro
  RUN SUN
  DUEOUT EXEC 08.00
ENDJOB

