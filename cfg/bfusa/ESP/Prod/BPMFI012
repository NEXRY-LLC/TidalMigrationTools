NOTIFY ABEND FAILURE ALERT(REMD)

SAP_JOB FSBP_FI_CO_FYEAR_PERIOD_VARINT_UPDATE EXTERNAL +
     APPLID(BPMFI006) SCOPE(-05)
   RUN LAST DAY OF MONTH
   RELEASE (FSBP_KKAI_WARRANTY_MONTHEND_REVENUE_1)
ENDJOB

SAP_JOB FSBP_KKAI_WARRANTY_MONTHEND_REVENUE_1
   AGENT SAP_BZP100
   SAPJOBNAME KKAI_WARRANTY_MONTHEND_REVENUE_1
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME SAPKKA01BG
   VARIANT ZSC_WARRANTY
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME SAPKKA01BG_
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   EARLYSUB 00.20
   RUN LAST DAY OF MONTH
   NOTIFY ABEND FAILURE JOBEND MAILBOX(BPMFI012)
   RELEASE (FSBP_KKAI_WARRANTY_MONTHEND_REVENUE_2)
ENDJOB

SAP_JOB FSBP_KKAI_WARRANTY_MONTHEND_REVENUE_2
   AGENT SAP_BZP100
   SAPJOBNAME KKAI_WARRANTY_MONTHEND_REVENUE_2
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RKO7KO8G
   VARIANT WARRANTY_REV
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME RKO7KO8G_S_B
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN LAST DAY OF MONTH
   NOTIFY ABEND FAILURE JOBEND MAILBOX(BPMFI012)
   RELEASE (FSBP_KKAI_WARRANTY_MONTHEND_REVENUE_3)
ENDJOB

SAP_JOB FSBP_KKAI_WARRANTY_MONTHEND_REVENUE_3
   AGENT SAP_BZP100
   SAPJOBNAME KKAI_WARRANTY_MONTHEND_REVENUE_3
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RKOLIS00
   VARIANT ZSC_WARRANTY_C
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME RKOLIS00_S_B
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN LAST DAY OF MONTH
   NOTIFY ABEND FAILURE JOBEND MAILBOX(BPMFI012)
