APPL QASHPMNT WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*REMEDY INC000000659851 - REMOVE CMD JOB - 04/08/2015 - JFC          */
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO NT JOB                 */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY OVERDUE MAILBOX(SQLSHAREDSERVICES)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB INTERFACE_1088 EXTERNAL APPLID(SSISPPIW) SCOPE(-96)
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(LIS.QASHPMNT)
ENDJOB

NT_JOB LIE.QAMTHLOK EXTERNAL APPLID(QAMTHLOK)
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(ALL_SHIPMENTSCMD)
ENDJOB

JOB LIS.QASHPMNT LINK PROCESS
  RUN 2ND WORKDAY OF MONTH
  DELAYSUB 19.00
  RELEASE ADD(ALL_SHIPMENTSCMD)
ENDJOB

NT_JOB ALL_SHIPMENTSCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\all_shipments.cmd
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(ALL_SHIPMENTS_CHECKCMD)
ENDJOB

NT_JOB ALL_SHIPMENTS_CHECKCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\all_shipments_check.cmd
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(PPM_OE_SHIPMENTSCMD)
  RELEASE ADD(GLOBAL_SHIPMENTSCMD)
ENDJOB

NT_JOB PPM_OE_SHIPMENTSCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\ppm_oe_shipments.cmd
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(LIE.QASHPMNT)
ENDJOB

NT_JOB GLOBAL_SHIPMENTSCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\global_shipments.cmd
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(LIE.QASHPMNT)
ENDJOB

NT_JOB LIE.QASHPMNT
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\LinkedInEnd.cmd
  USER maestro
  RUN 2ND WORKDAY OF MONTH
   DUEOUT EXEC 23:00
ENDJOB

