  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\global_upc_it_nbr_xref_checks.cmd
  ARGS
  USER maestro
  RUN THU SUN
  RELEASE ADD(GLOBAL_UPC_IT_NBR_XREF_TEMP_CHECKSCMD)
  RELEASE ADD(ITEM_MASTER_AR_CHECKSCMD)
  RELEASE ADD(ITEM_MASTER_BR_CHECKSCMD)
  RELEASE ADD(ITEM_MASTER_CL_CHECKSCMD)
  RELEASE ADD(ITEM_MASTER_CO_CHECKSCMD)
  RELEASE ADD(ITEM_MASTER_CR_CHECKSCMD)
  RELEASE ADD(ITEM_MASTER_VE_CHECKSCMD)
  RELEASE ADD(ITEM_MASTER_MX_CHECKSCMD)
ENDJOB

