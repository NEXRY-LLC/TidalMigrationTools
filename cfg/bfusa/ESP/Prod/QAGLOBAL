APPL QAGLOBAL WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB ITEM_MASTER_DELETED_CHECKSCMD EXTERNAL APPLID(QAITMMST)
  RUN THU SUN
  RELEASE ADD(GLOBAL_ITEM_MASTERCMD)
  RELEASE ADD(GLOBAL_WHEEL_POSITIONCMD)
ENDJOB

NT_JOB VMSDOTCMD EXTERNAL APPLID(QAVMSDOT)
  RUN THU SUN
  RELEASE ADD(GLOBAL_DOT_ITEM_MASTERCMD)
ENDJOB

NT_JOB CUSTOMER_MASTER EXTERNAL APPLID(QACUSMST)
  RUN THU SUN
  RELEASE ADD(GLOBAL_CUSTOMER_MASTERCMD)
ENDJOB

JOB LIS.QAGLOBAL LINK PROCESS
  RUN THU SUN
  DELAYSUB 01.00
  RELEASE ADD(GLOBAL_UPC_XREF_ADDS)
ENDJOB

NT_JOB GLOBAL_UPC_XREF_ADDS
  AGENT CYBA_PPIWPROD
  CMDNAME X:\CMD_SCRIPTS\GLOBAL_UPC_XREF_ADDS.CMD
  ARGS
  USER maestro
  RUN THU SUN
  RELEASE ADD(GLOBAL_ITEM_MASTERCMD)
ENDJOB

NT_JOB GLOBAL_ITEM_MASTERCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\global_item_master.cmd
  ARGS
  USER maestro
  RUN THU SUN
  RELEASE ADD(GLOBAL_UPC_IT_NBR_XREF_CHECKSCMD)
ENDJOB

