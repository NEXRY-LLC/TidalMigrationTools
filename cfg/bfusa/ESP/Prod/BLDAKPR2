SAP_JOB RSNAST00_BILLING_INVOICE_FAX_0530AM
   AGENT CYBB_BFP030
   SAPJOBNAME RSNAST00_BILLING_INVOICE_FAX
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RSNAST00
   VARIANT ZBILLOUT_FAX
   STEPUSER INVOICE_FAX
   LANGUAGE E
   PRINTDEST LOCL
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT SOLMAN
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN TUE WED THU FRI SAT SUN
   EARLYSUB 05.30
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB RESUME.BLD002PT LINK PROCESS
   RUN TUE WED THU FRI SAT SUN
   RELDELAY  2
   ESP RESUME BL01.BLD002PT
   RELEASE ADD(BLDAKPR2_CHILDREN_COMPLETE)
ENDJOB

FILE_TRIGGER BLDAKPR2_CHILDREN_COMPLETE
   AGENT CYBB_BFP030
   FILENAME +
'/data/BFP/interfaces/output/dataout/zsdbli10_bld002p_complete.txt' +
CREATE NOCHANGE(1)
   RUN TUE WED THU FRI SAT SUN
   RELEASE ADD(ARCBLDAKPR2)
ENDJOB

UNIX_JOB ARCBLDAKPR2
   AGENT CYBA_ISBFP3
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/output/dataout/zsdbli10_bld002p_complete.txt D
   USER bfpadm
   RUN TUE WED THU FRI SAT SUN
   RELEASE ADD(SUSPEND.BLD002PT)
   RELEASE ADD(TRANSFER_INVOICES_TO_PRINTSHOP)
ENDJOB

JOB SUSPEND.BLD002PT LINK PROCESS
   RUN TUE WED THU FRI SAT SUN
   ESP SUSPEND BL01.BLD002PT
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

AIX_JOB TRANSFER_INVOICES_TO_PRINTSHOP
   AGENT CYBA_ISBFP3
   USER root
   SCRIPTNAME /local/bin/prtshp_transfer.sh
   ARGS BFP INV
   RUN TUE WED THU FRI SAT SUN
   RELDELAY  81
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
   DUEOUT EXEC 8AM
ENDJOB


