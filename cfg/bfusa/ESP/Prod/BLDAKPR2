APPL BLDAKPR2 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*11/07/2018 -JFC- CHANGED DUMMY ZOS TO LINK JOBS                    */
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

UNIX_JOB BLDAKPR1.COMPLETE EXTERNAL APPLID(BLDAKPR1) SCOPE(-04.00)
   RUN TUE WED THU FRI SAT SUN
   RELEASE ADD(LIS.!ESPAPPL)
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
   RUN TUE WED THU FRI SAT SUN
   EARLYSUB 04.00
   RELEASE ADD(ZSD70AV3A_INVOICE_PRINT_1000_10)
   RELEASE ADD(ZSD70AV3A_INVOICE_PRINT_1000_20)
   RELEASE ADD(ZSD70AV3A_INVOICE_PRINT_1000_40)
   RELEASE ADD(ZSD70AV3A_INVOICE_PRINT_2000)
   RELEASE ADD(ZSD70AV3A_INVOICE_PRINT_3000)
   RELEASE ADD(ZSD70AV3A_INVOICE_PRINT_4000)
   RELEASE ADD(ZSD70AV3A_INVOICE_PRINT_6000)
   RELEASE ADD(RSNAST00_BILLING_INVOICE_FAX_0530AM)
ENDJOB

SAP_JOB ZSD70AV3A_INVOICE_PRINT_1000_10
   AGENT CYBB_BFP030
   SAPJOBNAME ZSD70AV3A_INVOICE_PRINT_1000_10
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSD70AV3A
   VARIANT ZBILLOUT_1000A
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST AK30
   PRINTIMMED YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN TUE WED THU FRI SAT SUN
   CHILDMONITOR YES
   RELEASE ADD(RESUME.BLD002PT)
ENDJOB

