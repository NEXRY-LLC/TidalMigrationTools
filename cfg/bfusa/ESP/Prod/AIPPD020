SAP_JOB ZMMBI018_TESS_F181
    SAPUSER SAPADM
    AGENT CYBB_BFP030
    SAPJOBNAME ZMMBI018_TESS_F181
    RECIPIENTTYPE MAIL
    SPOOLRECIPIENT 'restrepohernan@BFUSA.COM'
    SAPJOBCLASS C
    ABAPNAME ZMMBI018
    VARIANT ZMMBI018_F181T
    LANGUAGE E
    PRINTDEST CP40
    PRINTCOPIES 001
    EXPIRATION 8
    COLUMNS 132
    BANNER D
    RECIPIENT DUETTAD
    ARCMODE PRINT
    RUN DAILY
    RELEASE ADD(AR_BOM1T_F181TXT)
ENDJOB

UNIX_JOB AR_BOM1T_F181TXT
    AGENT CYBA_ISBFP3
    SCRIPTNAME /local/bin/arcfile.sh
    ARGS interfaces/input/datain/bom1t_f181.txt D
    USER bfpadm
    RUN DAILY
    RELEASE ADD(AR_BOM2T_F181TXT)
ENDJOB

