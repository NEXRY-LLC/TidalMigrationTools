APPL SWESPR31 JOB_ANCESTOR_WAIT

NOTIFY FAILURE ABEND ALERT(REMD)


JCLLIB 'FTRF.PRDJOBS'
COPYJCL 'SWS.ESP.COPYLIB'
TEMPLIB 'SWS.ESP.TEMPLIB'
DOCLIB 'SWS.ESP.DOCLIB'

IF !ESPAPSUB# > 1 THEN JCLLIB 'SWS.ESP.COPYLIB'

OPTIONS RESTARTSTEP

JOB SWESPR31
  RUN NOW
  RELEASE ADD(SWESPR32)
  RELEASE ADD(SWESPR33)
ENDJOB

JOB SWESPR32
  RUN NOW
ENDJOB

JOB SWESPR33
  RUN NOW
ENDJOB

