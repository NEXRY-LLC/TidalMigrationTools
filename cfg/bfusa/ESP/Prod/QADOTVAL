APPL QADOTVAL WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
NOTIFY OVERDUE MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB BFSCLAIMSCMD EXTERNAL APPLID(QABFSCLM)
  RUN SUN
  RELEASE ADD(LIS.QADOTVAL)
ENDJOB

NT_JOB WA_CONSOLIDATEDCMD EXTERNAL APPLID(QAWACNSL)
  RUN SUN
  DUEOUT EXEC 2PM
  RELEASE ADD(LIS.QADOTVAL)
ENDJOB

JOB LIS.QADOTVAL LINK PROCESS
   RUN SUN
   DELAYSUB 01.00
   RELEASE ADD(DOT_VALIDATION_XREF_CHECKSCMD)
ENDJOB

NT_JOB DOT_VALIDATION_XREF_CHECKSCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\DOT_VALIDATION_XREF_CHECKS.cmd
   ARGS
   RUN SUN
   USER maestro
   RELEASE ADD(LIE.QADOTVAL)
ENDJOB

JOB LIE.QADOTVAL LINK PROCESS
   RUN SUN
ENDJOB

