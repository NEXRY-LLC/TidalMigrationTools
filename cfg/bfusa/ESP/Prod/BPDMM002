APPL BPDMM002
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB FSBP_BPDMM002_DAILY LINK PROCESS
   RUN DAILY
   EARLYSUB 00.30
   RELEASE ADD(FSBP_MM_PANEL_ARCH_90 )
   RELEASE ADD(FSBP_MM_RFLOG_ARCH_90 )
ENDJOB

SAP_JOB FSBP_MM_PANEL_ARCH_90
   AGENT SAP_BZP100
   SAPJOBNAME MM_PANEL_ARCH_90
   SAPJOBCLASS C
   SAPTARGETSYSTEM fbpl09v006_BZP_20
   STARTMODE ASAP
   ABAPNAME ZMMR_PANEL_TBL_ARCH
   VARIANT ZV_PANELARC_90
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTSPOOLNAME ZMMR_PANEL_T
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT PALLECHANDRA
   PRINTDEPARTMENT CONTRACTOR
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   EARLYSUB 00.30
ENDJOB

SAP_JOB FSBP_MM_RFLOG_ARCH_90
   AGENT SAP_BZP100
   SAPJOBNAME MM_RFLOG_ARCH_90
   SAPJOBCLASS C
   SAPTARGETSYSTEM fbpl09v006_BZP_20
   STARTMODE ASAP
   ABAPNAME ZMMR_RF_ERRORLOG_ARCH
   VARIANT ZMM_RFLOGARCH
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMMR_RF_ERRO
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT PALLECHANDRA
   PRINTDEPARTMENT CONTRACTOR
   ARCMODE PRINT
   RUN DAILY
   EARLYSUB 00.30
ENDJOB


