JOB RT950N87
  /*@@ SYSTEM=RTNITE02
  RUN WEEKDAYS
  RUN FEB 28 2010
  RELEASE ADD(RT955N87)
/*@@ ADD CONDITION CODE START @@*/
  CCCHK RC(201:206) STEP(SPLIT) OK CONTINUE
  CCCHK RC(201:206) PROC(SPLIT) OK CONTINUE
  CCCHK RC(0:200) STEP(SPLIT) FAIL CONTINUE
  CCCHK RC(0:200) PROC(SPLIT) FAIL CONTINUE
  CCCHK RC(207:4095) STEP(SPLIT) FAIL CONTINUE
  CCCHK RC(207:4095) PROC(SPLIT) FAIL CONTINUE
  CCCHK RC(1:4095) FAIL CONTINUE
/*@@ ADD CONDITION CODE END   @@*/
ENDJOB

