JOB LIS.!ESPAPPL LINK PROCESS
   RUN TODAY
   EARLYSUB 22.00
   RELEASE (ZMM_INACTIVE_VENDOR_REPORT_BFP,+
ZMM_INACTIVE_VENDOR_REPORT_BZP,ZMM_INACTIVE_VENDOR_REPORT_IZP)
ENDJOB

SAP_JOB ZMM_INACTIVE_VENDOR_REPORT_BFP
   AGENT CYBB_BFP030
   SAPJOBNAME ZMM_INACTIVE_VENDOR_REPORT
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZMM_INACTIVE_VENDORS
   VARIANT BFP_INACT_VEND
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMM_INACTIVE
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN TODAY
   DELAYSUB 23.00
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

