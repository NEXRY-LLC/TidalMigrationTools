APPL GRMIPAUD WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB GRMIPAUD LINK PROCESS
   RUN 1ST DAY OF MONTH
   EARLYSUB 04.00
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SE38_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_DEBUG_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SU01_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_DEVLPR_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_ALL_TCD_USER)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_OS_CMND_USER)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_WE19_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_RZ10_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_ASYS_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SAP_ROLE_USR)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SUSERGRP_USR)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SUSERAGR_USR)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SM01_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SM50_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SM04_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SM12_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SA38_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SM59_USERS)
   RELEASE ADD(ZRSUSR002_SEC_AUDIT_SM13_USERS)
ENDJOB

SAP_JOB ZRSUSR002_SEC_AUDIT_SE38_USERS
   AGENT SAP_IZP100
   SAPJOBNAME ZRSUSR002_SEC_AUDIT_SE38_USERS
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RSUSR002
   VARIANT ZSEC_AUDT_SE38
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 190
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   PRINTCOPIES 001
   SAPEMAILADDR 'sap_security@bfusa.com'
   RUN 1ST DAY OF MONTH
   EARLYSUB 04.10
ENDJOB

