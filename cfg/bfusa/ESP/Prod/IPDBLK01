NOTIFY ABEND FAILURE ALERT(REMD)
NOTIFY OVERDUE ALERT(LATE)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   EARLYSUB 23.00
   RELEASE (BLACKLIN_SGL_EXT_IP_GL_BL)
ENDJOB

SAP_JOB BLACKLIN_SGL_EXT_IP_GL_BL
   AGENT SAP_IZP100
   SAPJOBNAME BLACKLIN_SGL_EXT_IP_GL_BL
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME /BLACKLIN/SGL_EXTRACT
   VARIANT 'IP GL BL'
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTDEST P060
   PRINTCOPIES 001
   PRINTSPOOLNAME /BLACKLIN/SG
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   RUN DAILY
   DELAYSUB 00.01
   RELEASE (BLACKLIN_SAR_EXT_IP_AR_SL)
ENDJOB

