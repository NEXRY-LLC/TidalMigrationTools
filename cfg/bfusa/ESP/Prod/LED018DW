APPL LED018DW WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*09/05/2018 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES
/*09/05/2018 -JFC- REMOVED GARY FEE FROM SAP SPOOL RECIPIENT
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)
TAG 'OTC CRITICAL'

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   RELEASE ADD(ZDELVWRAPSPLIT_DC_WST)
   RELEASE ADD(ZSDLEI06_SHP_BLK_REASON_RMV_DW)
ENDJOB

SAP_JOB ZDELVWRAPSPLIT_DC_WST
   AGENT CYBB_BFP030
   SAPJOBNAME ZDELVWRAPSPLIT_DC_WST
   SAPJOBCLASS A
   STARTMODE ASAP
   ABAPNAME ZDELVWRAPSPLIT
   VARIANT ZLESO_DC_WEST
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   PRINTCOPIES 001
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   RUN DAILY
   NOTWITH (ZSDLEI06_SHP_BLK_REASON_RMV_BLK.-)
   DUEOUT EXEC REALNOW PLUS 20 MINUTES
   NOTIFY OVERDUE MAILBOX(LED018XX)
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

SAP_JOB ZSDLEI06_SHP_BLK_REASON_RMV_DW
   AGENT CYBB_BFP030
   SAPJOBNAME ZSDLEI06_SHP_BLK_REASON_RMV_DW
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSDLEI06_SHP_BLK_REASON
   VARIANT ZLESO_DC_WEST
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
   DUEOUT EXEC NOW PLUS 1 HOUR
ENDJOB

