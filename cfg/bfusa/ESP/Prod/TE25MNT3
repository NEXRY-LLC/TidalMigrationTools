APPL TE25MNT3 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/********************************************************************
/* 02/20/19 -DSG- CHANGED ZOS JOBS TO LIS LINK PROCESSES
/* 04/29/19 -JFC- REARRANGED JOBS TO BETTER ILLUSTRATE FILE PATHS
/********************************************************************

NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN 8TH DAY OF JAN APR JUL OCT
   RUN 28TH DAY OF JAN APR JUL OCT
   EARLYSUB 09.00
   RELEASE ADD(COPYFILE.MEDQT1)
   RELEASE ADD(COPYFILE.MEDQT2)
   RELEASE ADD(COPYFILE.MEDQT2A)
   RELEASE ADD(COPYFILE.MEDQT2B)
ENDJOB

FILE_TRIGGER NGHPQO.INBOUND
    AGENT CYBA_ISHRP1
    FILENAME '/data/HRP/interfaces/output/dataout/+
PCOB.BA.MR.NGHPQO.R0011073.TXT' CREATE NOCHANGE(1)
   RUN 8TH DAY OF JAN APR JUL OCT
   RELEASE ADD(COPYFILE.MEDQT2)
ENDJOB

AIX_JOB COPYFILE.MEDQT2
   AGENT CYBA_ISHRP1
   CMDNAME /usr/bin/cp
   ARGS +
/data/HRP/interfaces/output/dataout/OPCOB.BA.MR.NGHPQO.R0011073.TXT +
/data/HRP/interfaces/output/dataout/SPCOB.BA.MR.NGHPQO.R0011073.TXT
   USER root
   RUN 8TH DAY OF JAN APR JUL OCT
   RELEASE ADD(ARCFILE.MEDQTB)
ENDJOB

AIX_JOB ARCFILE.MEDQTB REQUEST
   AGENT CYBA_ISHRP1
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/output/dataout/PCOB.BA.MR.NGHPQO.R0011073.TXT W
   USER hrpadm
   RUN 8TH DAY JAN APR JUL OCT
   RELEASE ADD(COPYFILE.PCOBPQO)
ENDJOB

AIX_JOB COPYFILE.PCOBPQO
   AGENT CYBA_ISHRP1
   USER root
   SCRIPTNAME /local/bin/scp2_pgp2akfis.sh
   ARGS /data/HRP1/interfaces/output/dataout/ +
PCOB.BA.MR.NGHPQO.R0011073.TXT
   RUN 8TH DAY OF JAN APR JUL OCT
   DELAYSUB 15:00
   RELEASE ADD(SENDPCOB.PQO2PGP2)
ENDJOB

LINUX_JOB SENDPCOB.PQO2PGP2
   AGENT CYBA_PGP2AKFIS
   SCRIPTNAME /usr/local/scripts/sendpgpfile.sh
   USER ceadmin
   ARGS PCOB.BA.MR.NGHPQO.R0011073.TXT
   RUN 8TH DAY OF JAN APR JUL OCT
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

