APPL QABRAND WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW

NT_JOB INTERFACE_1061 EXTERNAL APPLID(SSISPPIW) SCOPE(-10)
   RUN DAILY
   NORUN JUL 24 2014
   RELEASE ADD(LIS.QABRAND)
ENDJOB

JOB LIS.QABRAND LINK PROCESS
   RUN DAILY
   RELEASE ADD(BRANDMASTERCMD)
ENDJOB

NT_JOB BRANDMASTERCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\brandmaster.cmd
   ARGS
   USER maestro
   RUN DAILY
   EARLYSUB 01.00
   RELEASE ADD(PPIWBRAND_MASTER_EXPORTCMD)
ENDJOB

NT_JOB PPIWBRAND_MASTER_EXPORTCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\ppiw_brand_master_export.cmd
   ARGS
   USER maestro
   RUN DAILY
   RELEASE ADD(PPIWBRAND_MASTER_EXPORT_CTLCMD)
ENDJOB

NT_JOB PPIWBRAND_MASTER_EXPORT_CTLCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\ppiw_brand_master_export_ctl.cmd
   ARGS
   USER maestro
   RUN DAILY
   RELEASE ADD(LIE.QABRAND)
ENDJOB

JOB LIE.QABRAND LINK PROCESS
   RUN DAILY
ENDJOB

