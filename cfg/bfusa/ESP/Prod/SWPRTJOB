ENDJOB

JOB SWKELLY7 EXTERNAL SCOPE(-01:00)
    /*@@ SCHID=001
  RUN !SCHD1
  RELEASE ADD(SWPRTJOB)
ENDJOB

JOB SWPRTJOB
  OPTIONS NORESTARTSTEP
    /*@@ SCHID=001
