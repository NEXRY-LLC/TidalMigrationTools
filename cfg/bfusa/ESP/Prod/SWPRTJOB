APPL SWPRTJOB WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*02/26/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES    */
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)

SCHD1 = 'YESTERDAY'
IF !USER2 = 1 OR !USER2 = '' THEN SCHD1 = 'TODAY'

JOB SWKELLY1 EXTERNAL SCOPE(-01:00)
    /*@@ SCHID=001
  RUN !SCHD1
  RELEASE ADD(SWPRTJOB)
ENDJOB

