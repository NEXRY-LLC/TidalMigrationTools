APPL MSTR001 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)

JOB WA1560D EXTERNAL SCOPE(-24.00)
   RUN SUN
   RELEASE ADD(LIS.!ESPAPPL)
ENDJOB

JOB WA1565D EXTERNAL SCOPE(-24.00)
   RUN SUN
   RELEASE ADD(LIS.!ESPAPPL)
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
   RUN SUN
   DELAYSUB 00.40
   RELEASE ADD(WA_EGIEWA_ERR_DTL_TMP)
ENDJOB

NT_JOB WA_EGIEWA_ERR_DTL_TMP
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_egiewa_err_dtl_tmp.cmd
   ARGS
   RUN SUN
   USER maestro
   RELEASE ADD(WA_EGIEWA_ERR_HDR_TMP)
ENDJOB

NT_JOB WA_EGIEWA_ERR_HDR_TMP
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_egiewa_err_hdr_tmp.cmd
   ARGS
   RUN SUN
   USER maestro
   RELEASE ADD(WA_EGIEWA_ERR_HDR)
ENDJOB

NT_JOB WA_EGIEWA_ERR_HDR
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_egiewa_err_hdr.cmd
   ARGS
   RUN SUN
   USER maestro
   RELEASE ADD(WA_EGIEWA_ERR_DTL)
ENDJOB

NT_JOB WA_EGIEWA_ERR_DTL
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_egiewa_err_dtl.cmd
   ARGS
   RUN SUN
   USER maestro
   RELEASE ADD(WA_EGIEWA_EXPORT)
ENDJOB

NT_JOB WA_EGIEWA_EXPORT
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\wa_egiewa_export.cmd
   ARGS
   RUN SUN
   USER maestro
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN SUN
ENDJOB

