FILE_TRIGGER BSRO_FILE_FROM_TIVOLI_TO_DSRSAP01PAKR_COMPLETE
   AGENT LNX_DSRSAP01PAKR
   FILENAME '/mnt/bods/hems/prod/outbound/+
bsro_tdw_ncd_adw_????????.done.txt' CREATE NOCHANGE(1)
   RUN DAILY
   RELEASE ADD(DS_NCD_SALES_HDR_DELTA_LOAD)
ENDJOB

LINUX_JOB DS_NCD_SALES_HDR_DELTA_LOAD
