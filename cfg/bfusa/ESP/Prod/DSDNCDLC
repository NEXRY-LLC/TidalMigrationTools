    USER bodsadmin
    SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
    JOB_NCD_SALES_HDR_DELTA_LOAD.sh
    NOTIFY ABEND FAILURE MAILBOX(DSJOBADW)
    RUN DAILY
    RELEASE ADD(DS_FALCON_NCD_DATA_PURGE)
    RELEASE ADD(DS_NCD_SALES_PYMT_DELTA_LOAD)
ENDJOB

