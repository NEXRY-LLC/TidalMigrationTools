JOB LIS.!ESPAPPL LINK PROCESS
   RUN TUESDAY
   DELAYSUB 04.00
   RELEASE ADD(EXP_UID_CANADA)
ENDJOB

FILE_TRIGGER APW031_CONCUR_CANADA
   AGENT CYBA_ISBFP3
   FILENAME /data/BFP/interfaces/input/datain/ZFIAPI14CONCUREXPCA.dat
   RUN TUESDAY
   RELEASE ADD(EXP_UID_CANADA)
ENDJOB

SAP_JOB EXP_UID_CANADA
   AGENT CYBB_BFP030
   SAPJOBNAME EXP_UID_CANADA
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIAPI20
   VARIANT EXP-UID-CANADA
   STEPUSER SAPADM
   LANGUAGE E
   PRINTREL YES
   EXPIRATION 8
   COLUMNS 170
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN TUESDAY
   RELEASE ADD(EXP_CONCUR_CA)
ENDJOB

