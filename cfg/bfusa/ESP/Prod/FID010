APPL FID010 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*Clear OIM Accounts for BFCA and BFS1*/
/*04/01/2019 -JFC- CLEANED UP PROC - ADDED LIE - NO OTHER CHANGES
/*********************************************************************/

NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN TUE, WED, THU, FRI, SAT
   EARLYSUB 05.30
   RELEASE ADD(SAPF124_CLR_OIM_ACTG_1)
ENDJOB

SAP_JOB SAPF124_CLR_OIM_ACTG_1
   AGENT CYBB_BFP030
   SAPJOBNAME SAPF124_CLR_OIM_ACTG_1
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME SAPF124
   VARIANT 'CLR OIM ACTG 1'
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 58
   COLUMNS 153
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   RUN TUE, WED, THU, FRI, SAT
   RELEASE (SAPF124_CLR_OIM_ACTG_2)
ENDJOB

