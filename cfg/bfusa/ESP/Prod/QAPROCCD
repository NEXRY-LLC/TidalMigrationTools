APPL QAPROCCD WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW

JOB WA2275S EXTERNAL SCOPE(-48.00)
  RUN THU SUN
  RELEASE ADD(LIS.QAPROCCD)
ENDJOB

JOB LIS.QAPROCCD LINK PROCESS
  RUN THU SUN
  DELAYSUB 01.00
  RELEASE ADD(PROCES_CODE_XREF)
ENDJOB

NT_JOB PROCES_CODE_XREF
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\process_code_xref.cmd
  ARGS
  USER maestro
  RUN THU SUN
  DUEOUT EXEC 6AM
  RELEASE ADD(LIE.QAPROCCD)
ENDJOB

JOB LIE.QAPROCCD LINK PROCESS
  RUN THU SUN
ENDJOB

