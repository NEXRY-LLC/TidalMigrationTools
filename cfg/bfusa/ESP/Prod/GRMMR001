JOB GRMMR001 LINK PROCESS
  RUN EVERY 14 DAYS STARTING AUG 14 2017
  DELAYSUB 06:00
  RELEASE ADD(ME51N_USERS_BP)
  RELEASE ADD(ME51N_USERS_IP)
  RELEASE ADD(ME51N_USERS)
ENDJOB

SAP_JOB ME51N_USERS_BP
   AGENT SAP_BZP100
   SAPJOBNAME ME51N_USERS_BP
   RECIPIENTTYPE MAIL
   SPOOLRECIPIENT 'MROpartsadministration@bfusa.com'
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RSUSR002
   VARIANT ZPR_ME51N
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTSPOOLNAME RSUSR002_S_B
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 190
   PRINTFORMAT X_PAPER
   RECIPIENT S_BC_CYBERM
   ARCMODE PRINT
   PRINTCOPIES 001
   BANNER D
/* RUN EVERY 14 DAYS STARTING AUG 14 2017
   DELAYSUB 06:00
ENDJOB

