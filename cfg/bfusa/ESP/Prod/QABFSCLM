APPL QABFSCLM WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB V_PD_CLAIMS_NOT_CLOSED_CHECKSCMD EXTERNAL APPLID(QAPDCCHK)
  RUN SUN
  RELEASE ADD(BFSCLAIMSCMD)
ENDJOB

NT_JOB LOIS_UNIVERSE_EXTRACTCMD EXTERNAL APPLID(QALOIS)
  RUN SUN
  RELEASE ADD(BFSCLAIMSCMD)
ENDJOB

JOB LIS.QABFSCLM LINK PROCESS
  RUN SUN
  DELAYSUB 01.00
  RELEASE ADD(BFSCLAIMSCMD)
ENDJOB

NT_JOB BFSCLAIMSCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\bfsclaims.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(BFSCLAIMS_LAWSUITCMD)
ENDJOB

NT_JOB BFSCLAIMS_LAWSUITCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\bfsclaims_lawsuit.cmd
  ARGS
  USER maestro
  RUN SUN
  RELEASE ADD(LIE.QABFSCLM)
ENDJOB

JOB LIE.QABFSCLM LINK PROCESS
  RUN SUN
ENDJOB

