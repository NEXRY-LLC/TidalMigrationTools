APPL QABGROUP WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW
CRITPATH ON

NT_JOB INTERFACE_1062 EXTERNAL APPLID(SSISPPIW) SCOPE(-10)
   RUN DAILY
   NORUN JUL 24 2014
   RELEASE ADD(LIS.QABGROUP)
ENDJOB

JOB LIS.QABGROUP LINK PROCESS
  RUN DAILY
  RELEASE ADD(B_GROUP_XREFCMD)
ENDJOB

NT_JOB B_GROUP_XREFCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\b_group_xref.cmd
  ARGS
  USER maestro
  RUN DAILY
  DELAYSUB 01.00
  RELEASE ADD(LIE.QABGROUP)
ENDJOB

JOB LIE.QABGROUP LINK PROCESS
  RUN DAILY
ENDJOB

