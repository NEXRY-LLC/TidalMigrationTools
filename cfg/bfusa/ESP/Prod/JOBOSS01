APPL JOBOSS01 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/* 05/20/2016 -JFC- TAGGED APPL AS BOSS
/* 02/25/2020 -DSG- UPDATED AGENT FROM R7 TO R11.5.0
/*                - CHANGED JOBQ FROM CYBESPV210.CYBESPV2100
/* 03/05/2020 -DSG- ADDED RESOURCE TO AS400 JOBS - INC000002178387
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
TAG BOSS

AIX_JOB LIE.JOPED150 EXTERNAL APPLID(JOPED150) SCOPE(-00)
  RUN DAILY
  NORUN MAY 23 2015
  NORUN MAY 24 2015
  NORUN MAY 25 2015
  RELEASE ADD(LIS.!ESPAPPL)
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
  TAG MAJORAPPS
  RUN DAILY
  RELEASE ADD(JOMS_BOSS)
ENDJOB

AS400_JOB JOMS_BOSS
  RESOURCE ADD(1,AKNITS)
  AGENT AKNITS
  COMMAND CYBROBOT JOMS_BOSS
  JOBQ CAWAAGENT.CYBESPJOBS
  RUN DAILY
  RELEASE ADD(JOMS_BOSSC)
ENDJOB

AS400_JOB JOMS_BOSSC
  RESOURCE ADD(1,AKNITS)
  AGENT AKNITS
  COMMAND CYBROBOT JOMS_BOSSC
  JOBQ CAWAAGENT.CYBESPJOBS
  RUN DAILY
  RELEASE ADD(MTIC_LOADBAT)
ENDJOB

NT_JOB MTIC_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\DTS_RUN\BatchFiles\BOSS\MticLoad\MTIC_LOAD.bat
  ARGS
  USER maestro
  RUN DAILY
    NOTWITH (MTIC_LOADBAT/AIBOSS01)
    NOTWITH (MTIC_LOADBAT/AOBOSS01)
    NOTWITH (MTIC_LOADBAT/BLBOSS01)
    NOTWITH (MTIC_LOADBAT/DMBOSS01)
    NOTWITH (MTIC_LOADBAT/LVBOSS01)
    NOTWITH (MTIC_LOADBAT/WGBOSS01)
    NOTWITH (MTIC_LOADBAT/WNBOSS01)
  RELEASE ADD(BOSSSAP_CD_LOADBAT)
ENDJOB

NT_JOB BOSSSAP_CD_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_RUN\batchfiles\boss\SapLoad\BOSSSAP_CD_LOAD.BAT
  ARGS
  USER maestro
  RUN DAILY
    NOTWITH BOSSSAP_81_LOADBAT
    NOTWITH BOSSSAP_AO_LOADBAT
    NOTWITH BOSSSAP_BL_LOADBAT
    NOTWITH BOSSSAP_88_LOADBAT
    NOTWITH BOSSSAP_LV_LOADBAT
    NOTWITH BOSSSAP_8H_LOADBAT
    NOTWITH BOSSSAP_WN_LOADBAT
  RELEASE ADD(BOSSCUREBUILD_CD_LOADBAT)
ENDJOB

NT_JOB BOSSCUREBUILD_CD_LOADBAT
  AGENT CYBA_CDC1AKFIS
CMDNAME D:\dts_RUN\batchfiles\boss\CureLoad\BOSSCureBuild_CD_Load.bat
  ARGS
  USER maestro
  RUN DAILY
    NOTWITH BOSSCUREBUILD_81_LOADBAT
    NOTWITH BOSSCUREBUILD_AO_LOADBAT
    NOTWITH BOSSCUREBUILD_BL_LOADBAT
    NOTWITH BOSSCUREBUILD_88_LOADBAT
    NOTWITH BOSSCUREBUILD_LV_LOADBAT
    NOTWITH BOSSCUREBUILD_8H_LOADBAT
    NOTWITH BOSSCUREBUILD_WN_LOADBAT
  RELEASE ADD(BOSSCOMP_CD_LOADBAT)
ENDJOB

NT_JOB BOSSCOMP_CD_LOADBAT
  AGENT CYBA_CDC1AKFIS
  CMDNAME D:\dts_RUN\batchfiles\boss\CompLoad\BOSSComp_CD_Load.bat
  ARGS
   NOTIFY OVERDUE MAILBOX(JOBOSS1)
   NOTIFY FAILURE ABEND ALERT(REMD)
   IF ESPSHH = '13' THEN DUEOUT EXEC 4pm
   IF ESPSHH = '19' THEN DUEOUT EXEC 4AM
  USER maestro
  RUN DAILY
    NOTWITH BOSSCOMP_81_LOADBAT
    NOTWITH BOSSCOMP_AO_LOADBAT
    NOTWITH BOSSCOMP_BL_LOADBAT
    NOTWITH BOSSCOMP_88_LOADBAT
    NOTWITH BOSSCOMP_LV_LOADBAT
    NOTWITH BOSSCOMP_8H_LOADBAT
    NOTWITH BOSSCOMP_WN_LOADBAT
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

AIX_JOB LIE.!ESPAPPL
  AGENT CYBA_ISBFP3
  COMMAND /bin/sleep
  ARGS 3
  USER bfpadm
   TAG MAJORAPPS
  RUN DAILY
  NORUN MAY 23 2015
  NORUN MAY 24 2015
  NORUN MAY 25 2015
ENDJOB

