ENDJOB

JOB FCD500
   CCCHK RC(1:4095) FAIL CONTINUE
   RUN WEEKDAYS
   NORUN HOLIDAYS
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

SAP_JOB ZFIGLI11_TREASURY_INBOUND
  AGENT CYBB_BFP030
  SAPJOBNAME ZFIGLI11_TREASURY_INBOUND
  SAPJOBCLASS C
  STARTMODE ASAP
  ABAPNAME ZFIGLI11
  VARIANT TREASURY_FILE
  STEPUSER SAPADM
  LANGUAGE E
  PRINTDEST CP21
  EXPIRATION 8
  LINES 65
  COLUMNS 91
  BANNER D
  RECIPIENT SAPADM
  ARCMODE PRINT
  PRINTCOPIES 001
  RUN WEEKDAYS
  NORUN HOLIDAYS
  RELEASE ADD(ARCFILE.FCD180)
ENDJOB

UNIX_JOB ARCFILE.FCD180
   AGENT CYBA_ISBFP3
   SCRIPTNAME /local/bin/arcfile.sh
