SAP_JOB ZFIGLI64_TIPS_ACCR_BL
   AGENT CYBB_BFP030
   SAPJOBNAME ZFIGLI64_TIPS_ACCR_BL
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIGLI64
   VARIANT 'BL TIPS ACCR'
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT LESSMILLERLE
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN 2ND WORKDAY OF MONTH
   NORUN HOLIDAY
   RELEASE ADD(ARCHIVE_TIPS_ACCR_BL)
ENDJOB

UNIX_JOB ARCHIVE_TIPS_ACCR_BL
   AGENT CYBA_ISBFP3
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS interfaces/input/datain/ZFIGLI64_BL_TIPS_Accrual_Out.txt M
   USER bfpadm
   RUN 2ND WORKDAY OF MONTH
   NORUN HOLIDAY
ENDJOB


