   /*RUN DAILY
   RELEASE ADD(FSBP_PP_MRPNETCH_EUR_SUNDAY8PM)
   RELEASE ADD(FSBP_PP_MRPNETCH_EUR_MONDAY8PM)
   RELEASE ADD(FSBP_PP_MRPNETCH_EUR_TUESDAY8PM)
   RELEASE ADD(FSBP_PP_MRPNETCH_EUR_WEDNESDAY8PM)
   RELEASE ADD(FSBP_PP_MRPNETCH_EUR_THURSDAY8PM)
   RELEASE ADD(FSBP_PP_MRPNETCH_EUR_FRIDAY8PM)
   RELEASE ADD(FSBP_PP_MRPNEUPL_EUR_SATURDAY8PM)
ENDJOB

