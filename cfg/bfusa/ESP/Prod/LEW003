NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   DELAYSUB 10.00
   RUN MON
   RELEASE ADD(JOB ZMMLEI02_CTSI_INVOICE_PAYMENT)
ENDJOB

SAP_JOB ZMMLEI02_CTSI_INVOICE_PAYMENT
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMLEI02_CTSI_INVOICE_PAYMENT
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZFIAP_CTSI_INV_VER
   VARIANT CTSIPAYMENT
   STEPUSER SAPADM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   ARCMODE PRINT
   RUN MON
   RELEASE (RSEOUT00_CTSI_INVOICE_PAYMENT)
ENDJOB

SAP_JOB RSEOUT00_CTSI_INVOICE_PAYMENT
   AGENT CYBB_BFP030
   SAPJOBNAME RSEOUT00_CTSI_INVOICE_PAYMENT
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RSEOUT00
   VARIANT CTSIPAYMENT
   STEPUSER SAPADM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   ARCMODE PRINT
   RELDELAY 5
   RUN MON
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN MON
ENDJOB

