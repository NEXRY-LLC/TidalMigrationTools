APPL QACTRORG WAIT

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
TAG PPIW

JOB LIS.QACTRORG LINK PROCESS
   RUN DAILY
   RELEASE ADD(COUNTRYOFORIGINCMD)
ENDJOB

NT_JOB COUNTRYOFORIGINCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\countryoforigin.cmd
   ARGS
   RUN DAILY
   EARLYSUB 01.00
   USER maestro
   RELEASE ADD(COUNTRY_OF_ORIGIN_MASTER_CHECKSCMD)
ENDJOB

NT_JOB COUNTRY_OF_ORIGIN_MASTER_CHECKSCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\country_of_origin_master_checks.cmd
   ARGS
   RUN DAILY
   USER maestro
   RELEASE ADD(PPIWCOUNTRY_MASTER_EXPORTCMD)
ENDJOB

NT_JOB PPIWCOUNTRY_MASTER_EXPORTCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\ppiw_country_master_export.cmd
   ARGS
   RUN DAILY
   USER maestro
   RELEASE ADD(PPIWCOUNTRY_MASTER_EXPORT_CTLCMD)
ENDJOB

NT_JOB PPIWCOUNTRY_MASTER_EXPORT_CTLCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\ppiw_country_master_export_ctl.cmd
   ARGS
   RUN DAILY
   USER maestro
   RELEASE ADD(LIE.QACTRORG)
ENDJOB

JOB LIE.QACTRORG LINK PROCESS
   RUN DAILY
ENDJOB

