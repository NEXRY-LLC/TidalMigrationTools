NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   DELAYSUB 00.05
   RELEASE ADD(FSBP_SD_BILLINGRUN_USA_DAILY_1)
ENDJOB

SAP_JOB FSBP_SD_BILLINGRUN_USA_DAILY_1
   AGENT SAP_BZP100
   SAPJOBNAME SD_BILLINGRUN_USA_DAILY_1
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RV60SBAT
   VARIANT INV_NA_SO_F5
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME RV60SBAT_FSB
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN DAILY
   RELEASE (FSBP_SD_BILLINGRUN_USA_DAILY_2)
ENDJOB

