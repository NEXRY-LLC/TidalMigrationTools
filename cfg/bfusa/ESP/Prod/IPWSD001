APPL IPWSD001 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB FSIP_IPWSD001_WEEKLY_FRI_6PM LINK PROCESS
   RUN FRIDAY
   EARLYSUB 18.00
   RELEASE ADD(FSIP_SD_SALESFORCE_CUST_DATA_EXTRACT)
ENDJOB

SAP_JOB FSIP_SD_SALESFORCE_CUST_DATA_EXTRACT
   AGENT SAP_IZP100
   SAPJOBNAME SD_SALESFORCE_CUST_DATA_EXTRACT
   SAPJOBCLASS C
   ABAPNAME ZSDI_CUST_EXTRACT
   VARIANT DATA_EXTRACT
   STEPUSER FSIP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT BFDPBATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME ZSDI_CUST_EX
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
   STARTMODE ASAP
   RUN FRIDAY
ENDJOB


