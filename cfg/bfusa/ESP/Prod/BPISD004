APPL BPISD004 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

SAP_JOB FSBP_SD_TMS2SAP_INBOUND2_CPU_DAILY
   AGENT SAP_BZP100
   SAPJOBNAME SD_TMS2SAP_INBOUND2_CPU_DAILY
   SAPJOBCLASS C
   ABAPNAME ZSDI_FRT_ALLOWANCE_ERR2IN
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT FSBP_BATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME ZSDI_FRT_ALL
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
   STARTMODE ASAP
   ABAPNAME ZSDI_OUT_DELIVERY_PRICING_COND
   VARIANT TMS_CPU
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTIMMED YES
   PRINTREL YES
   ARCMODE PRINT
   BANNER D
   RECIPIENT FSBP_BATCH
   LINES 65
   COLUMNS 80
   EXPIRATION 8
   PRINTNEWSPOOL YES
   PRINTSPOOLNAME ZSDI_OUT_DEL
   PRINTFORMAT X_65_80
   PRINTCOPIES 001
   RUN DAILY
   DUEOUT EXEC REALNOW PLUS 20 MINUTES
   NOTIFY OVERDUE MAILBOX(AKRONJOBREQ)
ENDJOB


