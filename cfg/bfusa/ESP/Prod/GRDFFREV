APPL GRDFFREV WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'


JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   RELEASE ADD(GRC_EAM_WORKFLOW_SYNC_BFPCLNT030)
ENDJOB


SAP_JOB GRC_EAM_WORKFLOW_SYNC_BFPCLNT030
   AGENT CYBB_BGP030
   SAPJOBNAME GRC_EAM_WORKFLOW_SYNC_BFPCLNT030
   SAPJOBCLASS C
   PRINTCOPIES 001
   STARTMODE ASAP
   ABAPNAME GRAC_SPM_WORKFLOW_SYNC
   VARIANT BFPCLNT030
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTSPOOLNAME GRAC_SPM_WOR
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   RECIPIENT S_BC_CYBERM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   BANNER D
   RUN DAILY
   RELEASE ADD(GRC_EAM_WORKFLOW_SYNC_PLACLNT400)
ENDJOB

