APPL QACOSHIP WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/* 11/21/2016 -JFC- REMOVED CDC1 CONNECT DIRECT JOBS PER J. HRITZ     */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY OVERDUE MAILBOX(SQLSHAREDSERVICES)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL)
TAG PPIW

JOB LIS.QACOSHIP LINK PROCESS
  RUN 2ND WORKDAY OF MONTH
  DELAYSUB 11.30
  RELEASE ADD(DTS_CONTROL_SHIPMENTS_COCMD)
ENDJOB

NT_JOB DTS_CONTROL_SHIPMENTS_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\dts_control_shipments_co.cmd
   ARGS 6
   USER maestro
   RUN 2ND WORKDAY OF MONTH
   RELEASE ADD(SHIPMENTS_COCMD)
ENDJOB

NT_JOB SHIPMENTS_COCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\shipments_co.cmd
   ARGS
   USER maestro
   RUN 2ND WORKDAY OF MONTH
   RELEASE ADD(SHIPMENTS_CO_CHECKSCMD)
ENDJOB

NT_JOB SHIPMENTS_CO_CHECKSCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\shipments_co_checks.cmd
   ARGS
   USER maestro
   RUN 2ND WORKDAY OF MONTH
   RELEASE ADD(LIE.QACOSHIP)
ENDJOB

JOB LIE.QACOSHIP LINK PROCESS
   RUN 2ND WORKDAY OF MONTH
   DUEOUT EXEC 16:00
ENDJOB

