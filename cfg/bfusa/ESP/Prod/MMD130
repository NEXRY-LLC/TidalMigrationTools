APPL MMD130 WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*Extract Kings Mtn daily                                            */
/*********************************************************************/
/*05/09/2018 -JFC- UPDATED PROC AND CHANGED EVENT PREFIX FROM OP01   */
/*02/13/2019 -JFC- CHANGED SAP PRINDEST PER INC000001851980          */
/*03/27/2019 -JFC- changes made per INC000001858289                  */
/*********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)

DATA_OBJECT PARMSET.!ESPAPPL
   /*Create a timestamp in hhmmMMDDYY format*/
   SETVAR TMSTMP='!ESPSHH.:!ESPSMN.,!ESPSMM./!ESPSDD./!ESPSYY'
   SETVAR PID=''
   SETVAR STMPTM='!ESPSMM./!ESPSDD./!ESPSYY.,!ESPSHH.:!ESPSMN'
ENDJOB

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   DELAYSUB 21:00
   IF TODAY('SUN') THEN DELAYSUB 19.00
   RELEASE ADD(ZMMPO108_KINGS_MTN_PO_EXT)
ENDJOB

SAP_JOB ZMMPO108_KINGS_MTN_PO_EXT
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMPOI08_KINGS_MTN_PO_EXT
   SAPJOBCLASS C
   SAPUSER SAPADM
   ABAPNAME ZMMPOI08
   VARIANT ZMMPOI08_PO
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   RELEASE ADD(ZMMPOI08POCD)
ENDJOB

JOB SETPID.!ESPAPPL LINK PROCESS
   /*get the next PID number from the ConnectDirect*/
   /*Process ID table (CDPROD).  Save the value into*/
   /*the applications DataObject for use by the C:D*/
   /*job and the CDSTAT job (if needed).*/
   VINCR (PID) TABLE(CDPROD) CLANG
   VGET  (PID) TABLE(CDPROD) CLANG
   ESP MGRMSG * . . . PARMSET.!ESPAPPL/!ESPAPPL..!ESPAPGEN/MAIN +
   ACTION SET PID(!PID)
   RUN DAILY
   RELEASE ADD(ZMMPOI08POCD)
ENDJOB

UNIX_JOB ZMMPOI08POCD
   AGENT CYBA_ISBFP3
   USER cdprod
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   TMSTMP= WOBDATA('PARMSET.!ESPAPPL','TMSTMP')
   SCRIPTNAME /apps/condir/bfsbin/submitcd.sh
   ARGS zmmpoi08po.cd 5 !PID !TMSTMP
   RESOURCE ADD(1,ISBFP3CD)
   RUN DAILY
   RELEASE ADD(CDSTAT.ZMPOIZ14(A))
   RELEASE ADD(ZMMPO104_ZEXTRACTDATE_UPD_08PO)
ENDJOB

AIX_JOB CDSTAT.ZMPOIZ14 CONDITIONAL
   /*This job only runs if the C:D job does not end with*/
   /*a EXITCODE of 0.  It can be rerun as often as*/
   /*required to check the status of the C:D process.*/
   /*When the C:D process finishes, complete the job*/
   /*to release the successors.*/
   AGENT CYBA_ISBFP3
   USER cduser
   STMPTM= WOBDATA('PARMSET.!ESPAPPL','STMPTM')
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   CMDNAME /apps/condir/bfsbin/cdstat.sh
   ARGS !STMPTM  j!PID
   RUN DAILY
ENDJOB

SAP_JOB ZMMPO104_ZEXTRACTDATE_UPD_08PO
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMPOI04_ZEXTRACTDATE_UPD_08PO
   SAPJOBCLASS C
   SAPUSER SAPADM
   ABAPNAME ZMMPOI04
   VARIANT ZMMPOI08PO
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   DUEOUT EXEC 23.55
   RELEASE ADD(ZMMPOI08_KINGS_MTN_POC_EXT)
ENDJOB

SAP_JOB ZMMPOI08_KINGS_MTN_POC_EXT
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMPOI08_KINGS_MTN_POC_EXT
   SAPJOBCLASS C
   SAPUSER SAPADM
   ABAPNAME ZMMPOI08
   VARIANT ZMMPOI08_POC
   LANGUAGE E
   PRINTDEST HI45
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   RELEASE ADD(ZMMPOI08POCCD)
ENDJOB

JOB SETPIDA.!ESPAPPL LINK PROCESS
   /*get the next PID number from the ConnectDirect*/
   /*Process ID table (CDPROD).  Save the value into*/
   /*the applications DataObject for use by the C:D*/
   /*job and the CDSTAT job (if needed).*/
   VINCR (PID) TABLE(CDPROD) CLANG
   VGET  (PID) TABLE(CDPROD) CLANG
   ESP MGRMSG * . . . PARMSET.!ESPAPPL/!ESPAPPL..!ESPAPGEN/MAIN +
ACTION SET PID(!PID)
   RUN DAILY
   RELEASE ADD(ZMMPOI08POCCD)
ENDJOB

UNIX_JOB ZMMPOI08POCCD
   AGENT CYBA_ISBFP3
   USER cdprod
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   TMSTMP= WOBDATA('PARMSET.!ESPAPPL','TMSTMP')
   SCRIPTNAME /apps/condir/bfsbin/submitcd.sh
   ARGS zmmpoi08poc.cd 5 !PID !TMSTMP
   RESOURCE ADD(1,ISBFP3CD)
   RUN DAILY
   RELEASE ADD(CDSTAT.ZMPOIZ13(A))
   RELEASE ADD(ZMMPOI04_ZEXTRACTDATE_UPD_08POC)
ENDJOB

AIX_JOB CDSTAT.ZMPOIZ13 CONDITIONAL
   /*This job only runs if the C:D job does not end with*/
   /*a EXITCODE of 0.  It can be rerun as often as*/
   /*required to check the status of the C:D process.*/
   /*When the C:D process finishes, complete the job*/
   /*to release the successors.*/
   AGENT CYBA_ISBFP3
   USER cduser
   STMPTM= WOBDATA('PARMSET.!ESPAPPL','STMPTM')
   PID= WOBDATA('PARMSET.!ESPAPPL','PID')
   CMDNAME /apps/condir/bfsbin/cdstat.sh
   ARGS !STMPTM  j!PID
   RUN DAILY
ENDJOB

SAP_JOB ZMMPOI04_ZEXTRACTDATE_UPD_08POC
   AGENT CYBB_BFP030
   SAPJOBNAME ZMMPOI04_ZEXTRACTDATE_UPD_08POC
   SAPJOBCLASS C
   SAPUSER SAPADM
   ABAPNAME ZMMPOI04
   VARIANT ZMMPOI08POC
   LANGUAGE E
   PRINTDEST CP21
   EXPIRATION 8
   COLUMNS 132
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN DAILY
   DUEOUT EXEC 23.55
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
ENDJOB

