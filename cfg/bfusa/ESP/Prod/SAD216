NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN WEEKDAYS
   IF ESPSHH = '02' THEN DELAYSUB 04.45
   IF ESPSHH = '06' THEN DELAYSUB 07.00
   RELEASE ADD(RBDAPP01_COSTCO_850_DLY_ORDERS)
ENDJOB

SAP_JOB RBDAPP01_COSTCO_850_DLY_ORDERS
   AGENT CYBB_BFP030
   SAPJOBNAME RBDAPP01_COSTCO_850_DLY_ORDERS
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME RBDAPP01
   VARIANT COSTCO_850_DLY
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST LP01
   EXPIRATION 8
   LINES 65
   COLUMNS 152
   BANNER D
   RECIPIENT SAPADM
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN WEEKDAYS
   RELEASE ADD(RBDAPP01_WALMART_850_DLY_ORDERS)
ENDJOB

