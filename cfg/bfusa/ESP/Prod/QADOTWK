APPL QADOTWK WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

JOB LIS.QADOTWK LINK PROCESS
   RUN 1ST WORKDAY OF DECEMBER
   DELAYSUB 07.00
   RELEASE ADD(DOT_WEEK_TO_MONTH_XREFCMD)
ENDJOB

NT_JOB DOT_WEEK_TO_MONTH_XREFCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\dot_week_to_month_xref.cmd
   ARGS
   USER maestro
   RUN 1ST WORKDAY OF DECEMBER
   RELEASE ADD(PPIW_DOT_WEEK_TO_MONTH_XREF_EXPORTCMD)
ENDJOB

NT_JOB PPIW_DOT_WEEK_TO_MONTH_XREF_EXPORTCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\ppiw_dot_week_to_month_xref_export.cmd
   ARGS
   USER maestro
   RUN 1ST WORKDAY OF DECEMBER
   RELEASE ADD(PPIW_DOT_WEEK_TO_MONTH_XREF_EXPORT_CTLCMD)
ENDJOB

NT_JOB PPIW_DOT_WEEK_TO_MONTH_XREF_EXPORT_CTLCMD
   AGENT CYBA_PPIWPROD
   CMDNAME X:\cmd_scripts\ppiw_dot_week_to_month_xref_export_ctl.cmd
   ARGS
   USER maestro
   RUN 1ST WORKDAY OF DECEMBER
   RELEASE ADD(LIE.QADOTWK)
ENDJOB

JOB LIE.QADOTWK LINK PROCESS
   RUN 1ST WORKDAY OF DECEMBER
ENDJOB

