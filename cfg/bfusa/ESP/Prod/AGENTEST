APPL AGENTEST WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'


JOB LIS.!ESPAPPL LINK PROCESS
  RUN DAILY
  RELEASE ADD(WA_AGENT_TEST_APPGEN02_DEV_WIN)
  RELEASE ADD(WA_AGENT_TEST_APPGEN02_PRD_WIN)
ENDJOB

NT_JOB WA_AGENT_TEST_APPGEN02_PRD_WIN
  AGENT APPGEN02_PRD_WIN
  CMDNAME "C:\Program Files\CA\WA Agent\test\wa_agent_test.bat"
  ARGS
  RUN DAILY
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

NT_JOB WA_AGENT_TEST_APPGEN02_DEV_WIN
  AGENT APPGEN02_DEV_WIN
  CMDNAME "C:\Program Files\CA\WA Agent\test\wa_agent_test.bat"
  ARGS
  RUN DAILY
  RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
  RUN DAILY
ENDJOB

