NOTIFY ABEND FAILURE ALERT(REMD)

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   IF TODAY('APR 7 2019') AND ESPSHH EQ '17' THEN JOBATTR HOLD
   RELEASE ADD(DS_JDA_IDP_ITEM_BR2)
ENDJOB

LINUX_JOB LIE.JDAOP003 EXTERNAL APPL(JDAOP003)
   RUN DAILY
   RELEASE ADD(DS_JDA_IDP_ITEM_BR2)
ENDJOB

LINUX_JOB DS_JDA_IDP_ITEM_BR2
    AGENT LNX_DSRSAP01PAKR
    USER bodsadmin
    SCRIPTNAME /opt/ds_install/dataservices/JOB_scripts/+
    JOB_JDA_IDP_ITEM_BR2.sh
    RUN DAILY
    IF ESPSHH = "09" THEN DELAYSUB 11.30
    IF ESPSHH = "21" THEN DELAYSUB 23.30
    IF ESPSHH = "17" THEN DELAYSUB 19.00
    INVOKE 'SWS.ESP.APPL.PROCLIB(#DSDJD)'
    NOTIFY OVERDUE ALERT(DS02)
      DUEOUT EXEC NOW PLUS 125 MINUTES
    NOTIFY FAILURE EVENT(SW01.RESUBJOB)
    NOTIFY ABEND FAILURE ALERT(REMD)
    NOTIFY JOBSTART MAILBOX(JDAOP) SUBJECT('Appl DSDJDA01 started')
    RELEASE ADD(DS_JDA_IDP_LOC_BR2)
 ENDJOB

