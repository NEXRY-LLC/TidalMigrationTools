APPL GRDRAINC WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'


JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   EARLYSUB 03.00
   RELEASE ADD(GRC_BATCH_RISK_ANLYSIS_BFP_INC)
ENDJOB

SAP_JOB GRC_BATCH_RISK_ANLYSIS_BFP_INC
   AGENT CYBB_BGP030
   SAPJOBNAME GRC_BATCH_RISK_ANLYSIS_BFP_INC
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME GRAC_BATCH_RISK_ANALYSIS
   VARIANT BFP_INC
   STEPUSER S_BC_CYBERM
   LANGUAGE E
   PRINTSPOOLNAME GRAC_BATCH_R
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   RECIPIENT SAPBATCH
   ARCMODE PRINT
   PRINTCOPIES 001
   BANNER D
   RUN DAILY
   RELEASE ADD(GRC_BATCH_RISK_ANLYSIS_PLA_INC)
ENDJOB

