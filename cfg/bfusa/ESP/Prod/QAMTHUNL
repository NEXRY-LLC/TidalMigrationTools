APPL QAMTHUNL WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*10/18/2016-JFC-CONVERTED LIE JOB FROM MVS TO LINK                   */
/**********************************************************************/

NOTIFY FAILURE ABEND ALERT(REMD)
NOTIFY FAILURE ABEND OVERDUE MAILBOX(QAFAIL1)
TAG PPIW
CRITPATH ON

NT_JOB LIE.QAPRDORG EXTERNAL APPLID(QAPRDORG)
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(LIS.QAMTHUNL)
ENDJOB

JOB LIS.QAMTHUNL LINK PROCESS
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(UNLOCK_QA_SCOPING_MONTHLYCMD)
ENDJOB

NT_JOB UNLOCK_QA_SCOPING_MONTHLYCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\UNLOCK_QA_SCOPING_MONTHLY.cmd
  ARGS
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(UNLOCK_PPIW_PROD_MONTHLYCMD)
ENDJOB

NT_JOB UNLOCK_PPIW_PROD_MONTHLYCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\UNLOCK_PPIW_PROD_MONTHLY.cmd
  ARGS
  USER maestro
  RUN 2ND WORKDAY OF MONTH
  RELEASE ADD(WEB_SITE_UNLOCK_PPIW_MONTHLYCMD)
ENDJOB

NT_JOB WEB_SITE_UNLOCK_PPIW_MONTHLYCMD
  AGENT CYBA_PPIWPROD
  CMDNAME X:\cmd_scripts\WEB_SITE_UNLOCK_PPIW_MONTHLY.cmd
  ARGS
  USER maestro
  RUN 2ND WORKDAY OF MONTH
   DUEOUT EXEC 07:00
  RELEASE ADD(LIE.QAMTHUNL)
ENDJOB

JOB LIE.QAMTHUNL LINK PROCESS
  RUN 2ND WORKDAY OF MONTH
ENDJOB

