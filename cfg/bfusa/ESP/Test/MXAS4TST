APPL MXAS4TST WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*********************************************************************/
/*11/20/2019 -JFC- NEW APPL TO CHECK AGENT JOB ON MEXICO PROD AS400  */
/*********************************************************************/

JOB LIS.!ESPAPPL LINK PROCESS
   RUN DAILY
   RELEASE ADD(MS_BOSSCMP.BFMXPRO)
ENDJOB

AS400_JOB MS_BOSSCMP.BFMXPRO
   AGENT BFMXPRO
   CLPNAME TEBOSSCOM
 /*CURLIB MSPTOBJ
   LIBL (MSCUEXT MSPTFILE MSPTOBJ QGPL YOB600 QTEMP)
   JOBD MSCUEXT/TESSJOBD
   RUN DAILY
   RELEASE ADD(MS_BOSSTAM.BFMXPRO)
ENDJOB

AS400_JOB MS_BOSSTAM.BFMXPRO
   AGENT BFMXPRO
   CLPNAME TEBOSS
   LIBL (MSCUEXT MSPTFILE MSPTOBJ QGPL YOB600 QTEMP)
   JOBD MSCUEXT/TESSJOBD
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

JOB LIE.!ESPAPPL LINK PROCESS
   RUN DAILY
ENDJOB

