APPL AOTSTFTG WAIT

INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/**********************************************************************/
/*03/21/2018 -JFC= APPL TO TEST FILE TRANSFERS FROM STERLING INTEGRATOR
/**********************************************************************/


JOB AOTSTFTG.BEG LINK PROCESS
   RUN DAILY
   RELEASE ADD(SWESPJC2)
ENDJOB

JOB SWESPJC2 HOLD
   RUN DAILY
   RELEASE ADD(BOM1C_F191)
   RELEASE ADD(BOM2C_F191)
   RELEASE ADD(BOM3C_F191)
   RELEASE ADD(MMC_F191)
   RELEASE ADD(BOM1T_F191)
   RELEASE ADD(BOM2T_F191)
   RELEASE ADD(BOM3T_F191)
   RELEASE ADD(MMT_F191)
ENDJOB

