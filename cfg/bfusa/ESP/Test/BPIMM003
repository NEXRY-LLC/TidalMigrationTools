APPL BPIMM003
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

/*JOB TO RUN PERIODIC - THAT'S WHAT THEY SAID*/

SAP_JOB BPFP_MM_MAINT_CONNECT_PI_TRIGGER
   AGENT SAP_BZP100
   SAPJOBNAME MM_MAINT_CONNECT_PI_TRIGGER
   SAPJOBCLASS C
   ABAPNAME ZMMI_GOODS_MOVEMENT_BATCH
   VARIANT MC_INBOUND
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZMMI_GOODS_M
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   ABAPNAME ZCA_FILE_UTILITY
   VARIANT MC_INBOUND
   STEPUSER FSBP_BATCH
   LANGUAGE E
   PRINTDEST LOCL
   PRINTCOPIES 001
   PRINTSPOOLNAME ZCA_FILE_UTI
   PRINTIMMED YES
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_65_80
   BANNER D
   RECIPIENT FSBP_BATCH
   ARCMODE PRINT
   RUN DAILY
ENDJOB


