APPL QLED002 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
IF !ESPAPSUB# > 1 THEN JCLLIB 'SWS.ESP.COPYLIB'

JOB LISLE.LED002 LINK PROCESS
   RUN DAILY
   IF TODAY('SUN') THEN DELAYSUB 00.30
   RELEASE ADD(ZSDLEI03_EQUIPMENT_ORDERS)
ENDJOB

SAP_JOB ZSDLEI03_EQUIPMENT_ORDERS
   AGENT CYBB_BFQ030
   SAPJOBNAME ZSDLEI03_EQUIPMENT_ORDERS
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSDLEI03
   VARIANT EQP_PS_DAILY
   STEPUSER SAPADM
   LANGUAGE E
   PRINTREL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   ARCMODE PRINT
   PRINTCOPIES 001
   RUN REF LISLE.LED002
   RELEASE ADD(SFP2WPSM)
ENDJOB

AIX_JOB SFP2WPSM
   AGENT CYBA_ISBFQ
   USER wpsbfq
   SCRIPTNAME /local/bin/sftp2_wps.sh
   ARGS zsdlei03_demandorder_to_peoplesoft.txt
   RUN REF LISLE.LED002
   RELEASE ADD(ARZSDLEI03)
ENDJOB

UNIX_JOB ARZSDLEI03
   AGENT CYBA_ISBFQ
   SCRIPTNAME /local/bin/arcfile.sh
   ARGS +
interfaces/output/dataout/zsdlei03_demandorder_to_peoplesoft.txt D
   RUN REF LISLE.LED002
   USER bfqadm
ENDJOB


