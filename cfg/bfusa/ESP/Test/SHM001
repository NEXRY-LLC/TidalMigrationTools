APPL SHM001 WAIT
NOTIFY ABEND FAILURE ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
IF !ESPAPSUB# > 1 THEN JCLLIB 'SWS.ESP.COPYLIB'

SAP_JOB ZSDSHI01_RAC_MATL_CHARS
   AGENT CYBB_BFP030
   SAPJOBNAME ZSDSHI01_RAC_MATL_CHARS
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZSDSHI01
   VARIANT RAC_MONTHLY
   STEPUSER SAPADM
   LANGUAGE E
   PRINTDEST CP21
   PRINTCOPIES 001
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   BANNER D
   RECIPIENT FEEGARY
   ARCMODE PRINT
   RUN 1ST DAY OF MONTH
   EARLYSUB 01.05
   RELEASE ADD(SFP2WPSM)
ENDJOB

