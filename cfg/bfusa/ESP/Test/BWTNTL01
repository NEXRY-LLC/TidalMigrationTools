APPL BWTNTL01 WAIT

NOTIFY FAILURE ABEND ALERT(REMD)
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'

JOB BWTNTL01 LINK PROCESS
   RUN DAILY
   DELAYSUB 19.55
   IF TODAY('SUN') THEN EARLYSUB 14.55
   IF TODAY('1ST DAY OF MONTH') AND TODAY('WEEKDAY SAT') -
   THEN EARLYSUB 18.55
   IF TODAY('JUL 19 2017') THEN JOBATTR HOLD
   RELEASE ADD(ZDD_CUSTOMER_META)                /*APPL BWPCSTMR
   RELEASE ADD(ZDD_MATERIAL_CLASS_META)          /*APPL BWPMATRL
   RELEASE ADD(ZMSTR_DATA_LOAD)                  /*APPL BWPMDLD1
   RELEASE ADD(ZDD_VENDOR_META)                  /*APPL BWPVENDR
   RELEASE ADD(FSIP_TRANSFER_GLOBAL_SETTING_UOM) /*APPL BWPDPGS
ENDJOB

BWPC_JOB ZDD_CUSTOMER_META                   /*APPL BWPCSTMR
   AGENT CYBB_BWT100
   CHAIN 'ZDD_CUSTOMER_META'
   RUN DAILY
   DELAYSUB 21.30
   IF TODAY('SUN') THEN EARLYSUB 16.00
   RELEASE ADD(ZCUST_MD_CONSOL)                  /*APPL BWPCSTM2
ENDJOB

BWPC_JOB ZDD_MATERIAL_CLASS_META                /*APPL BWPMATRL
   AGENT CYBB_BWT100
   CHAIN 'ZDD_MATERIAL_CLASS_META'
   RUN DAILY
   DELAYSUB 21.00
   IF TODAY('SUN') THEN EARLYSUB 16.00
   RELEASE ADD(ZCA_MD_MATLMASTER)                  /*APPL BWPMMCN1
   RELEASE ADD(ZDD_MASTERDATA_TEXT)                /*APPL BWPMATR2
   RELEASE ADD(ZREP_MATBATCHCLAS_P)                /*APPL BWPMBCL1
ENDJOB

BWPC_JOB ZCUST_MD_CONSOL                       /*APPL BWPCSTM2
   AGENT CYBB_BWT100
   CHAIN 'ZCUST_MD_CONSOL'
   RUN DAILY
   RELEASE ADD(BWTNTL01.COMPLETE)
ENDJOB

BWPC_JOB ZMSTR_DATA_LOAD                         /*APPL BWPMDLD1
   AGENT CYBB_BWT100
   CHAIN 'ZMSTR_DATA_LOAD'
   RUN DAILY
   DELAYSUB 20.00
   IF TODAY('SUNDAY') THEN EARLYSUB 16.00
   RELEASE ADD(ZDD_MASTERDATA_TEXT)                 /*APPL BWPMATR2
   RELEASE ADD(ZREP_MATBATCHCLAS_P)                 /*APPL BWPMBCL1
   RELEASE ADD(ZCA_MD_MATLMASTER)                   /*APPL BWPMMCN1
ENDJOB

BWPC_JOB ZCA_MD_MATLMASTER                        /*APPL BWPMMCN1
   AGENT CYBB_BWT100
   CHAIN 'ZCA_MD_MATLMASTER'
   RUN WEEKDAYS SAT
   RELEASE ADD(BWTNTL01.COMPLETE)
ENDJOB

BWPC_JOB ZREP_MATBATCHCLAS_P                      /*APPL BWPMBCL1
   AGENT CYBB_BWT100
   CHAIN 'ZREP_MATBATCHCLAS_P'
   RUN WEEKDAYS SAT
   RELEASE ADD(BWTNTL01.COMPLETE)
ENDJOB

BWPC_JOB ZDD_MASTERDATA_TEXT                      /*APPL BWPMATR2
   AGENT CYBB_BWT100
   CHAIN 'ZDD_MASTERDATA_TEXT'
   RUN SAT
   RELEASE ADD(BWTNTL01.COMPLETE)
ENDJOB

BWPC_JOB ZDD_VENDOR_META                        /*APPL BWPVENDR
   AGENT CYBB_BWT100
   CHAIN 'ZDD_VENDOR_META'
   RUN DAILY
   DELAYSUB 21.00
   IF TODAY('SUN') THEN EARLYSUB 16.00
   RELEASE ADD(BWTNTL01.COMPLETE)
ENDJOB

BWPC_JOB FSIP_TRANSFER_GLOBAL_SETTING_UOM      /*APPL BWPDPGS
   AGENT CYBB_BWT100
   CHAIN 'ZFSIPGLOBALUOM'
   RUN 1ST DAY OF MONTH
   DELAYSUB 19.00
   IF TODAY('SUN') THEN EARLYSUB 15.00
   RELEASE ADD(BWTNTL01.COMPLETE)
ENDJOB

JOB BWTNTL01.COMPLETE
   MEMBER USIEBR14
   RUN DAILY
ENDJOB
