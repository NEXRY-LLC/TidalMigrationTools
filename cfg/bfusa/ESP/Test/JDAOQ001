FILE_TRIGGER ZOP_RE_BRU_ZASP_ORDER_LIST_CREATE CONDITIONAL
   AGENT CYBB_BFT030
   FILENAME +
'/data/BFT/interfaces/input/datain/ZOP_RE_BRU_ZASP_ORDER_LIST.TXT' +
CREATE
   RUN DAILY
   IF ESPSHH = "01" THEN RELEASE ADD(JDAOQ_ZOP_RE_BRU_OPZSP_SAP_SY_N)
   IF ESPSHH = "13" THEN RELEASE ADD(JDAOQ_ZOP_RE_BRU_OPZSP_SAP_SYNC)
   /*RELEASE ADD(LIS.JDAOQ001)*/
ENDJOB

