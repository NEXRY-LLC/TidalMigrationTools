/******************************************************************/

SAP_JOB JDAOQ_ZOP_RE_BRU_OPZSP_SAP_SYNC CONDITIONAL
   AGENT CYBB_BFT030
   SAPJOBNAME JDAOQ_ZOP_RE_BRU_OPZSP_SAP_SYNC
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME /JDAOP/ZOP_RE_BRU
   VARIANT OPZSP_SAP_SYNC
   STEPUSER SAPADM
   LANGUAGE E
   PRINTSPOOLNAME /JDAOP/ZOP_R
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 30
   COLUMNS 80
   PRINTFORMAT X_PAPER
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   NOTIFY JOBSTART MAILBOX(JDAOP)
   PRINTCOPIES 001
   BANNER D
   RELDELAY  20
   IF ESPSHH = "13" THEN RUN TODAY
   RELEASE (JDAOQ_ZOP_RE_BRU_RE_SYNC_PART_1)
ENDJOB

