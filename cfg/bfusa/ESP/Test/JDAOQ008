APPL JDAOQ008 WAIT
INVOKE 'SWS.ESP.APPL.PROCLIB(#DISTRIB)'
OPTIONS NODUEOUTPROPAGATION
/**********************************************************************/
/* CREATED ON 04/03/18 PER JOHN GREGORIC - DSG -INC000001568677       */
/**********************************************************************/
NOTIFY ABEND FAILURE ALERT(REMD)

FILE_TRIGGER OUT_UDT_BOR_LOG_REPORT_FLAT
   AGENT CYBB_BFT030
   FILENAME '/data/BFT/interfaces/input/datain/+
   OUT_UDT_BOR_LOG_REPORT_FLAT*.dat*' CREATE
   RUN DAILY
   RELEASE ADD(JDAOQ_ZJDA_BOR_UPDATE)
ENDJOB

SAP_JOB JDAOQ_ZJDA_BOR_UPDATE
   AGENT CYBB_BFT030
   SAPJOBNAME JDAOQ_ZJDA_BOR_UPDATE
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZJDA_BOR_UPDATE
   VARIANT ZJDA_BOR_UPDAT
   STEPUSER SAPADM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTSPOOLNAME ZJDA_BOR_UPD
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   BANNER D
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   NOTIFY JOBSTART MAILBOX(JDAOP)
   RELDELAY 2
   RUN DAILY
   RELEASE (JDAOQ_ZJDA_BOR_DELETE)
ENDJOB

SAP_JOB JDAOQ_ZJDA_BOR_DELETE
   AGENT CYBB_BFT030
   SAPJOBNAME JDAOQ_ZJDA_BOR_DELETE
   SAPJOBCLASS C
   STARTMODE ASAP
   ABAPNAME ZJDA_BOR_DELETE
   VARIANT ZJDA_BOR_DELET
   STEPUSER SAPADM
   LANGUAGE E
   PRINTCOPIES 001
   PRINTSPOOLNAME ZJDA_BOR_UPD
   PRINTREL YES
   PRINTNEWSPOOL YES
   EXPIRATION 8
   LINES 65
   COLUMNS 80
   PRINTFORMAT X_PAPER
   BANNER D
   RECIPIENT SAPADM
   PRINTDEPARTMENT Basis
   ARCMODE PRINT
   NOTIFY JOBEND MAILBOX(JDAOP)
   RUN DAILY
   RELEASE ADD(LIE.!ESPAPPL)
ENDJOB

APPLEND TRIGNEW1.!ESPAPPL
   ESPNOMSG TRIGGER !ESPEVENT ADD
   RELDELAY 5
ENDJOB

